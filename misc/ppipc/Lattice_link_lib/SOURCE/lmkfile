# Make file for PPIPC Shared Library link routines 89:4:18
# -- assumes path and assigns for assembly and compilation are correct

IPC.lib : CheckIPCPort.o CreateIPCMsg.o ServeIPCPort.o \
LoadIPCPort.o UseIPCPort.o PutIPCMsg.o FindIPCPort.o DropIPCPort.o \
DeleteIPCMsg.o LeaveIPCPort.o ShutIPCPort.o GetIPCPort.o \
MakeIPCId.o FindIPCItem.o
    oml IPC.lib r CheckIPCPort.o CreateIPCMsg.o ServeIPCPort.o \
     LoadIPCPort.o UseIPCPort.o PutIPCMsg.o \
     FindIPCPort.o DropIPCPort.o DeleteIPCMsg.o LeaveIPCPort.o \
     ShutIPCPort.o GetIPCPort.o \
     MakeIPCId.o FindIPCItem.o

.a.o:
    asm -d -iAINCL: $*

CheckIPCPort.o : CheckIPCPort.a IPClink.i IPC_lib.i

CreateIPCMsg.o : CreateIPCMsg.a IPClink.i IPC_lib.i

ServeIPCPort.o : ServeIPCPort.a IPClink.i IPC_lib.i

LoadIPCPort.o : LoadIPCPort.a IPClink.i IPC_lib.i

UseIPCPort.o : UseIPCPort.a IPClink.i IPC_lib.i

PutIPCMsg.o : PutIPCMsg.a IPClink.i IPC_lib.i

FindIPCPort.o : FindIPCPort.a IPClink.i IPC_lib.i

DropIPCPort.o : DropIPCPort.a IPClink.i IPC_lib.i

DeleteIPCMsg.o : DeleteIPCMsg.a IPClink.i IPC_lib.i

LeaveIPCPort.o : LeaveIPCPort.a IPClink.i IPC_lib.i

ShutIPCPort.o : ShutIPCPort.a IPClink.i IPC_lib.i

GetIPCPort.o : GetIPCPort.a IPClink.i IPC_lib.i

MakeIPCId.o : MakeIPCId.a IPCLink.i IPC_lib.i

FindIPCItem.o : FindIPCItem.a IPCLink.i IPC_lib.i

