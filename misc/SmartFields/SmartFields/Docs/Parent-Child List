field_cut ................ field_copy
field_cut ................ field_delete
field_cut ................ FLASH_SCREEN
field_delete ............. con_char_mult_delete
field_delete ............. cursor_jump_left
field_delete ............. FLASH_SCREEN
field_delete ............. strcpy
field_delete_backward .... con_char_mult_delete
field_delete_backward .... cursor_jump_left
field_delete_backward .... FLASH_SCREEN
field_delete_forward ..... con_char_mult_delete
field_delete_forward ..... FLASH_SCREEN
field_disable ............ cursor_invisible
field_display ............ con_graphic_rend
