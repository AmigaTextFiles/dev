INC = dinclude:amiga30

#
#
#

all: BST_Example1 BST_Example2 BST_Example3 BST_Example4 BST_ApplicationExample

BST_Example1: BST_Example1.c
  dcc -mi BST_Example1.c


BST_Example2: BST_Example2.c
  dcc -mi BST_Example2.c


BST_Example3: BST_Example3.c
  dcc -mi BST_Example3.c


BST_Example4: BST_Example4.c
  dcc -mi BST_Example4.c

BST_ApplicationExample: BST_ApplicationExample.c
  dcc -mi -d1 BST_ApplicationExample.c
