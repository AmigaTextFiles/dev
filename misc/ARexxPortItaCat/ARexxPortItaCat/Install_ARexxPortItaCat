; ARexxPortItaCat
; $VER: ARexxPortItaCat 1.0 (04.04.2020)

; Generated by InstallerGen 1.5!

(complete 0)
; MSG_Benvenuto
(set @app-name "ARexxPortItaCat")
(set @default-dest "Locale:Catalogs/italiano/")
(welcome "Benvenuto nell'installatore del catalogo ITALIANO di  ARexxPort.catalog v1.0")

(complete 20)
; MSG_CreaDirLocale
(makedir (tackon "Locale:" "Catalogs/Italiano/")
  (prompt "")
  (help @makedir-help)
)

(complete 50)
; MSG_Copia il catalogo
(copylib
  (source "ARexxPort.catalog")
  (dest "Locale:Catalogs/Italiano/")
  (prompt "Copia ARexxPort.catalog")
  (help @copylib-help)
  (confirm "average")
)

(complete 100)
; MSG_Fine
(exit)
