LIB    = ../../$(L).a
CFLAGS = -O2
ARLIB  = ar r $(LIB) $?
RANLIB = ranlib $(LIB)
CCOMP  = gcc
o      = .o
.c.o:
	g++ -I../../incl $(CFLAGS) $(DFLAGS) -c $*.c



all: $(LIB)

include Make.lst

$(LIB): $(OBJECTS)
	$(ARLIB)
	$(RANLIB)

clean:
	rm -f $(OBJECTS)

