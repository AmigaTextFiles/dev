#ifndef __config_h_
#define __config_h_

/* Define to empty if the keyword does not work.  */
#undef const

/* Define to `unsigned' if <sys/types.h> doesn't define.  */
#undef size_t

/* Define if you have the ANSI C header files.  */
#undef STDC_HEADERS

/* Define if you have POSIX compliant sys/wait.h */
#undef HAVE_SYS_WAIT_H

/* "printf" buffer size */
#define PRINTF_BUFFER_SIZE  (8*1024)

/* "system" buffer size */
#define SYSTEM_BUFFER_SIZE  (8*1024)

/* maximal number of function arguments */
#define MAX_NARGS 32

#endif
