ask "Install includes?"
if warn
	copy taglist_protos_pragmas.h	include:proto/taglist.h
	copy taglist_protos.h			include:clib/
	copy taglist_pragmas.h			include:pragmas/
	copy taglist.h					include:libraries/taglist.h
	echo "Includes installed."
endif
ask "Install library?"
if warn
	copy taglist.library			libs:taglist.library
	echo "Library installed."
endif
echo "Done."
