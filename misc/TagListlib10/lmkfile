# makefile compiling taglist.library using SAS/C 5.10 and Macro68 3.165

H=main.p
O=main.o taglist.o
E=taglist.library

all: $(E)

.c.p:
	makeproto $*.p $*.c

.c.o:
	sc5 -D_USEOLDEXEC_ -b0 -cf -ma -ms -v -O -iinclude: -Hinclude:allsyms.gst $*.c

.a.o:
	macro68 $*.a OBJFILE $*.o

$(E): $(H) $(O)
	slink lib:resident.o $(O) TO $(E) LIB lib:amis.lib lib:lc.lib WITH defines SMALLCODE SMALLDATA NOICONS ADDSYM BATCH

clean:
	delete $(H) $(O) $(E)

