OBJS     = insertbyname.o checksuffix.o checkentrytype.o getfiletype.o \
           saveicon.o showerror.o gauge.o writebuffer.o mysprintf.o \
           insertbyiname.o getamigaguidenode.o scanpattern.o scanwindow.o \
           calctextwidth.o convertsuffix.o timecalc.o getmaxdigitwidth.o

LIB      = xrefsupport.lib

CC       = sc
RM       = delete quiet

CFLAGS   = NOSTKCHK OPT OPTTIME GST=sc:def.m OBJLIB $(LIB)

all: $(OBJS)

.c.o:
   $(CC) $(CFLAGS) $*.c

.asm.o:
   $(CC) $(CFLAGS) ASM $*.asm

release:
   -$(RM) $(OBJS)

