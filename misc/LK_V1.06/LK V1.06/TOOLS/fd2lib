.key src,typ,verbose/s
.bra {
.ket }

; $VER: fd2lib 1.00 (17.7.94)
; Create a library from an fd file

If "{src}" EQ ""
  Echo "fd2lib -- V1.01 Quick FD file to library tranformer"
  Echo "Usage: fd2lib [SRC] <filename> [TYP] ASM or C or PASCAL or OBJECT [VERBOSE]"
  Quit 0
EndIf
If "{typ}" EQ "ASM"
  lk QDSTOP FDLIB {src} FDINCLUDE {verbose}
Else
If "{typ}" EQ "C"
  lk QDSTOP FDLIB {src} FDHEADER {verbose}
Else
If "{typ}" EQ "PASCAL"
  lk QDSTOP FDLIB {src} FDPASCAL {verbose}
Else
If "{typ}" EQ "OBJECT"
  lk QDSTOP FDLIB {src} FDOBJECT
Else
  Echo "fd2lib -- V1.01 Quick FD file to library tranformer"
  Echo "Usage: fd2lib [SRC] <filename> [TYP] ASM or C or PASCAL or OBJECT [VERBOSE]"
  Echo ""
  Echo "ERROR: invalid type."
  Quit 5
EndIf
EndIf
EndIf
EndIf
