#
# Makefile for the man system 
# (C)1997 Mark Papadakis
#

MAKEMANOBJS = main.o
MANOBJS     = man.o

all : $(MAKEMANOBJS) $(MANOBJS)
    sc $(MAKEMANOBJS) LINK ProgramName=MakeMan
    sc $(MANOBJS) LINK ProgramName=Man

main.o: main.c
man.o : man.c

install :
    @copy makeman  C:
    @copy man      C:
clean :
    @delete *.o

