@NODE "GetAmigaGuideMsg" "amigaguide.library/GetAmigaGuideMsg()"
@{jcenter}
@{u}@{b}GetAmigaGuideMsg()@{uu}@{ub}
@{jleft}@{lindent 4}
@{fg shine}@{b}NAME@{lindent 8}@{ub}@{fg text}
GetAmigaGuideMsg - Receive async AmigaGuide message.  @{FG SHINE}(V34)@{FG TEXT}
@{lindent 4}
@{fg shine}@{b}SYNOPSIS@{lindent 8}@{ub}@{fg text}
msg = GetAmigaGuideMsg (handle);
d0                       a0

struct @{"AmigaGuideMsg" LINK "include:libraries/amigaguide.h/MAIN" 86} *GetAmigaGuideMsg (@{"AMIGAGUIDECONTEXT" LINK "include:libraries/amigaguide.h/MAIN" 84});
@{lindent 4}
@{fg shine}@{b}FUNCTION@{lindent 8}@{ub}@{fg text}
This function returns a SIPC message from the AmigaGuide system,
if there is a message available.
@{lindent 4}
@{fg shine}@{b}INPUTS@{lindent 8}@{ub}@{fg text}
handle - Handle to a AmigaGuide system.
@{lindent 4}
@{fg shine}@{b}EXAMPLE@{lindent 8}@{ub}@{fg text}

@{"AMIGAGUIDECONTEXT" LINK "include:libraries/amigaguide.h/MAIN" 84} handle;
struct @{"AmigaGuideMsg" LINK "include:libraries/amigaguide.h/MAIN" 86} *agm;

/* get a AmigaGuide message */
while (agm = GetAmigaGuideMsg(handle))
{
    /* process the event */
    switch (agm->agm_Type)
    {
        case ToolCmdReplyID:    /* a command has completed */
            if (agm->agm_Pri_Ret)
            {
                /* An error occurred, the reason is in agm_Sec_Ret.
                 * The command string is in agm_Data
                 */
            }
            break;

        case ToolStatusID:      /* status message */
            if (agm->agm_Pri_Ret)
            {
                /* an error occurred, the reason is in agm_Sec_Ret */
            }
            break;

        default:
            break;
    }

    /* reply to the AmigaGuide message */
    @{"ReplyAmigaGuideMsg" LINK "ReplyAmigaGuideMsg" 0}(agm);
}
@{lindent 4}
@{fg shine}@{b}RETURNS@{lindent 8}@{ub}@{fg text}
msg - Pointer to a SIPC message or NULL if no message was available.
@{lindent 4}
@{fg shine}@{b}SEE ALSO@{lindent 8}@{ub}@{fg text}
@{"OpenAmigaGuideAsyncA()" LINK "OpenAmigaGuideAsyncA" 0}, @{"AmigaGuideSignal()" LINK "AmigaGuideSignal" 0}, @{"ReplyAmigaGuideMsg()" LINK "ReplyAmigaGuideMsg" 0}
@ENDNODE

