##############################################################################
#
#	MorphOS/gcc examples makefile
#
##############################################################################

RM = rm -f
CC = gcc

CFLAGS = -noixemul -mcpu=603e -mmultiple -fomit-frame-pointer -Wall -O2 -I ../../
LDFLAGS = -L ../ -ltek


all:	dynamicsystem balls bashing	array dino client server killserver \
		heaptest randomtest string subtask subtask2 timetest taskterror


dynamicsystem:	
	$(CC) dynamicsystem.c $(CFLAGS) $(LDFLAGS) -o dynamicsystem  

balls :	
	$(CC) balls.c $(CFLAGS) $(LDFLAGS) -o balls 

bashing: bashing.c
	$(CC) bashing.c $(CFLAGS) $(LDFLAGS) -o bashing 

array :
	$(CC) array.c $(CFLAGS) $(LDFLAGS) -o array 

dino :
	$(CC) dino.c $(CFLAGS) $(LDFLAGS) -o dino 

client :
	$(CC) client.c $(CFLAGS) $(LDFLAGS) -o client 

server :
	$(CC) server.c $(CFLAGS) $(LDFLAGS) -o server 

killserver :
	$(CC) killserver.c $(CFLAGS) $(LDFLAGS) -o killserver 

heaptest :
	$(CC) heaptest.c $(CFLAGS) $(LDFLAGS) -o heaptest 

randomtest :
	$(CC) randomtest.c $(CFLAGS) $(LDFLAGS) -o randomtest 

string :
	$(CC) string.c $(CFLAGS) $(LDFLAGS) -o string 

subtask :
	$(CC) subtask.c $(CFLAGS) $(LDFLAGS) -o subtask 

subtask2 :
	$(CC) subtask2.c $(CFLAGS) $(LDFLAGS) -o subtask2 

timetest :
	$(CC) timetest.c $(CFLAGS) $(LDFLAGS) -o timetest 

taskterror :
	$(CC) taskterror.c $(CFLAGS) $(LDFLAGS) -o taskterror 


clean:
	rm -f *.o balls dynamicsystem balls bashing	array dino client server killserver \
		heaptest randomtest string subtask subtask2 timetest taskterror
