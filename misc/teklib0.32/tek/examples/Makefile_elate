##############################################################################
#
#	Elate/vpcc examples makefile
#
##############################################################################

RM = rm -f
CC = vpcc

CFLAGS = -Wall -I ../../ -O3

.c.o: 
	$(CC) -c $(CFLAGS) $< -o $@

all:	dynamicsystem balls bashing array dino \
		client server killserver heaptest randomtest \
		string subtask subtask2 timetest taskterror \
		kine/kine

dynamicsystem: dynamicsystem.o

balls: balls.o

bashing:	bashing.o

array: array.o

dino: dino.o

client: client.o

server: server.o

killserver: killserver.o

heaptest: heaptest.o

randomtest: randomtest.o

string: string.o

subtask: subtask.o

subtask2: subtask2.o

timetest: timetest.o

taskterror: taskterror.o

kine/kine: kine/kine.o kine/mathutil.o

clean:
	$(RM) *.00
	-$(RM) *.o
	-$(RM) kine/*.00
	-$(RM) kine/*.o



