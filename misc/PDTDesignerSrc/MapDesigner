MapDesignerV2.0:TheDesigner.exe
	MapDesignerV2.0:Source/MapDesignerV2.i

		echo "Must assemble all files.  Please wait..."
		genim2 >NIL: MapDesignerV2.0:Source/Control.asm
		genim2 >NIL: MapDesignerV2.0:Source/Project.asm
		genim2 >NIL: MapDesignerV2.0:Source/Tiles.asm
		genim2 >NIL: MapDesignerV2.0:Source/Map.asm
		genim2 >NIL: MapDesignerV2.0:Source/Blocks.asm
		genim2 >NIL: MapDesignerV2.0:Source/Prefs.asm
		genim2 >NIL: MapDesignerV2.0:Source/Dialogue.asm
		genim2 >NIL: MapDesignerV2.0:Source/InputTables.asm
		genim2 >NIL: MapDesignerV2.0:Source/MenuDefs.asm
		genim2 >NIL: MapDesignerV2.0:Source/LoadILBM.asm -oMapDesignerV2.0:Modules/LoadILBM.o
		genim2 >NIL: MapDesignerV2.0:Source/SaveILBM.asm -oMapDesignerV2.0:Modules/SaveILBM.o
		genim2 >NIL: MapDesignerV2.0:Source/FileRequester.asm -oMapDesignerV2.0:Modules/FileRequester.o
		genim2 >NIL: MapDesignerV2.0:Source/PaletteRequester.asm -oMapDesignerV2.0:Modules/PaletteRequester.o

MapDesignerV2.0:Modules/ControlModule.o
	MapDesignerV2.0:Source/Control.asm

		echo "Assembling: Control.asm"
