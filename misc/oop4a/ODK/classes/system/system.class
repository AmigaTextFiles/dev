
class system
{
    Object class com.amiga.system.doswrapper dos
    
    public print(string myString)
    {
        Object myString
        
        long mem

        mem=myString.getAddr()
        dos.printf("%s",mem)
    }

    public println(string myString)
    {
        Object myString
        
        long mem

        mem=myString.getAddr()
        dos.printf("%s\n",mem)
    }

}
