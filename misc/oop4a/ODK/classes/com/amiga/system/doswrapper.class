
usefd=odk:classes/com/amiga/system/doswrapper.fd

class doswrapper
{
    Object DosBase

    public Open(long mystring,long args)
    {
      long res
      res=dos__Open(mystring,args)
      return res
    }

    public Close(long args)
    {
       if args#0
        {
          dos__Close(Args)
        }
       return -1
    }

    public Read(long handle,long buffer,long len)
    {
       long handle buffer len res
       
       res=dos__Read(handle,buffer,len)
       return res
    }

    public Write(long handle,long buffer,long len)
    {
       long handle buffer len res
       
       res=dos__Write(handle,buffer,len)
       return res
    }

    public Seek(long handle,long Offset,long len)
    {
       long handle Offset len res
       
       res=dos__Seek(handle,Offset,len)
       return res
    }

    public printf(long mystring,long arg1,long arg2,long arg3,long arg4,long arg5,long arg6,long arg7,long arg8,long arg9,long arg10,long arg11)
    {
       dos__VPrintf(mystring,>myArgs:*arg1,*arg2,*arg3,*arg4,*arg5,*arg6,*arg7,*arg8,*arg9,*arg10,*arg11,0)
    }

    public Constructor()
    {
      if (DosBase=OpenLibrary("dos.library",0))=0
       {
         return -1
       }
      return Dosbase
    }

    public DeConstructor()
    {
      CloseLibrary(DosBase)
    }

    Include "odk:classes/com/amiga/system/doswrapper_lib.i"

}
