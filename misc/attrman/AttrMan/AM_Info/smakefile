LFLAGS =             VERBOSE SMALLCODE SMALLDATA
LIB =                LIB:sc.lib LIB:amiga.lib

All: c:AM_Info

c:AM_Info: AM_Info.c
	@echo "*e[32m- AM_Info by Jeroen Massar -*e[0m"
	@date
	@echo "*e[32mCompiling »AM_Info«*e[0m"
	sc AM_Info.c OBJNAME t:AM_Info.o
	@slink to c:AM_Info from t:AM_Info.o lib $(LIB) $(LFLAGS) noicons
	@protect c:AM_Info +p
	@delete >nil: t:AM_Info.o
	@echo "*e[32mAlrighty then...*e[0m"
