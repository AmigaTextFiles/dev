@Node "GetAmigaGuideMsg()" "amigaguide.library/GetAmigaGuideMsg"
@{b}@{u}GetAmigaGuideMsg@{uu}@{ub} - Receive async AmigaGuide message.(V34)
  This function returns a SIPC message from the AmigaGuide system,
  if there is a message available.
SYNOPSIS  @{FG shine}struct@{FG text} @{"AmigaGuideMsg" Link "include:libraries/amigaguide.h/Main" 86} *GetAmigaGuideMsg (AMIGAGUIDECONTEXT);
  @{i}@{b}msg@{ub}@{ui} = GetAmigaGuideMsg (@{i}handle@{ui});
   d0                      a0
  @{i}handle@{ui} - Handle to a AmigaGuide system.
  @{i}@{b}msg@{ub}@{ui} - Pointer to a SIPC message or NULL if no message was available.
EXAMPLE
  AMIGAGUIDECONTEXT @{i}handle@{ui};
  @{FG shine}struct@{FG text} @{"AmigaGuideMsg" Link "include:libraries/amigaguide.h/Main" 86} *@{i}agm@{ui};
  /* get a AmigaGuide message */
  @{FG shine}while@{FG text} (@{i}agm@{ui} = GetAmigaGuideMsg(@{i}handle@{ui}))
  {
    /* process the event */
    @{FG shine}switch@{FG text} (agm->@{i}agm_Type@{ui})
    {
      @{FG shine}case@{FG text} ToolCmdReplyID:    /* a command has completed */
        @{FG shine}if@{FG text} (agm->@{i}agm_Pri_Ret@{ui})
        {
          /* An error occurred, the reason is in @{i}agm_Sec_Ret@{ui}.
           * The command string is in @{i}agm_Data@{ui} */
        }
        @{FG shine}break@{FG text};
      @{FG shine}case@{FG text} ToolStatusID:      /* status message */
        @{FG shine}if@{FG text} (agm->@{i}agm_Pri_Ret@{ui})
        {
          /* an error occurred, the reason is in @{i}agm_Sec_Ret@{ui} */
        }
        @{FG shine}break@{FG text};
      @{FG shine}default@{FG text}:
        @{FG shine}break@{FG text};
    }
    /* reply to the AmigaGuide message */
    ReplyAmigaGuideMsg(@{i}agm@{ui});
  }
