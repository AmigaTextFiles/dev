CFLAGS = IDLEN 128

FILES = toc1 toc2 getdisc1 getdisc2 getdisc3 getdisc4 getdisc5 match

all: $(FILES)

toc1: toc1.c
    sc $(CFLAGS) link toc1.c

toc2: toc2.c
    sc $(CFLAGS) link toc2.c

getdisc1: getdisc1.c
    sc $(CFLAGS) link getdisc1.c

getdisc2: getdisc2.c
    sc $(CFLAGS) link getdisc2.c

getdisc3: getdisc3.c
    sc $(CFLAGS) link getdisc3.c

getdisc4: getdisc4.c
    sc $(CFLAGS) link getdisc4.c

getdisc5: getdisc5.c
    sc $(CFLAGS) link getdisc5.c

match: match.c
    sc $(CFLAGS) link match.c
