// Note: cpp requires assignment GNU: to the root of your C++ compiler
preprocess "cpp -lang-c++ -Iinclude: -Icxxinclude: %s >%s"
exclude
  iostream.h iomanip.h fstream.h
  assert.h ctype.h errno.h float.h limits.h locale.h math.h setjmp.h signal.h
  stdarg.h stddef.h stdio.h stdlib.h string.h time.h stat.h
