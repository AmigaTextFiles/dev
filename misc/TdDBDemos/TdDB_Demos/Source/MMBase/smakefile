#
# Smakefile for MMBase
#

Obj = MMBase.o Control.o Control_temp.o

#CompilerOpts för alla filer
COpt = nodebug GST=include:all.gst nochkabort parm=r errrexx opt optsize

MMBase: $(Obj)
	slink lib:c.o $(Obj) to MMBase lib lib:sc.lib tdlib:toolbox.lib \
		smallcode smalldata stripdebug 

.c.o:
	sc $*.c $(COpt)
