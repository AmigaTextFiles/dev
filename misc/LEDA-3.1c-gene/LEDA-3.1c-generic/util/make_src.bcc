#.SWAP
LIB    = ..\..\$(L).lib
ARLIB  =
RANLIB =
o      = .obj
.c.obj:
	bcc -P -Qx -w- -mh -N -I..\..\incl $(DFLAGS) -c $*.c
	tlib /C $(LIB) +- $*.obj
 

#-----------------------------------------------------------------------------
# Targets
#-----------------------------------------------------------------------------

all: $(LIB)

include Make.lst

#BCC !include "Make.lst"


$(LIB): $(OBJECTS)
	$(ARLIB)
	$(RANLIB)

clean:
	rm -f $(OBJECTS)



