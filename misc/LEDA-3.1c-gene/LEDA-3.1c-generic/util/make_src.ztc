#-----------------------------------------------------------------------------
# ZORTECH
#-----------------------------------------------------------------------------

LIB    = ..\..\$(L).lib
ARLIB  =
RANLIB =
o      = .obj
.c.obj:
	ztc -cpp -w -mx -I..\..\incl $(DFLAGS) -c $*.c
	zorlibx /N /B $(LIB) +- $*.obj,;
 
#-----------------------------------------------------------------------------
# Targets
#-----------------------------------------------------------------------------

all: $(LIB)

include Make.lst

#BCC !include "Make.lst"

$(LIB): $(OBJECTS)
	$(ARLIB)
	$(RANLIB)

clean:
	rm -f $(OBJECTS)



