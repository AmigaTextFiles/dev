LIB    = ../../$(L).a
ARLIB  =
RANLIB = 
o      = .o
.c.o:
	gcc -I../../incl $(DFLAGS) -c -xc++ $*.c -xnone
	ar r $(LIB) 


#-----------------------------------------------------------------------------
# Targets
#-----------------------------------------------------------------------------

all: $(LIB)

include Make.lst

#BCC !include "Make.lst"


$(LIB): $(OBJECTS)
	$(ARLIB)
	$(RANLIB)

clean:
	rm -f $(OBJECTS)



