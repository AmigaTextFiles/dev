Procedure _FILE[MSG$,MO]
   If Exist("libs:req.library")=True
      Amos To Back 
      Lpos Freq 75,2
      Lcust Freq 16,40,20
      Lset Freq Dir Dir$
      FILE$=Lfreq(MSG$,MO+$20+$4+$40+$80)
      Amos To Front 
   End If 
End Proc[FILE$]
Procedure CONVERT[L$,WAY]
   For I=1 To Len(L$)
      A$=Mid$(L$,I,1)
      If WAY=1
         If A$=","
            Mid$(L$,I,1)="|"
         End If 
      End If 
      If WAY=2
         If A$="|"
            Mid$(L$,I,1)=","
         End If 
      End If 
   Next I
End Proc[L$]








