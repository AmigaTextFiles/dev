' Command
'  -Wb Fast Hex
'
E=Execall(-132)
Timer=0
For T=0 To 50000
   'Amos
   E$=Hex$(10530,8)-"$"
Next T
Print "Amos:";Timer
Timer=0
For T=0 To 50000
   'IExt
   E$=Wb Fast Hex(10530)
Next T
Print "IExt:";Timer
