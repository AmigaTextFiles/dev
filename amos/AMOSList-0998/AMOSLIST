| andrewcrowe@enterprise.net |         AsciiArt Studio               |
+----------------------------+---------------------------------------+
                                </PRE>
--BOUNDARY.1198026832.2
Content-Type: text/plain; name="rot_zoom.asc"
Content-Disposition: attachment; filename="rot_zoom.asc"

Input "What Detail (1-200) ?";SKL

ST:

Trap Load Iff "3d/worldmap.lores",0

If Errtrap : Dir$="work:amospro/" : Goto ST : End If 

Load Iff "3d/worldmap.lores",1



Screen 0
Cls 0

Degree 


'Double Buffer 
Autoback 0



PX=160
PY=100

Screen To Front 0


MX=320/SKL
MY=200/SKL





ZOM=2


Do 
   
   Timer=0
   
   
   ROTATE[ANG,ZOM,MX,MY,PX,PY,SKL]

   T#=Timer+1

   Screen 0
   F Plot ZOM,220,1

   Print At(0,0);"VBL:";T#-1;"  ","FPS:";50/T#;"  "
   
   Screen Swap 
   Wait Vbl 
   
   ANG=ANG+10
   ZOM=ZOM+1
   
   'If Jleft(1) 
   '   ANG=ANG+10 
   'End If  
   'If Jright(1)
   '   ANG=ANG-10 
   'End If  
   ''Bell   
   
Loop 


Procedure ROTATE[ANG,ZOM,MX,MY,PX,PY,SKL]
   
   MX2=MX/2
   MY2=MY/2
   
   ZOM=ZOM*SKL
   'ZOM#=ZOM#/64
   
   S=Sin(ANG)*256
   C=Cos(ANG)*256
   
   For Y=0 To MY
      
      'BY=(Y^2)/(128/skl)
      
      For X=0 To MX
         
         
         
         XM=(X-MX2)*ZOM
         YM=(Y-MY2)*ZOM
         
         OX=XM*C+YM*S+Rnd(16384)
         OY=YM*C-XM*S+Rnd(16384)
         
         
         
         
         OX=OX/16384 : OY=OY/16384
         
         Screen 1
         P=F Point(OX+PX,OY+PY)
         If P>-1
            
            Screen 0
            Trap F Plot X*SKL,Y*SKL,P
            'If SKL>1
            'F Plot X*SKL+1,Y*SKL+1,1
            'End If  
            
         End If 
         
         
         'Trap F Plot X*SKL,Y*SKL/2+50+BY,P 
         'Ink P : Bar X*SKL,Y*SKL To X*SKL+SKL,Y*SKL+SKL
         
         
      Next 
   Next 
   
End Proc

--BOUNDARY.1198026832.2--



