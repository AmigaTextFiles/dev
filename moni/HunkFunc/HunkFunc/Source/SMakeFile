SCOPTS  = RESOPT PARAMETERS=REGISTERS NOSTACKCHECK STRINGMERGE UNSIGNEDCHARS\
NOCHECKABORT OPTIMIZE OPTIMIZERSIZE OPTINLOCAL NOICONS MEMSIZE=HUGE

SLOPTS  = SMALLDATA SMALLCODE NOICONS STRIPDEBUG

# *** HunkFunc **********************************

HunkFunc: T:HunkFunc.o
	slink $(SLOPTS) FROM $? TO $@
	Protect $@ +p

T:HunkFunc.o: HunkFunc.c
	sc $(SCOPTS) DATA=FAR DEF=__NOLIBBASE__ $? OBJNAME=$@

