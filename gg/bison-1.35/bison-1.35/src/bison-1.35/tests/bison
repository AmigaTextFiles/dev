#! /bin/sh
# tests/bison.  Generated from bison.in by configure.
# Wrapper around a non installed bison to make it work as an installed one.

# We want to use the files shipped with Bison.
BISON_SIMPLE='/src/src/bison-1.35/src/bison.simple'
export BISON_SIMPLE
BISON_HAIRY='/src/src/bison-1.35/src/bison.hairy'
export BISON_HAIRY

exec '/src/src/bison-1.35/src/bison' ${1+"$@"}
