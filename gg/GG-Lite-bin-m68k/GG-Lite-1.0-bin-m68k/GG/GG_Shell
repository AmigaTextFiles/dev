; Geek Gadgets Korn Shell wrapper using KingCON

if "$ggshell" not eq "1"
  set ggshell 1
  execute gg_init
  failat 21
  mount >NIL: KCON: from GG:/Devs/Mountlist.kingcon
  newcli KCON:0/0/640/512/%n.GG[sh]/JUMP/ASYNC/FASTUPDATE/MAXBUF256/CLOSE from GG:gg_shell
else
  failat 21
  changetaskpri -10
  stack ${IXSTACK}
  cd ${HOME}
  sh
  endcli
endif
