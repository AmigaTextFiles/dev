@NODE "DoAsyncLayout" "datatypes.library/DoAsyncLayout()"
@{b}

    NAME@{ub}
        @{"DoAsyncLayout" LINK "gg:doc/NDK/Guide/Include/inline/datatypes.h/File" 108} - Call the @{"DTM_ASYNCLAYOUT" LINK "gg:doc/NDK/Guide/Include/datatypes/datatypesclass.h/File" 291} method on a separate process.
                                                               (V39)@{b}

    SYNOPSIS@{ub}
        retval = @{"DoAsyncLayout" LINK "gg:doc/NDK/Guide/Include/inline/datatypes.h/File" 108} (object, gpl);
        d0                      a0      a1

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"DoAsyncLayout" LINK "gg:doc/NDK/Guide/Include/inline/datatypes.h/File" 108} (Object *, @{"struct gpLayout" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 460} *);@{b}

    FUNCTION@{ub}
        This function is used to asyncronously perform the object's
        @{"DTM_ASYNCLAYOUT" LINK "gg:doc/NDK/Guide/Include/datatypes/datatypesclass.h/File" 291} method.  This is used to offload the layout method
        from @{"input.device" LINK "gg:doc/NDK/Guide/input/MAIN"}.

        The @{"DTM_ASYNCLAYOUT" LINK "gg:doc/NDK/Guide/Include/datatypes/datatypesclass.h/File" 291} method must exit when @{"SIGBREAKF_CTRL_C" LINK "gg:doc/NDK/Guide/Include/dos/dos.h/File" 245} signal
        is set.   This indicates that the data has become obsolete and
        the @{"DTM_ASYNCLAYOUT" LINK "gg:doc/NDK/Guide/Include/datatypes/datatypesclass.h/File" 291} method will be called again.@{b}

    INPUTS@{ub}
        object - Pointer to the data types object.
        gpl - Pointer to a gpLayout message.@{b}

    RETURNS@{ub}@{b}

    SEE ALSO@{ub}

@ENDNODE
