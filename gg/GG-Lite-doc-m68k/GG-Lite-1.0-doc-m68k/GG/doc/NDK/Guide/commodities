               defined in <@{"libraries/commodities.h" LINK "gg:doc/NDK/Guide/Include/libraries/commodities.h/File"}>. Returns @{"CX_INVALID" LINK "gg:doc/NDK/Guide/Include/libraries/commodities.h/File" 87}
