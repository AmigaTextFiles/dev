@NODE "AddFreeList" "icon.library/AddFreeList()"
@{b}

   NAME@{ub}
        @{"AddFreeList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 27} - add memory to a free list.@{b}

   SYNOPSIS@{ub}
        status = @{"AddFreeList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 27}(free, mem, len)
          D0                  A0    A1   A2

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"AddFreeList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 27}(@{"struct FreeList" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 86} *, @{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49}, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52});@{b}

   FUNCTION@{ub}
        This routine adds the specified memory to the free list.
        The free list will be extended (if required).  If there
        is not enough memory to complete the call, @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} is returned.

        Note that @{"AddFreeList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 27}() does @{"NOT" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 1500} allocate the requested memory.
        It only records the memory in the free list.@{b}

   INPUTS@{ub}
        free -- a pointer to a FreeList structure
        mem -- the base of the memory to be recorded
        len -- the length of the memory to be recorded@{b}

   RESULTS@{ub}
        status -- @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if the call succeeded else @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98};@{b}

   SEE ALSO@{ub}
        @{"exec.library/AllocEntry" LINK "gg:doc/NDK/Guide/exec/AllocEntry"}
        @{"exec.library/FreeEntry" LINK "gg:doc/NDK/Guide/exec/FreeEntry"}
        @{"icon.library/FreeFreeList" LINK "FreeFreeList"}
        @{"workbench/workbench.h" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File"}

@ENDNODE
@NODE "BumpRevision" "icon.library/BumpRevision()"
@{b}

   NAME@{ub}
        @{"BumpRevision" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 51} - reformat a name for a second copy.@{b}

   SYNOPSIS@{ub}
        result = @{"BumpRevision" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 51}(newbuf, oldname)
          D0                    A0      A1

        @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} @{"BumpRevision" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 51}(@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}, @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67});@{b}

   FUNCTION@{ub}
        @{"BumpRevision" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 51} takes a name and turns it into a "copy_of_name".
        It knows how to deal with copies of copies.  The routine
        will truncate the new name to the maximum dos name size
        (currently 30 characters).@{b}

   INPUTS@{ub}
        newbuf - the new buffer that will receive the name
                 (it must be at least 31 characters long).
        oldname - the original name@{b}

   RESULTS@{ub}
        result - a pointer to newbuf@{b}

   EXAMPLE@{ub}
        oldname                          newbuf
        -------                          ------
        "foo"                            "copy_of_foo"
        "copy_of_foo"                    "copy_2_of_foo"
        "copy_2_of_foo"                  "copy_3_of_foo"
        "copy_199_of_foo"                "copy_200_of_foo"
        "copy foo"                       "copy_of_copy foo"
        "copy_0_of_foo"                  "copy_1_of_foo"
        "012345678901234567890123456789" "copy_of_0123456789012345678901"

@ENDNODE
@NODE "ChangeToSelectedIconColor" "icon.library/ChangeToSelectedIconColor()"
@{b}

   NAME@{ub}
        @{"ChangeToSelectedIconColor" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 138} -- Modify an RGB colour value for use in
            a selected icon image (V44)@{b}

   SYNOPSIS@{ub}
        @{"ChangeToSelectedIconColor" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 138}(cr)
                                  A0

        @{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} @{"ChangeToSelectedIconColor" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 138}(@{"struct ColorRegister" LINK "gg:doc/NDK/Guide/Include/datatypes/pictureclass.h/File" 243} *cr);@{b}

   FUNCTION@{ub}
        This function will change the provided RGB colour value to make it
        suitable for use in an icon's select image. This may involve darkening
        or toning the colour. Usually, icon.library calls this function
        when creating a select image for palette mapped icons which do not
        contain "real" select images.@{b}

   INPUTS@{ub}
        cr -- Pointer to a ColorRegister, containing the RGB colour value
            to be changed.

@ENDNODE
@NODE "DeleteDiskObject" "icon.library/DeleteDiskObject()"
@{b}

   NAME@{ub}
        @{"DeleteDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 67} - Delete a Workbench disk object from disk.     (V37)@{b}

   SYNOPSIS@{ub}
        result = @{"DeleteDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 67}(name)
          D0                      A0

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"DeleteDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 67}(@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67});@{b}

   FUNCTION@{ub}
        This routine will try to delete a Workbench disk object from disk.
        The name parameter will have ".info" postpended to it, and the
        icon file of that name will be deleted.  If the call fails,
        it will return @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}. The reason for the failure may be obtained
        via @{"dos.library/IoErr()" LINK "gg:doc/NDK/Guide/dos/IoErr"}.

        This call also updates the Workbench screen if needed.

        Using this routine protects you from any future changes to
        the way icons are stored within the system.@{b}

   INPUTS@{ub}
        name -- name of the object (char *)@{b}

   RESULTS@{ub}
        result -- @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if it worked, @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98} if not.@{b}

   EXAMPLE@{ub}

        error = 0;

        /* Check if you have the right library version */
        if(IconBase->lib_Version > 36)
        {
            if (!@{"DeleteDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 67}(name))
                error = @{"IoErr()" LINK "gg:doc/NDK/Guide/dos/IoErr"};
        }
        else
        {
            /* Delete name plus ".info" */
        }

        if (error != 0)
        {
            /* Do error routine...*/
        }@{b}

   SEE ALSO@{ub}
        @{"dos.library/IoErr" LINK "gg:doc/NDK/Guide/dos/IoErr"}
        @{"icon.library/PutDiskObject" LINK "PutDiskObject"}
        @{"icon.library/GetDiskObject" LINK "GetDiskObject"}
        @{"icon.library/FreeDiskObject" LINK "FreeDiskObject"}
        @{"icon.library/GetIconTagList" LINK "GetIconTagList"}
        @{"icon.library/PutIconTagList" LINK "PutIconTagList"}

@ENDNODE
@NODE "DrawIconStateA" "icon.library/DrawIconStateA()"
@{b}

   NAME@{ub}
        @{"DrawIconStateA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 89} -- Draw an icon as if it were an image (V44)@{b}

   SYNOPSIS@{ub}
        @{"DrawIconStateA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 89}(rp,icon,label,leftEdge,topEdge,state,tags)
                       A0 A1   A2    D0       D1      D2    A3

        @{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} @{"DrawIconStateA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 89}(@{"struct RastPort" LINK "gg:doc/NDK/Guide/Include/graphics/rastport.h/File" 52} *rp,@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon,
                            @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} label,@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} leftEdge,@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} topEdge,
                            @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} state,@{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *tags);

        @{"DrawIconState" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 94}(rp,icon,label,leftEdge,topEdge,state,...);

        @{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} @{"DrawIconState" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 94}(@{"struct RastPort" LINK "gg:doc/NDK/Guide/Include/graphics/rastport.h/File" 52} *rp,@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon,
                           @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} label,@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} leftEdge,@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} topEdge,
                           @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} state,...);@{b}

   FUNCTION@{ub}
        This function will draw an icon as if it were an image; if
        a label is provided, it will be printed below it.@{b}

   INPUTS@{ub}
        rp -- Pointer to the RastPort to draw into; the RastPort clipping
            rules, font, style, text colours and drawing mode will be used.
        icon -- Pointer to a @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65}.
        label -- Pointer to a NUL-terminated string, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if
            no label text is to be printed.
        leftEdge, topEdge -- Coordinates at which the icon image
            should be drawn, including its border; please note that the
            icon label may extend in whole or in part beyond the
            leftEdge you have specified.
        state -- Select how and which icon image should be drawn;
            see @{"intuition/imageclass.h" LINK "gg:doc/NDK/Guide/Include/intuition/imageclass.h/File"} for supported states.
        tags -- Additional drawing options@{b}

   TAGS@{ub}
        @{"ICONDRAWA_DrawInfo" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 308} (@{"struct DrawInfo" LINK "gg:doc/NDK/Guide/Include/intuition/screens.h/File" 66} *) -- Drawing information data
            associated with the RastPort to draw into. The DrawInfo
            contents affect the colours of the border drawn around the
            icon image and also carry display aspect ratio information.
            If this tag is @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, the default rendering pens for the
            screen selected via @{"IconControl" LINK "IconControlA"}(...,ICONA_SetGlobalScreen,...)
            will be used instead.

            This tag defaults to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.

        @{"ICONDRAWA_Frameless" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 311} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Draw the icon without the
            surrounding border.

            This tag defaults to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.

        @{"ICONDRAWA_Borderless" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 317} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Draw the icon without the
            surrounding border and frame.

            This tag defaults to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.

        @{"ICONDRAWA_EraseBackground" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 314} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- When drawing a frameless
            icon, erase the icon background before drawing the icon.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.@{b}

   EXAMPLE@{ub}
        /* Draw the icon in its normal state, without
         * a label but including a frame.
         */
        @{"struct RastPort" LINK "gg:doc/NDK/Guide/Include/graphics/rastport.h/File" 52} *rp;
        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon;

        @{"DrawIconStateA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 89}(rp,icon,@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},0,0,@{"IDS_NORMAL" LINK "gg:doc/NDK/Guide/Include/intuition/imageclass.h/File" 197},@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101});

        /* The same again, but without a frame. */
        @{"DrawIconState" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 94}(rp,icon,@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},0,0,@{"IDS_NORMAL" LINK "gg:doc/NDK/Guide/Include/intuition/imageclass.h/File" 197},
            @{"ICONDRAWA_Frameless" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 311},@{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95},
        @{"TAG_DONE" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 39});

        /* Draw the icon in selected state with a border; the
         * frame colours come from the given DrawInfo.
         */
        @{"struct DrawInfo" LINK "gg:doc/NDK/Guide/Include/intuition/screens.h/File" 66} *drawInfo;

        @{"DrawIconState" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 94}(rp,icon,@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},0,0,@{"IDS_SELECTED" LINK "gg:doc/NDK/Guide/Include/intuition/imageclass.h/File" 198},
            @{"ICONDRAWA_DrawInfo" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 308},drawInfo,
        @{"TAG_DONE" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 39});@{b}

   SEE ALSO@{ub}
        @{"workbench/icon.h" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File"}
        @{"workbench/workbench.h" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File"}
        @{"intuition/imageclass.h" LINK "gg:doc/NDK/Guide/Include/intuition/imageclass.h/File"}
        @{"intuition/screens.h" LINK "gg:doc/NDK/Guide/Include/intuition/screens.h/File"}
