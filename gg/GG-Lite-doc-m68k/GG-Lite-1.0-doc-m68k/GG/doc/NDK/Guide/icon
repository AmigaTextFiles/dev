@NODE "DupDiskObjectA" "icon.library/DupDiskObjectA()"
@{b}

   NAME@{ub}
        @{"DupDiskObjectA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 71} -- Duplicate an icon (V44)@{b}

   SYNOPSIS@{ub}
        icon = @{"DupDiskObjectA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 71}(icon,tags);
        D0                    A0   A1

        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} * @{"DupDiskObjectA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 71}(@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon,
                                           @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *tags);

        icon = @{"DupDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 76}(icon,tag1,...);

        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} * @{"DupDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 76}(@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon,
                                          Tag tag1,...);@{b}

   FUNCTION@{ub}
        This function is used to create a duplicate of a DiskObject
        in memory. It can also be used to create an extended
        ("native") DiskObject data structure from an existing,
        traditional format DiskObject data structure.@{b}

   INPUTS@{ub}
        icon -- Pointer to the icon to be duplicated.
        tags -- Control options.@{b}

   TAGS@{ub}
        @{"ICONDUPA_DuplicateDrawerData" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 280} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Duplicate the DrawerData,
            if available.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONDUPA_DuplicateImages" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 283} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Duplicate the icon images;
            note that this DOES @{"NOT" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 1500} include the associated image
            data. To get the image data duplicated, too, also use
            the @{"ICONDUPA_DuplicateImageData" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 286} tag.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONDUPA_DuplicateImageData" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 286} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Duplicate the icon image
            data; this tag works together with the @{"ICONDUPA_DuplicateImages" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 283}
            tag. If @{"ICONDUPA_DuplicateImageData" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 286} is set to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}, you must
            also set @{"ICONDUPA_DuplicateImages" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 283} to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONDUPA_DuplicateDefaultTool" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 289} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Duplicate the icon default
            tool, if available.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONDUPA_DuplicateToolTypes" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 292} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Duplicate the icon tool
            types, if available.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONDUPA_DuplicateToolWindow" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 295} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Duplicate the icon tool
            window if available.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONDUPA_ActivateImageData" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 301} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- If the source icon contains
            palette mapped image data that was never put to use (this
            happens if it is retrieved with the @{"GetDiskObject()" LINK "GetDiskObject"} call rather
            than the new @{"GetIconTagList()" LINK "GetIconTagList"} call), this tag will cause the
            image data to be analyzed and an attempt to be made to use that
            image data in the duplicate. The resulting icon will be remapped
            for display on the default screen, such as the Workbench screen
            unless you use the @{"ICONGETA_Screen" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 203} tag (see below).

            This tag defaults to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.

        @{"ICONGETA_Screen" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 203} (@{"struct Screen" LINK "gg:doc/NDK/Guide/Include/intuition/screens.h/File" 133} *) -- If you specified that the
            palette mapped image data attached to an icon should be
            activated (via "@{"ICONDUPA_ActivateImageData" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 301},@{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}") this
            tag allows you to choose the screen the icon is to be remapped
            to. It works just like the corresponding tag for the
            @{"icon.library/GetIconTagList" LINK "GetIconTagList"} call.

            There is no default value for this tag, i.e. if the tag is
            omitted, the icon will be remapped to the default screen.

            This tag was introduced in icon.library 44.535.

        @{"ICONA_ErrorCode" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 44} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Pointer to a @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} word variable to
            store error codes in. Note that this variable will be
            initialized regardless of whether an error occured or not.
            Thus, you can check for an error condition by comparing the
            variable contents against 0; 0 indicates success, all other
            values are error codes as defined by @{"dos.library" LINK "gg:doc/NDK/Guide/dos/MAIN"}.@{b}

   RESULT@{ub}
        icon -- Pointer to a @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} in case of error.
            You can use @{"IoErr()" LINK "gg:doc/NDK/Guide/dos/IoErr"} to retrieve the error code or use the
            @{"ICONA_ErrorCode" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 44} tag instead.@{b}

   NOTES@{ub}
        Any data you chose not to have duplicated will be set to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.
        For example, if you specified "@{"ICONDUPA_DuplicateImages" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 283},@{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}",
        then the resulting icon's do_Gadget.GadgetRender and
        do_Gadget.SelectRender will both be @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.@{b}

   BUGS@{ub}
        icon.library will try to preserve the original bitmapped image
        data loaded with a palette mapped icon. If you duplicated this
        type of icon, the preserved original bitmapped image data would
        not be transferred to the copy. This was fixed in V44.527.
        For this feature to work, you must specify that the image data
        should be duplicated via "@{"ICONDUPA_DuplicateImages" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 283},@{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}".@{b}

   SEE ALSO@{ub}
        @{"dos.library/IoErr" LINK "gg:doc/NDK/Guide/dos/IoErr"}
        @{"icon.library/GetIconTagList" LINK "GetIconTagList"}
        @{"workbench/icon.h" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File"}
        @{"workbench/workbench.h" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File"}

@ENDNODE
@NODE "FindToolType" "icon.library/FindToolType()"
@{b}

   NAME@{ub}
        @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43} - find the value of a ToolType variable.@{b}

   SYNOPSIS@{ub}
        value = @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43}(toolTypeArray, typeName)
          D0                      A0           A1

        @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43}(@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} *, @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67});@{b}

   FUNCTION@{ub}
        This function searches a tool type array for a given entry,
        and returns a pointer to that entry.  This is useful for
        finding standard tool type variables.  The returned
        value is not a new copy of the string but is only
        a pointer to the part of the string after typeName.@{b}

   INPUTS@{ub}
        toolTypeArray -- an array of strings (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} *).
        typeName -- the name of the tooltype entry (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}).@{b}

   RESULTS@{ub}
        value -- a pointer to a string that is the value bound to typeName,
                or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if typeName is not in the toolTypeArray.@{b}

   EXAMPLE@{ub}
        Assume the tool type array has two strings in it:
            "FILETYPE=text"
            "TEMPDIR=:t"

        @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43}( toolTypeArray, "FILETYPE" ) returns "text"
        @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43}( toolTypeArray, "filetype" ) returns "text"
        @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43}( toolTypeArray, "TEMPDIR" )  returns ":t"
        @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43}( toolTypeArray, "MAXSIZE" )  returns @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}
        @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43}( toolTypeArray, "text" )     returns @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}
        @{"FindToolType" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 43}( toolTypeArray, ":t" )       returns @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}@{b}

   NOTES@{ub}
        icon.library V44 tolerates tool type strings with additional
        blanks around the '=' character, such as in "FILETYPE = text".
        Older icon.library versions did not support this.@{b}

   SEE ALSO@{ub}
        @{"icon.library/MatchToolValue" LINK "MatchToolValue"}

@ENDNODE
@NODE "FreeDiskObject" "icon.library/FreeDiskObject()"
@{b}

   NAME@{ub}
        @{"FreeDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 39} - free all memory in a Workbench disk object.@{b}

   SYNOPSIS@{ub}
        @{"FreeDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 39}(diskobj)
                       A0

        void @{"FreeDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 39}(@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *);@{b}

   FUNCTION@{ub}
        This routine frees all memory in a Workbench disk object, and the
        object itself.  It is implemented via @{"FreeFreeList()" LINK "FreeFreeList"}.

        @{"GetDiskObject()" LINK "GetDiskObject"} takes care of all the initialization required
        to set up the object's free list.  This procedure may ONLY
        be called on a DiskObject allocated via @{"GetDiskObject()" LINK "GetDiskObject"}.@{b}

   INPUTS@{ub}
        diskobj -- a pointer to a DiskObject structure; as of V44,
            a @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} diskobj pointer will be ignored.@{b}

   SEE ALSO@{ub}
        @{"icon.library/GetDiskObject" LINK "GetDiskObject"}
        @{"icon.library/PutDiskObject" LINK "PutDiskObject"}
        @{"icon.library/DeleteDiskObject" LINK "DeleteDiskObject"}
        @{"icon.library/FreeFreeList" LINK "FreeFreeList"}
        @{"icon.library/GetIconTagList" LINK "GetIconTagList"}
        @{"icon.library/PutIconTagList" LINK "PutIconTagList"}

@ENDNODE
@NODE "FreeFreeList" "icon.library/FreeFreeList()"
@{b}

   NAME@{ub}
        @{"FreeFreeList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 23} - free all memory in a free list.@{b}

   SYNOPSIS@{ub}
        @{"FreeFreeList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 23}(free)
                     A0

        void @{"FreeFreeList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 23}(@{"struct FreeList" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 86} *);@{b}

   FUNCTION@{ub}
        This routine frees all memory in a free list, and the
        free list itself.  It is useful for easily getting
        rid of all memory in a series of structures.  There is
        a free list in a Workbench object, and this contains
        all the memory associated with that object.

        A FreeList is a list of MemList structures.  See the
        MemList and MemEntry documentation for more information.

        If the FreeList itself is in the free list, it must be
        in the first MemList in the FreeList.@{b}

   INPUTS@{ub}
        free -- a pointer to a FreeList structure; as of V44
            a @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} free parameter will be ignored.@{b}

   SEE ALSO@{ub}
        @{"exec.library/AllocEntry" LINK "gg:doc/NDK/Guide/exec/AllocEntry"}
        @{"exec.library/FreeEntry" LINK "gg:doc/NDK/Guide/exec/FreeEntry"}
        @{"icon.library/AddFreeList" LINK "AddFreeList"}
        @{"workbench/workbench.h" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File"}

@ENDNODE
@NODE "GetDefDiskObject" "icon.library/GetDefDiskObject()"
@{b}

   NAME@{ub}
        @{"GetDefDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 55} - read default wb disk object from disk.       (V36)@{b}

   SYNOPSIS@{ub}
        diskobj = @{"GetDefDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 55}(def_type)
          D0                          D0

        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *@{"GetDefDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 55}(@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51});@{b}

   FUNCTION@{ub}
        This routine reads in a default Workbench disk object from disk.
        The valid def_types can be found in @{"workbench/workbench.h" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File"} and
        currently include @{"WBDISK" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 25} thru @{"WBKICK" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 31}. If the call fails,
        it will return @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}. The reason for the failure may be obtained
        via @{"IoErr()" LINK "gg:doc/NDK/Guide/dos/IoErr"}.

        Using this routine protects you from any future changes to
        the way default icons are stored within the system.@{b}

   INPUTS@{ub}
        def_type -- default icon type (@{"WBDISK" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 25} thru @{"WBKICK" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 31}).@{b}

   RESULTS@{ub}
        diskobj -- the default Workbench disk object in question@{b}

   NOTES@{ub}
        icon.library V36 through V40 did not support the @{"WBDEVICE" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 30} default
        icon type.@{b}

   SEE ALSO@{ub}
        @{"dos.library/IoErr" LINK "gg:doc/NDK/Guide/dos/IoErr"}
        @{"icon.library/PutDefDiskObject" LINK "PutDefDiskObject"}
        @{"icon.library/PutIconTagList" LINK "PutIconTagList"}

@ENDNODE
@NODE "GetDiskObject" "icon.library/GetDiskObject()"
@{b}

   NAME@{ub}
        @{"GetDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 31} - read in a Workbench disk object from disk.@{b}

   SYNOPSIS@{ub}
        diskobj = @{"GetDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 31}(name)
          D0                      A0

        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *@{"GetDiskObject" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 31}(@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67});@{b}

   FUNCTION@{ub}
        This routine reads in a Workbench disk object in from disk.  The
        name parameter will have ".info" postpended to it, and the
        icon file of that name will be read.  If the call fails,
        it will return @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.  The reason for the failure may be obtained
        via @{"IoErr()" LINK "gg:doc/NDK/Guide/dos/IoErr"}.

        Using this routine protects you from any future changes to
        the way icons are stored within the system.

        A FreeList structure is allocated just after the DiskObject
        structure; @{"FreeDiskObject" LINK "FreeDiskObject"} makes use of this to get rid of the
        memory that was allocated.@{b}

   INPUTS@{ub}
        name -- name of the object (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if you just want a
                DiskObject structure allocated for you (useful when
                calling @{"AddAppIcon()" LINK "gg:doc/NDK/Guide/wb/AddAppIconA"} in @{"workbench.library" LINK "gg:doc/NDK/Guide/wb/MAIN"}).@{b}

   RESULTS@{ub}
        diskobj -- the Workbench disk object in question@{b}

   SEE ALSO@{ub}
        @{"dos.library/IoErr" LINK "gg:doc/NDK/Guide/dos/IoErr"}
        @{"icon.library/DeleteDiskObject" LINK "DeleteDiskObject"}
        @{"icon.library/FreeDiskObject" LINK "FreeDiskObject"}
        @{"icon.library/GetDiskObjectNew" LINK "GetDiskObjectNew"}
        @{"icon.library/GetIconTagList" LINK "GetIconTagList"}
        @{"icon.library/NewDiskObject" LINK "NewDiskObject"}
        @{"icon.library/PutDiskObject" LINK "PutDiskObject"}
        @{"icon.library/PutIconTagList" LINK "PutIconTagList"}
        @{"workbench.library/AddAppIconA" LINK "gg:doc/NDK/Guide/wb/AddAppIconA"}

@ENDNODE
@NODE "GetDiskObjectNew" "icon.library/GetDiskObjectNew()"
@{b}

   NAME@{ub}
        @{"GetDiskObjectNew" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 63} - read in a Workbench disk object from disk.    (V36)@{b}

   SYNOPSIS@{ub}
        diskobj = @{"GetDiskObjectNew" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 63}(name)
          D0                      A0

        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *@{"GetDiskObjectNew" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 63}(@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67});@{b}

   FUNCTION@{ub}
        This routine reads in a Workbench disk object in from disk.  The
        name parameter will have ".info" postpended to it, and the
        icon file of that name will be read.  If the call fails,
        it will return zero.  The reason for the failure may be obtained
        via @{"IoErr()" LINK "gg:doc/NDK/Guide/dos/IoErr"}.

        Using this routine protects you from any future changes to
        the way icons are stored within the system.

        A FreeList structure is allocated just after the DiskObject
        structure; @{"FreeDiskObject" LINK "FreeDiskObject"} makes use of this to get rid of the
        memory that was allocated.

        This call is functionally identical to @{"GetDiskObject()" LINK "GetDiskObject"} with one
        exception. If its call to @{"GetDiskObject()" LINK "GetDiskObject"} fails, this function calls
        @{"GetDefDiskObject()" LINK "GetDefDiskObject"}. This is useful when there is no .info file for the
        icon you are trying to get a disk object for. Applications that use
        workbench application windows MUST use this call if they want to handle
        the user dropping an icon (that doesn't have a ".info" file) on their
        window. The V2.0 icon editor program is an example of a Workbench
        application window that uses this call.@{b}

   INPUTS@{ub}
        name -- name of the object (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if you just want a
                DiskObject structure allocated for you (useful when
                calling @{"AddAppIcon()" LINK "gg:doc/NDK/Guide/wb/AddAppIconA"} in @{"workbench.library" LINK "gg:doc/NDK/Guide/wb/MAIN"}).@{b}

   RESULTS@{ub}
        diskobj -- the Workbench disk object in question@{b}

   SEE ALSO@{ub}
        @{"dos.library/IoErr" LINK "gg:doc/NDK/Guide/dos/IoErr"}
        @{"icon.library/FreeDiskObject" LINK "FreeDiskObject"}
        @{"icon.library/GetDiskObject" LINK "GetDiskObject"}
        @{"icon.library/PutDiskObject" LINK "PutDiskObject"}
        @{"icon.library/DeleteDiskObject" LINK "DeleteDiskObject"}
        @{"icon.library/GetIconTagList" LINK "GetIconTagList"}
        @{"icon.library/PutIconTagList" LINK "PutIconTagList"}
        @{"workbench.library/AddAppIconA" LINK "gg:doc/NDK/Guide/wb/AddAppIconA"}

@ENDNODE
@NODE "GetIconRectangleA" "icon.library/GetIconRectangleA()"
@{b}

   NAME@{ub}
        @{"GetIconRectangleA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 98} -- Query the size of the area an icon renders into (V44)@{b}

   SYNOPSIS@{ub}
        success = @{"GetIconRectangleA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 98}(rp,icon,label,rectangle,tags)
        D0                          A0 A1   A2    A3        A4

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"GetIconRectangleA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 98}(@{"struct RastPort" LINK "gg:doc/NDK/Guide/Include/graphics/rastport.h/File" 52} *rp,@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon,
                               @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} label,@{"struct Rectangle" LINK "gg:doc/NDK/Guide/Include/graphics/gfx.h/File" 27} *rectangle,
                               @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *tags);

        success = @{"GetIconRectangle" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 103}(rp,icon,label,rectangle,...);

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"GetIconRectangle" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 103}(@{"struct RastPort" LINK "gg:doc/NDK/Guide/Include/graphics/rastport.h/File" 52} *rp,@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon,
                              @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} label,@{"struct Rectangle" LINK "gg:doc/NDK/Guide/Include/graphics/gfx.h/File" 27} *rectangle,...);@{b}

   FUNCTION@{ub}
        This function will calculate the size of the area icon rendering
        would affect.@{b}

   INPUTS@{ub}
        rp -- Pointer to the RastPort to use for calculating the
            the size of the label with respect to the font and
            style options currently in use; this parameter may be
            @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if the label is @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, too.
        icon -- Pointer to a @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65}.
        label -- Pointer to a NUL-terminated string, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if
            no label text is to enter the calculation.
        rectangle -- Pointer to a @{"struct Rectangle" LINK "gg:doc/NDK/Guide/Include/graphics/gfx.h/File" 27} to fill in. The
            contents will take the size of the icon image, the icon
            border size and -- if you provide RastPort and label
            text -- the icon label into account. Due to how labels
            are printed, the rectangle->MinX and rectangle->MinY
            members may be negative.
        tags -- Additional drawing options to be taken into account.@{b}

   TAGS@{ub}
        @{"ICONDRAWA_DrawInfo" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 308} (@{"struct DrawInfo" LINK "gg:doc/NDK/Guide/Include/intuition/screens.h/File" 66} *) -- Drawing information data
            associated with the RastPort parameter passed in.

            This tag defaults to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.

        @{"ICONDRAWA_Borderless" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 317} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- When calculating the size of
            the rectangle, this tag allows you to leave the icon
            border size out of the equation.

            This tag defaults to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.@{b}

   RESULTS@{ub}
        success -- @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if the parameters were well-formed and
            the output data fits into the Rectangle data
            structure (which may not be the case if the image
            is too large for a signed 16 bit integer to hold).@{b}

   EXAMPLE@{ub}
        /* Find out how large an icon is. */
        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon;
        @{"struct Rectangle" LINK "gg:doc/NDK/Guide/Include/graphics/gfx.h/File" 27} rect;

        if(@{"GetIconRectangleA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 98}(@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},icon,@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},&rect,@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}))
        {
            @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} width,height;

            width = rect.MaxX - rect.MinX + 1;
            height = rect.MaxY - rect.MinY + 1;

            @{"Printf" LINK "gg:doc/NDK/Guide/dos/VPrintf"}("icon is %ld×%ld pixels in size.\\n",
                width,height);
        }

        /* Find out which area the icon rendering would
         * cover if a label would be printed below it.
         */
        @{"struct RastPort" LINK "gg:doc/NDK/Guide/Include/graphics/rastport.h/File" 52} *rp;

        if(@{"GetIconRectangle" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 103}(rp,@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},icon,"a rather long label text",&rect,@{"TAG_DONE" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 39}))
        {
            @{"Printf" LINK "gg:doc/NDK/Guide/dos/VPrintf"}("icon plus label would cover the area %ld,%ld×%ld,%ld\\n",
                rect.MinX,rect.MinY,rect.MaxX,rect.MaxY);
        }@{b}

   NOTES@{ub}
        This function can be used to optimize on-screen rendering by
        precalculating the area rendering would cover. To make sure that
        the precalculated area size matches the one covered by the actual
        drawing operation, the RastPorts you use for measuring and for
        drawing should share the same font and the same text styles.

        The image size calculated by this function can be
        slightly larger than the data you will find in the icon's
        do_Gadget.GadgetRender Image structure since the icon border
        will be taken into account. This happens regardless of whether
        icon.library was switched into frameless icon rendering mode
        or whether the icon is frameless. To obtain the size of the
        icon without taking the border into account, use the
        @{"ICONDRAWA_Borderless" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 317} tag.

        The size of the icon, as returned by this function, may be different
        depending upon whether @{"LayoutIconA()" LINK "LayoutIconA"} has been called or not.@{b}

   SEE ALSO@{ub}
        @{"workbench/icon.h" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File"}
        @{"workbench/workbench.h" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File"}
        @{"icon.library/DrawIconState" LINK "DrawIconStateA"}
        @{"icon.library/IconControlA" LINK "IconControlA"}
        @{"icon.library/LayoutIconA" LINK "LayoutIconA"}

@ENDNODE
@NODE "GetIconTagList" "icon.library/GetIconTagList()"
@{b}

   NAME@{ub}
        @{"GetIconTagList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 111} -- Retrieve an icon (V44)@{b}

   SYNOPSIS@{ub}
        icon = @{"GetIconTagList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 111}(name,tags);
        D0                    A0   A1

        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} * @{"GetIconTagList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 111}(@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} name,@{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *tags);

        icon = @{"GetIconTags" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 116}(name,tag1,...);

        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} * @{"GetIconTags" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 116}(@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} name,Tag tag1,...);@{b}

   FUNCTION@{ub}

        This function is used to retrieve an icon; the icon can
        belong to a file/drawer/volume or it can be a default icon.@{b}

   INPUTS@{ub}
        name -- Name of the object an icon is to be retrieved for,
            or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if a default icon is to be retrieved.
        tags -- Retrieval options.@{b}

   TAGS@{ub}
        @{"ICONGETA_GetDefaultType" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 177} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) -- Type of the default icon
            to retrieve; must be one of @{"WBDISK" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 25}, @{"WBDRAWER" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 26}, @{"WBTOOL" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 27},
            @{"WBPROJECT" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 28}, @{"WBGARBAGE" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 29}, @{"WBDEVICE" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 30} or @{"WBKICK" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 31}.

            If this tag is used, the "name" parameter will be ignored.

        @{"ICONGETA_GetDefaultName" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 180} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) -- Name of the default object
            type to retrieve a default icon for. If the named default
            icon cannot be found, @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} will be returned. To avoid
            this, also specify @{"ICONGETA_GetDefaultType" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 177}: this will provide
            a fall-back and return a "default" default icon instead.

            If this tag is used, the "name" parameter will be ignored.

        @{"ICONGETA_FailIfUnavailable" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 185} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- If there is no icon for
            the specified object, this tag will control how to
            proceed: if @{"ICONGETA_FailIfUnavailable" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 185} is set to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95},
            @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} and an error code will be returned; if set to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98},
            icon.library will first invoke the global identification
            hook (see @{"icon.library/IconControlA" LINK "IconControlA"}) and then, if necessary,
            provide a default icon for the object in question.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONGETA_GetPaletteMappedIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 188} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- By default, icon.library
            will try to retrieve the palette mapped version of an icon. To
            change this, set the @{"ICONGETA_GetPaletteMappedIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 188} tag to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONGETA_RemapIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 194} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Palette mapped icons will usually
            be automatically remapped for display on the screen
            selected using @{"IconControlA()" LINK "IconControlA"}. If you do not want this to
            happen because, for example, you want to use @{"LayoutIcon()" LINK "LayoutIconA"}
            instead, you can set the @{"ICONGETA_RemapIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 194} tag to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.
            Take care when doing this as the icon returned to you
            will have @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} pointers in the do_Gadget.GadgetRender
            and do_Gadget.SelectRender members.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONGETA_IsDefaultIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 191} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- When retrieving an icon with
            the @{"ICONGETA_FailIfUnavailable" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 185} tag set to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}, you may
            receive a default icon because there is no on-disk icon
            file for the object you wanted to retrieve it for. This
            type of default icon is also known as a "fake" icon.
            To find out whether you have received a fake icon, use the
            @{"ICONGETA_IsDefaultIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 191} tag.

        @{"ICONGETA_GenerateImageMasks" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 197} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- When loading icons with
            transparent colours or backfill imagery, bit plane masks
            will be generated for later use with @{"DrawIconStateA()" LINK "DrawIconStateA"}.
            If you do not need these masks, set this tag to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.

            This tag defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONGETA_Screen" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 203} (@{"struct Screen" LINK "gg:doc/NDK/Guide/Include/intuition/screens.h/File" 133} *) -- Pointer to the screen
            to remap the icon to; if set to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, this keeps the
            icon from getting remapped in the first place (see
            @{"ICONGETA_RemapIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 194} for the consequences).

            This tag defaults to the default screen to remap to, as
            preset by @{"IconControlA()" LINK "IconControlA"}.

        @{"ICONGETA_Label" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 200} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) -- The label text to be printed below
            the icon when it is put to use lateron. This parameter
            can be useful to figure out the type of the icon to
            be retrieved by the identification hook. For example,
            Workbench may label unreadable disk icons as "DF0:????".
            The identification hook could key on this and supply an
            appropriate icon for the disk.

            This tag defaults to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.

        @{"ICONA_ErrorCode" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 44} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Pointer to a @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} word variable to
            store error codes in. Note that this variable will be
            initialized regardless of whether an error occured or not.
            Thus, you can check for an error condition by comparing the
            variable contents against 0; 0 indicates success, all other
            values are error codes as defined by @{"dos.library" LINK "gg:doc/NDK/Guide/dos/MAIN"}.@{b}

   RESULT@{ub}
        icon -- Pointer to a @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} in case of error.
            You can use @{"IoErr()" LINK "gg:doc/NDK/Guide/dos/IoErr"} to retrieve the error code or use the
            @{"ICONA_ErrorCode" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 44} tag instead.@{b}

   EXAMPLE@{ub}

        /* Retrieve the default drawer icon. */
        @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *icon;
        @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} errorCode;

        icon = @{"GetIconTags" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 116}(@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},
            @{"ICONGETA_GetDefaultType" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 177},@{"WBDRAWER" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 26},
            @{"ICONA_ErrorCode" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 44},&errorCode,
        @{"TAG_DONE" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 39});

        if(icon == @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101})
        {
            @{"Printf" LINK "gg:doc/NDK/Guide/dos/VPrintf"}("could not retrieve default drawer icon;\\n");
            @{"PrintFault" LINK "gg:doc/NDK/Guide/dos/PrintFault"}(errorCode,@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101});
        }

        /* Retrieve the default "picture" icon. */
        icon = @{"GetIconTags" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 116}(@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},
            @{"ICONGETA_GetDefaultName" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 180},"picture",
        @{"TAG_DONE" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 39});

        /* Retrieve the default "picture" icon; if there is no
         * such default icon, retrieve the default project
         * icon instead.
         */
        icon = @{"GetIconTags" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 116}(@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101},
            @{"ICONGETA_GetDefaultName" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 180},"picture",
            @{"ICONGETA_GetDefaultType" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 177},@{"WBPROJECT" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 28},
        @{"TAG_DONE" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 39});

        /* Retrieve the regular, non-palette-mapped version of
         * an icon.
         */
        icon = @{"GetIconTags" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 116}("file",
            @{"ICONGETA_GetPaletteMappedIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 188},@{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98},
        @{"TAG_DONE" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 39});

        /* Retrieve the icon for a file; if there is no icon associated
         * with it, retrieve a default icon that matches the file.
         * Also, remember whether we got a real icon or a fake one.
         */
        @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} isFakeIcon;

        icon = @{"GetIconTags" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 116}("file",
            @{"ICONGETA_FailIfUnavailable" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 185},@{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98},
            @{"ICONGETA_IsDefaultIcon" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 191},&isFakeIcon,
        @{"TAG_DONE" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 39});

        if(icon != @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101})
        {
            @{"Printf" LINK "gg:doc/NDK/Guide/dos/VPrintf"}("got an icon at 0x%08lx, and it is a %s icon.\\n",
                icon,isFakeIcon ? "fake" : "real");
        }@{b}

   NOTES@{ub}
        This function is a superset of @{"GetDefDiskObject()" LINK "GetDefDiskObject"}, @{"GetDiskObject()" LINK "GetDiskObject"}
        and @{"GetDiskObjectNew()" LINK "GetDiskObjectNew"}.

        If a palette-mapped icon is set to allocate its colours from the
        default screen, such as the Workbench screen, icon.library may
        resort to remap its colours in the course of Workbench screen
        close/reopen transitions. This means that you cannot depend upon
        the icon's image bitmaps to stay the same size, shape or colour.
        If this is what you need, you should either create your own images
        from the palette mapped data or make copies of the icon image
        bitmaps and the associated colour table.

        If during Workbench close/open transitions the global remap
        screen becomes @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, @{"GetIconTagList" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 111}() may refuse to return a
        remapped icon and return with an error (@{"ERROR_REQUIRED_ARG_MISSING" LINK "gg:doc/NDK/Guide/Include/dos/dos.h/File" 185})
        instead.@{b}

   SEE ALSO@{ub}
        @{"workbench/icon.h" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File"}
        @{"workbench/workbench.h" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File"}
        @{"icon.library/GetDiskObject" LINK "GetDiskObject"}
        @{"icon.library/GetDiskObjectNew" LINK "GetDiskObjectNew"}
        @{"icon.library/GetDefDiskObject" LINK "GetDefDiskObject"}
        @{"icon.library/IconControlA" LINK "IconControlA"}
        @{"icon.library/PutIconTagList" LINK "PutIconTagList"}

@ENDNODE
@NODE "IconControlA" "icon.library/IconControlA()"
@{b}

   NAME@{ub}
        @{"IconControlA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 80} -- Set and get icon and icon.library options (V44)@{b}

   SYNOPSIS@{ub}
        processed = @{"IconControlA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 80}(icon,tags);
        D0                       A0   A1

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"IconControlA" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 80}(@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *,@{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *);

        processed = @{"IconControl" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 85}(icon,tag1,...);

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"IconControl" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 85}(@{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} *,Tag tag1,...);@{b}

   FUNCTION@{ub}

        This function is used to modify and query icon and icon.library
        options.@{b}

   INPUTS@{ub}
        icon -- Pointer to a @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65}, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if global
                options are to be modified/queried.
        tags -- Options to modify/query.@{b}

   TAGS@{ub}
        @{"ICONCTRLA_SetGlobalScreen" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 54} (@{"struct Screen" LINK "gg:doc/NDK/Guide/Include/intuition/screens.h/File" 133} *) -- Pointer to the
            screen to remap palette mapped icons to. This is commonly
            used by @{"workbench.library" LINK "gg:doc/NDK/Guide/wb/MAIN"} after it has initialized itself
            and before/after screen open/close transitions. If the
            screen is set to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, the icon colour remapping strategy is
            changed: colours are no longer allocated from a screen
            and the colours to map to will come from a default colour
            colour table with four palette entries. Please note that
            once you select a screen to remap to, you cannot tell
            icon.library to use a different screen; you first need to
            tell icon.library to "let go" of the previously selected
            screen by passing a @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} @{"ICONCTRLA_SetGlobalScreen" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 54} parameter.
            You can set a different screen only after this is done.

        @{"ICONCTRLA_GetGlobalScreen" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 55} (@{"struct Screen" LINK "gg:doc/NDK/Guide/Include/intuition/screens.h/File" 133} **) -- Pointer to the
            screen to remap palette mapped icons to.

        @{"ICONCTRLA_SetGlobalPrecision" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 58} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) -- Set the global colour
            allocation precision used when remapping icons for
            display. Default is @{"PRECISION_ICON" LINK "gg:doc/NDK/Guide/Include/graphics/view.h/File" 240}.

        @{"OBP_Precision" LINK "gg:doc/NDK/Guide/Include/graphics/view.h/File" 245} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) -- Can be used as an alias for the
            @{"ICONCTRLA_SetGlobalPrecision" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 58} tag.

        @{"ICONCTRLA_GetGlobalPrecision" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 59} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Get the global colour
            allocation precision used when remapping icons for
            display.

        @{"ICONCTRLA_SetGlobalEmbossRect" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 62} (@{"struct Rectangle" LINK "gg:doc/NDK/Guide/Include/graphics/gfx.h/File" 27} *) -- Set the
            frame sizes used for drawing icons. The Rectangle
            must be initialized as follows:

                Rectangle->MinX  Width of left edge border (negative),
                                 must be in the range [-255..-1]
                Rectangle->MinY  Height of top edge border (negative),
                                 must be in the range [-255..-1]
                Rectangle->MaxX  Width of right edge border (positive),
                                 must be in the range [1..255]
                Rectangle->MaxY  Height of bottom edge border (positive),
                                 must be in the range [1..255]

        @{"ICONCTRLA_GetGlobalEmbossRect" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 63} (@{"struct Rectangle" LINK "gg:doc/NDK/Guide/Include/graphics/gfx.h/File" 27} *) -- Get the
            frame sizes used for drawing icons.

        @{"ICONCTRLA_SetGlobalFrameless" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 66} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Select whether icons
            should always be drawn without a surrounding frame.
            This option defaults to @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.

        @{"ICONCTRLA_GetGlobalFrameless" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 67} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Query whether icons
            are always drawn without a surrounding frame.

        @{"ICONCTRLA_SetGlobalIdentifyHook" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 78} (@{"struct Hook" LINK "gg:doc/NDK/Guide/Include/utility/hooks.h/File" 28} *) -- Set the
            hook that is invoked when trying to find an icon
            for a file/drawer/volume that does not have an
            icon associated with it. If set to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, no hook
            will be called. The hook will be called with the
            following parameters:

            hookFunc(hook,reserved,iim)
                      A0     A2    A1

            @{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} hookFunc(@{"struct Hook" LINK "gg:doc/NDK/Guide/Include/utility/hooks.h/File" 28} *hook,@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} reserved,
                          @{"struct IconIdentifyMsg" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 256} *iim);

            The reserved parameter will be set to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} (V44).

            The icon identification message contents are as follows:

                iim_SysBase
                iim_DOSBase
                iim_UtilityBase
                iim_IconBase
                    Library base pointers set up for your use.

                iim_FileLock
                    A lock on the object to return an icon for.
                    This lock is guaranteed to be non-@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.

                iim_ParentLock
                    A lock on the object's parent directory. This
                    may be @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if iim_FileLock points to a volume's
                    root directory. In such a case you should return
                    a @{"WBDISK" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 25} type icon instead of a @{"WBDRAWER" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 26} icon.

                iim_FIB
                    An initialized FileInfoBlock structure; prior
                    to passing iim_FIB to you, @{"Examine" LINK "gg:doc/NDK/Guide/dos/Examine"}(iim_FileLock,iim_FIB)
                    was called. Unlike the other members of this data
                    structure, you may modify the contents of iim_FIB.

                iim_FileHandle
                    If the object to return an icon for is a file,
                    this member will contain a file handle that can
                    be used to @{"Read()" LINK "gg:doc/NDK/Guide/dos/Read"} its contents. The file read
                    position will be set to the beginning of
                    the file. Note that this member can be @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.

                iim_Tags
                    A copy of the tag item list passed to @{"GetIconTagList()" LINK "GetIconTagList"};
                    if you are going to call @{"GetIconTagList()" LINK "GetIconTagList"} in your
                    hook, be sure to include this list as part of the
                    tag list you pass to it.

                    NOTE: This list does not include the tag
                          ICONA_FailIfUnavailable. Keep in mind that
                          the use of this tag can trigger a recursion
                          if the icon you tried to retrieve via
                          @{"GetIconTagList()" LINK "GetIconTagList"} does not exist: your hook
                          code will be invoked again.

            With the exception of iim_FIB, all members of the
            IconIdentifyMsg are read-only; you may pass them to
            functions like @{"Read()" LINK "gg:doc/NDK/Guide/dos/Read"}, @{"Examine()" LINK "gg:doc/NDK/Guide/dos/Examine"}, etc. but you
            may not @{"Close()" LINK "gg:doc/NDK/Guide/dos/Close"} files, @{"UnLock()" LINK "gg:doc/NDK/Guide/dos/UnLock"} locks or perform
            other functions that may result in the data structures
            getting deallocated or trashed.

            When you manage to find the right icon type for the object
            to be examined, you should return a pointer to a
            @{"struct DiskObject" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 65} that can be freed using @{"FreeDiskObject()" LINK "FreeDiskObject"}
            lateron. Note that you cannot just return a pointer to a
            statically initialized data structure in your code, you
            must return a pointer to a DiskObject allocated by icon.library
            itself, such as through @{"GetIconTagList()" LINK "GetIconTagList"}, @{"DupDiskObjectA()" LINK "DupDiskObjectA"}
            or @{"NewDiskObject()" LINK "NewDiskObject"}.

            The icon you return will be checked to make sure that its
            type matches the corresponding object. For example, returning
            a @{"WBDRAWER" LINK "gg:doc/NDK/Guide/Include/workbench/workbench.h/File" 26} icon for a volume root directory will cause that
            icon to be released and to be ignored.

        @{"ICONCTRLA_GetGlobalIdentifyHook" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 79} (@{"struct Hook" LINK "gg:doc/NDK/Guide/Include/utility/hooks.h/File" 28} **) -- Get the
            hook that is invoked when trying to find an icon
            for a file/drawer/volume that does not have an
            icon associated with it.

        @{"ICONCTRLA_SetGlobalMaxNameLength" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 84} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) -- icon.library has
            a built-in file name length limit that affects whether
            icon files are read and written. Currently, the default
            for this value is 25 characters. This is sufficient for
            an icon file name acceptable with the ROM filing system.
            This limit has two effects: if an icon file is to be
            written whose name would be longer than the limit,
            icon.library will silently abort the write attempt and
            pretend that the icon file has been written; this is done
            in order to avoid overwriting the original data file with
            the icon file. If an icon file was read that turns out to
            contain invalid icon data, another test is done in order to
            find out whether the file name was longer than the limit. If
            this is the case, it is assumed that the data was read from
            the original file rather than from the icon file. The library
            then pretends that the icon file was not found.

            This tag can be used to set this length limit. The limit
            must be in the range [8..255].

        @{"ICONCTRLA_GetGlobalMaxNameLength" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 85} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Retrieve the
            file name length limit used by icon.library.

        @{"ICONCTRLA_SetGlobalNewIconsSupport" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 70} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Select whether
            icon.libray should read and return coloured icon images stored
            in NewIcons format.
            This option defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONCTRLA_GetGlobalNewIconsSupport" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 71} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Query whether
            icon.library will read and return coloured icon images
            stored in NewIcons format.

        @{"ICONCTRLA_SetGlobalColorIconSupport" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 74} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) -- Select whether
            icon.libray should read and return coloured icon images.
            This option defaults to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}.

        @{"ICONCTRLA_GetGlobalColorIconSupport" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 75} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Query whether
            icon.library will read and return coloured icon images.

        @{"ICONA_ErrorCode" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 44} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Pointer to a @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} word variable to
            store error codes in. Note that this variable will be
            initialized regardless of whether an error occured or not.
            Thus, you can check for an error condition by comparing the
            variable contents against 0; 0 indicates success, all other
            values are error codes as defined by @{"dos.library" LINK "gg:doc/NDK/Guide/dos/MAIN"}.

        @{"ICONA_ErrorTagItem" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 47} (@{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} **) -- When an error occurs
            whilst processing the tag item list passed to @{"IconControl" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 85}(),
            you can have a pointer to the item that caused the error
            passed back via the @{"ICONA_ErrorTagItem" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 47} tag. Note that for
            TagItem lists allocated from the stack the address passed
            back to you may no longer be valid when control returns from
            @{"IconControl" LINK "gg:doc/NDK/Guide/Include/inline/icon.h/File" 85}().

        The following tags need a valid icon parameter to operate on.
        If the parameter is missing or invalid, it will be registered
        as an error.

        @{"ICONCTRLA_GetImageMask1" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 92} (@{"PLANEPTR" LINK "gg:doc/NDK/Guide/Include/graphics/gfx.h/File" 44} *) -- Pointer to the mask plane
            for the regular icon image (icon->do_Gadget.GadgetRender).
            Note that this may be @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.

        @{"ICONCTRLA_GetImageMask2" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 93} (@{"PLANEPTR" LINK "gg:doc/NDK/Guide/Include/graphics/gfx.h/File" 44} *) -- Pointer to the mask plane
            for the alternate icon image (icon->do_Gadget.SelectRender).
            Note that this may be @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.

        @{"ICONCTRLA_SetTransparentColor1" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 96} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) -- Set the transparent
            colour for a palette mapped icon image, in this case the
            regular icon image (icon->do_Gadget.GadgetRender). The
            colour must be among the valid palette entries, e.g. for
            a 16 colour image, the transparent colour may not be
            larger than 15. To make the icon image opaque, set its
            transparent colour to -1.

        @{"ICONCTRLA_GetTransparentColor1" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 97} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Get the transparent
            colour for a palette mapped icon image, in this case the
            regular icon image (icon->do_Gadget.GadgetRender). If
            the image is opaque, its "transparent colour" will be
            returned as -1. Note that this data is valid only for
            palette mapped icons; for other types, you may not
            receive any meaningful result.

        @{"ICONCTRLA_SetTransparentColor2" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 98} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) -- Set the transparent
            colour for a palette mapped icon image, in this case the
            alternate icon image (icon->do_Gadget.SelectRender). The
            colour must be among the valid palette entries, e.g. for
            a 16 colour image, the transparent colour may not be
            larger than 15. To make the icon image opaque, set its
            transparent colour to -1. Please note that you can set
            the alternate image transparent colour only if there is
            an alternate image.

        @{"ICONCTRLA_GetTransparentColor2" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 99} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) -- Get the transparent
            colour for a palette mapped icon image, in this case the
            alternate icon image (icon->do_Gadget.SelectRender). If
            the image is opaque, its "transparent colour" will be
            returned as -1. Note that this data is valid only for
            palette mapped icons; for other types, you may not
            receive any meaningful result. Likewise, if there is
            no alternate image, no information may be available.

        @{"ICONCTRLA_SetPalette1" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 102} (@{"struct ColorRegister" LINK "gg:doc/NDK/Guide/Include/datatypes/pictureclass.h/File" 243} *) -- Set the colour
