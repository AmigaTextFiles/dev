        @{"ICONCTRLA_SetAspectRatio" LINK "gg:doc/NDK/Guide/Include/workbench/icon.h/File" 128} (@{"UBYTE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 62}) -- Set the icon aspect ratio; this
