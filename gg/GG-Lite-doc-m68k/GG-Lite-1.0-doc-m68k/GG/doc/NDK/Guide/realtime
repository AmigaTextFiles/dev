@NODE "CreatePlayer" "realtime.library/CreatePlayer()"
@{b}

   NAME@{ub}
        @{"CreatePlayerA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 34} -- create a player and link it to a conductor. (V37)
        @{"CreatePlayer" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 39} -- varargs stub for @{"CreatePlayerA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 34}(). (V37)@{b}

   SYNOPSIS@{ub}
        player = @{"CreatePlayerA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 34}(tagList);
        D0                     A0

        @{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *@{"CreatePlayerA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 34}(@{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *);

        player = @{"CreatePlayer" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 39}(firstTag, ...);

        @{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *@{"CreatePlayer" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 39}(Tag, ...);@{b}

   FUNCTION@{ub}
        Creates a player structure with the desired attributes.@{b}

   INPUTS@{ub}
        tagList - pointer to an array of tags providing optional extra
                  parameters, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}@{b}

   TAGS@{ub}
        @{"PLAYER_Name" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 141} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) - name of the player (generally the
                        application's name). Default is no name. (V37)

        @{"PLAYER_Hook" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 140} (@{"struct Hook" LINK "gg:doc/NDK/Guide/Include/utility/hooks.h/File" 28} *) - function to call when time changes
                        occur. Default is no function. The Hook is called
                        with:

                                A0 - address of Hook structure
                                A1 - message, currently pmTime or pmState
                                A2 - address of Player structure

                        Note that your hook function is not necessarily called
                        @{"TICK_FREQ" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 46} times per second. This is the maximum number
                        of times it can be called. (V37)

        @{"PLAYER_Priority" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 142} (@{"BYTE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 58}) - priority for the player, from -128 to +127.
                        Default is 0. (V37)

        @{"PLAYER_Conductor" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 143} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) - name of the conductor to link with. If
                        this conductor doesn't exist, it is created
                        automatically. If ~0 is passed, creates a private
                        conductor. (V37)

        @{"PLAYER_Ready" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 144} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - set/clear the "ready" flag. Default is @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.
                        (V37)

        @{"PLAYER_AlarmTime" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 145} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) - sets this player's alarm time, and the
                        @{"PLAYERF_ALARMSET" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 123} flag. (V37)

        @{"PLAYER_Alarm" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 146} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - if @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} sets the @{"PLAYERF_ALARMSET" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 123} flag, @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}
                        clears the flag. Default is @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}. (V37)

        @{"PLAYER_AlarmSigTask" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 147} (@{"struct Task" LINK "gg:doc/NDK/Guide/Include/exec/tasks.h/File" 24} *) - task to signal on notify or
                        alarm. Default is no task. Having no task automatically
                        forces the @{"PLAYERF_ALARMSET" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 123} flag off. (V37)

        @{"PLAYER_AlarmSigBit" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 148} (@{"BYTE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 58}) - signal bit to use on alarm or -1 to
                        disable. Default is -1. Having a signal bit of -1
                        automatically forces the @{"PLAYERF_ALARMSET" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 123} flag
                        off. (V37)

        @{"PLAYER_Quiet" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 150} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - when @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}, this player is ignored. Mainly
                        used by external sync applications. Default is @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.
                        (V37)

        @{"PLAYER_UserData" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 151} (@{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} *) - sets the player's UserData value.
                        Default is @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}. (V37)

        @{"PLAYER_ID" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 152} (@{"UWORD" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 55}) - sets the player's ID value. Default is 0. (V37)

        @{"PLAYER_Conducted" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 149} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - if @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} sets the @{"PLAYERF_CONDUCTED" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 125} flag,
                        @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98} clears the flag. Default is @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}. (V37)

        @{"PLAYER_ExtSync" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 153} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - if @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}, attempts to become external sync
                        source. (V37)

        @{"PLAYER_ErrorCode" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 154} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) - optional pointer to a longword which
                        will receive an error code whenever this function
                        fails. Possible error values currently include:
                          @{"RTE_NOMEMORY" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 192} - memory allocation failed
                          @{"RTE_NOTIMER" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 194}  - timer (CIA) allocation failed@{b}

   RESULTS@{ub}
        player - a pointer to a Player structure on success or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} on
                 failure. When @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} is returned, an error code can be returned
                 in the longword variable pointed to by the optional
                 @{"PLAYER_ErrorCode" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 154} tag.@{b}

   SEE ALSO@{ub}
        @{"DeletePlayer()" LINK "DeletePlayer"}, @{"GetPlayerAttrs()" LINK "GetPlayerAttrsA"}, @{"SetPlayerAttrs()" LINK "SetPlayerAttrs"}

@ENDNODE
@NODE "DeletePlayer" "realtime.library/DeletePlayer()"
@{b}

   NAME@{ub}
        @{"DeletePlayer" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 43} -- delete a player. (V37)@{b}

   SYNOPSIS@{ub}
        @{"DeletePlayer" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 43}(player);
                     A0

        @{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} @{"DeletePlayer" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 43}(@{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *);@{b}

   FUNCTION@{ub}
        Deletes the specified player.

        Flushes the conductor that the player was connected to if this
        is the last player connected to that conductor.@{b}

   INPUTS@{ub}
        player - Player structure to delete, as allocated by
                 @{"CreatePlayer()" LINK "CreatePlayer"}. May be @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, in which case this function
                 does nothing.@{b}

   SEE ALSO@{ub}
        @{"CreatePlayer()" LINK "CreatePlayer"}

@ENDNODE
@NODE "ExternalSync" "realtime.library/ExternalSync()"
@{b}

   NAME@{ub}
        @{"ExternalSync" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 60} -- provide external source clock for a player's
                        conductor. (V37)@{b}

   SYNOPSIS@{ub}
        result = @{"ExternalSync" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 60}(player, minTime, maxTime);
        D0                    A0      D0       D1

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"ExternalSync" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 60}(@{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *, @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}, @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51});@{b}

   FUNCTION@{ub}
        Allows external applications to constrain conductor time between
        minTime and maxTime, with the restraint that time can never run
        backwards. Does nothing if the given player is not the
        current External Sync source.@{b}

   INPUTS@{ub}
        player - player referencing the conductor to change
        minTime, maxTime - time constraints@{b}

   RESULTS@{ub}
        result - @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if everything went OK, @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98} if the player is not the
                 current sync source or there is no conductor for the player.

@ENDNODE
@NODE "FindConductor" "realtime.library/FindConductor()"
@{b}

   NAME@{ub}
        @{"FindConductor" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 68} -- find a conductor by name. (V37)@{b}

   SYNOPSIS@{ub}
        conductor = @{"FindConductor" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 68}(name);
        D0                        A0

        @{"struct Conductor" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 58} *@{"FindConductor" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 68}(@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67});@{b}

   FUNCTION@{ub}
        Returns the conductor with the given name or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if not found.

        The conductor list must be locked before calling this function. This
        is done by calling @{"LockRealTime" LINK "LockRealTime"}(@{"RT_CONDUCTORS" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 185}).@{b}

   INPUTS@{ub}
        name - name of conductor to find.@{b}

   RESULTS@{ub}
        conductor - pointer to a Conductor structure, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if not found.@{b}

   SEE ALSO@{ub}
        @{"NextConductor()" LINK "NextConductor"}, @{"LockRealTime()" LINK "LockRealTime"}, @{"UnlockRealTime()" LINK "UnlockRealTime"}

@ENDNODE
@NODE "GetPlayerAttrsA" "realtime.library/GetPlayerAttrsA()"
@{b}

   NAME@{ub}
        @{"GetPlayerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 72} -- get the attributes of a player. (V37)
        @{"GetPlayerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 77} -- varargs stub for @{"GetPlayerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 72}(). (V37)@{b}

   SYNOPSIS@{ub}
        numProcessed = @{"GetPlayerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 72}(player, tagList);
        D0                             A0      A1

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"GetPlayerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 72}(@{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *, @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *);

        numProcessed = @{"GetPlayerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 77}(player, firstTag, ...);

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"GetPlayerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 77}(@{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *, Tag, ...);@{b}

   FUNCTION@{ub}
        Retrieves the attributes of the specified player, according to the
        attributes chosen in the tag list. For each entry in the tag list,
        ti_Tag identifies the attribute, and ti_Data is a pointer to
        the longword variable where you wish the result to be stored.@{b}

   INPUTS@{ub}
        player - pointer to the player in question. May be @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, in which case
                 this function returns 0
        tagList - pointer to TagItem list.@{b}

   TAGS@{ub}
        @{"PLAYER_Name" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 141} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) - return the name of the player, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}
                        if this is an unnamed player. (V37)

        @{"PLAYER_Hook" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 140} (@{"struct Hook" LINK "gg:doc/NDK/Guide/Include/utility/hooks.h/File" 28} *) - returns a pointer to the current
                        function called when time changes occur, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}
                        if no function is currently installed. (V37)

        @{"PLAYER_Priority" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 142} (@{"BYTE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 58}) - returns the priority for the player. (V37)

        @{"PLAYER_Conductor" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 143} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) - returns the name of the conductor this
                        player is currently linked with, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if the
                        player is not linked to a conductor. (V37)

        @{"PLAYER_Ready" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 144} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - gets the state of the "ready" flag. (V37)

        @{"PLAYER_AlarmTime" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 145} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) - gets the current alarm time for this
                        player. (V37)

        @{"PLAYER_Alarm" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 146} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - returns @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if this player's alarm is
                        currently on. (V37)

        @{"PLAYER_AlarmSigTask" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 147} (@{"struct Task" LINK "gg:doc/NDK/Guide/Include/exec/tasks.h/File" 24} *) - returns a pointer to the task to
                        signal on notify or alarm, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if there is no
                        task to signal. (V37)

        @{"PLAYER_AlarmSigBit" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 148} (@{"BYTE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 58}) - returns the signal bit to use on alarm or
                        -1 if no signal. (V37)

        @{"PLAYER_Quiet" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 150} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - returns @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if this player is currently being
                        ignored. @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98} if not. (V37)

        @{"PLAYER_UserData" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 151} (@{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} *) - returns the current value of this player's
                        UserData. (V37)

        @{"PLAYER_ID" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 152} (@{"UWORD" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 55}) - gets the value of this player's ID. (V37)

        @{"PLAYER_Conducted" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 149} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - returns @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if this player is currently
                        being conducted. (V37)

        @{"PLAYER_ExtSync" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 153} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - returns @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if this player is the current
                        external sync source. (V37)@{b}

   RESULTS@{ub}
        numProcessed - the number of attributes successfully filled in.@{b}

   SEE ALSO@{ub}
        @{"CreatePlayer()" LINK "CreatePlayer"}, @{"SetPlayerAttrs()" LINK "SetPlayerAttrs"}

@ENDNODE
@NODE "LockRealTime" "realtime.library/LockRealTime()"
@{b}

   NAME@{ub}
        @{"LockRealTime" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 26} -- prevent other tasks from changing internal structures.
                        (V37)@{b}

   SYNOPSIS@{ub}
        lockHandle = @{"LockRealTime" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 26}(lockType);
        D0                        D0

        @{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} @{"LockRealTime" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 26}(@{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52});@{b}

   FUNCTION@{ub}
        This routine will lock the internal sempahores in the RealTime library.
        If they are already locked by another task, this routine will wait
        until they are free.@{b}

   INPUTS@{ub}
        lockType - the internal list to lock. Only @{"RT_CONDUCTORS" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 185} is
                   currently defined.@{b}

   RESULT@{ub}
        handle - if lockType is valid, returns a value that must be passed
                 later to @{"UnlockRealTime()" LINK "UnlockRealTime"} to unlock the list. Returns @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}
                 if passed an invalid lock type.@{b}

   SEE ALSO@{ub}
        @{"UnlockRealTime()" LINK "UnlockRealTime"}

@ENDNODE
@NODE "NextConductor" "realtime.library/NextConductor()"
@{b}

   NAME@{ub}
        @{"NextConductor" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 64} -- return the next conductor on realtime.library's
                         conductor list. (V37)@{b}

   SYNOPSIS@{ub}
        conductor = @{"NextConductor" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 64}(previousConductor);
        D0                        A0

        @{"struct Conductor" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 58} *@{"NextConductor" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 64}(@{"struct Conductor" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 58} *);@{b}

   FUNCTION@{ub}
        Returns the next conductor on realtime.library's conductor list. If
        previousConductor is @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, returns the first conductor in the list.
        Returns @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if no more conductors.

        The conductor list must be locked before calling this function. This
        is done by calling @{"LockRealTime" LINK "LockRealTime"}(@{"RT_CONDUCTORS" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 185}).@{b}

   INPUTS@{ub}
        previousConductor - previous conductor or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} to get first conductor.@{b}

   RESULTS@{ub}
        conductor - next conductor on the list, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if no more.@{b}

   SEE ALSO@{ub}
        @{"FindConductor()" LINK "FindConductor"}, @{"LockRealTime()" LINK "LockRealTime"}, @{"UnlockRealTime()" LINK "UnlockRealTime"}

@ENDNODE
@NODE "SetConductorState" "realtime.library/SetConductorState()"
@{b}

   NAME@{ub}
        @{"SetConductorState" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 56} -- change the play state of a player's
                             conductor. (V37)@{b}

   SYNOPSIS@{ub}
        result = @{"SetConductorState" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 56}(player, state, time);
        D0                         A0      D0     D1

        @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} @{"SetConductorState" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 56}(@{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52}, @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51});@{b}

   FUNCTION@{ub}
        Changes the play state of the conductor referenced by the
        player. The states can be @{"CONDSTATE_STOPPED" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 85}, @{"CONDSTATE_PAUSED" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 86},
        @{"CONDSTATE_LOCATE" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 87}, @{"CONDSTATE_RUNNING" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 88}, or the special value
        @{"CONDSTATE_METRIC" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 93} which asks the highest priority conducted node
        to do a @{"CONDSTATE_LOCATE" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 87}, or the special value @{"CONDSTATE_SHUTTLE" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 94}
        which informs the players that the clock value is changing, but the
        clock isn't actually running. Note that going from @{"CONDSTATE_PAUSED" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 86}
        to @{"CONDSTATE_RUNNING" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 88} does not reset the cdt_ClockTime of the
        conductor.@{b}

   INPUTS@{ub}
        player - player referencing the conductor to change
        state - new play state of conductor
        time - start time offset in realtime.library heartbeat units@{b}

   RESULTS@{ub}
        result - 0 if everything went OK, or a realtime.library error code
                 if an error occured. These currently include @{"RTE_PLAYING" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 195}
                 and @{"RTE_NOCONDUCTOR" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 193}.

@ENDNODE
@NODE "SetPlayerAttrs" "realtime.library/SetPlayerAttrs()"
@{b}

   NAME@{ub}
        @{"SetPlayerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 47} -- set the attributes of a player. (V37)
        @{"SetPlayerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 52} -- varargs stub for @{"SetPlayerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 47}(). (V37)@{b}

   SYNOPSIS@{ub}
        result = @{"SetPlayerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 47}(player, tagList);
        D0                       A0      A1

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"SetPlayerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 47}(@{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *, @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *);

        result = @{"SetPlayerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 52}(player,firstTag, ...);

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"SetPlayerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/realtime.h/File" 52}(@{"struct Player" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 106} *, Tag, ...);@{b}

   FUNCTION@{ub}
        Changes the attributes of the specified player, according to the
        attributes chosen in the tag list. If an attribute is not provided
        in the tag list, its value remains unchanged.@{b}

   INPUTS@{ub}
        player - player to set the attributes of.
        tagList - pointer to an array of tags specifying the attributes
                  to change, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.@{b}

   TAGS@{ub}
        @{"PLAYER_Name" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 141} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) - name of the player (generally the
                        application's name). (V37)

        @{"PLAYER_Hook" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 140} (@{"struct Hook" LINK "gg:doc/NDK/Guide/Include/utility/hooks.h/File" 28} *) - function to call when time changes
                        occur. The Hook is called
                        with:

                                A0 - address of Hook structure
                                A1 - message, currently pmTime or pmState
                                A2 - address of Player structure

                        Note that your hook function is not necessarily called
                        @{"TICK_FREQ" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 46} times per second. This is the maximum number
                        of times it can be called. (V37)

        @{"PLAYER_Priority" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 142} (@{"BYTE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 58}) - priority for the player, from -128 to +127.
                        (V37)

        @{"PLAYER_Conductor" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 143} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) - name of the conductor to link with. If
                        @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, delink from conductor. (V37)

        @{"PLAYER_Ready" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 144} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - set/clear the "ready" flag. (V37)

        @{"PLAYER_AlarmTime" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 145} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) - sets this player's alarm time, and the
                        @{"PLAYERF_ALARMSET" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 123} flag. (V37)

        @{"PLAYER_Alarm" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 146} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - if @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} sets the @{"PLAYERF_ALARMSET" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 123} flag, @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}
                        clears the flag. (V37)

        @{"PLAYER_AlarmSigTask" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 147} (@{"struct Task" LINK "gg:doc/NDK/Guide/Include/exec/tasks.h/File" 24} *) - task to signal on notify or
                        alarm. Setting this to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} automatically clears the
                        @{"PLAYERF_ALARMSET" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 123} flag. (V37)

        @{"PLAYER_AlarmSigBit" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 148} (@{"BYTE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 58}) - signal bit to use on alarm or -1 to
                        disable. Setting this to -1 automatically clears the
                        @{"PLAYERF_ALARMSET" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 123}. (V37)

        @{"PLAYER_Quiet" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 150} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - when @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}, this player is ignored. Mainly
                        used by external sync applications. (V37)

        @{"PLAYER_UserData" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 151} (@{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} *) - sets this player's UserData value. (V37)

        @{"PLAYER_ID" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 152} (@{"UWORD" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 55}) - sets this player's ID value. (V37)

        @{"PLAYER_Conducted" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 149} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - if @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} sets the @{"PLAYERF_CONDUCTED" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 125} flag,
                        @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98} clears the flag. (V37)

        @{"PLAYER_ExtSync" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 153} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - if @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95}, attempt to become external sync
                        source. If @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}, release external sync. (V37)

        @{"PLAYER_ErrorCode" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 154} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} *) - optional pointer to a longword which
                        will receive an error code whenever this function
                        fails. Possible error values currently include:
                          RTE_NOMEM   - memory allocation failed
                          @{"RTE_NOTIMER" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 194} - timer (CIA) allocation failed@{b}

   RESULTS@{ub}
        result - @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if all went well, @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98} if there was an error.
                 When an error occurs, an error code can be returned
                 in the longword variable pointed to by the optional
                 @{"PLAYER_ErrorCode" LINK "gg:doc/NDK/Guide/Include/libraries/realtime.h/File" 154} tag.@{b}

   SEE ALSO@{ub}
        @{"CreatePlayer()" LINK "CreatePlayer"}, @{"DeletePlayer()" LINK "DeletePlayer"}, @{"GetPlayerAttrs()" LINK "GetPlayerAttrsA"}

@ENDNODE
