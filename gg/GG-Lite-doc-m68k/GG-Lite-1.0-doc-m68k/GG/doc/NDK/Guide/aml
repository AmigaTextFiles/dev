        @{"ScanFolderIndex" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 136}(folder, hook, flags)
        D0                        A0            A1      DO

        void @{"ScanFolderIndex" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 136}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} folder, @{"struct Hook" LINK "gg:doc/NDK/Guide/Include/utility/hooks.h/File" 28} *hook, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} flags);@{b}

