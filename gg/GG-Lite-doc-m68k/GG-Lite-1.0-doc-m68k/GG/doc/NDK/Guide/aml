@NODE "AddArticlePartA" "aml.library/AddArticlePartA()"
@{b}

NAME@{ub}
        @{"AddArticlePartA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 195} -- Add a part to an article
        @{"AddArticlePart" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 200} -- Varargs stub for @{"AddArticlePartA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 195}@{b}

SYNOPSIS@{ub}
        success = @{"AddArticlePartA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 195}(article, part, tags)
        D0                                      A0              A1         A2

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"AddArticlePartA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 195}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} article, @{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} part, @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *tags);

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"AddArticlePart" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 200}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} article, @{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} part, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} tag1, ...);@{b}

FUNCTION@{ub}
        Adds a previously allocated article part to the specified
        article.  Parts added are always in order of addition, which is
        how the resulting article will be built.

        Note that at least one part must be added to the article, which
        is normally considered the main body of the article.@{b}

INPUTS@{ub}
        article - Pointer to an article object
        part - Pointer to an article part object
        tags - An array of TagItems@{b}

TAGS@{ub}
        See @{"SetArticlePartDataA()" LINK "SetArticlePartDataA"}.@{b}

RETURNS@{ub}
        success - 1 if successful, 0 otherwise.@{b}

SEE ALSO@{ub}
        @{"RemArticlePart" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 204}()

@ENDNODE
@NODE "AddCustomField" "aml.library/AddCustomField()"
@{b}

NAME@{ub}
        @{"AddCustomField" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 335} -- Add a custom data field to an address
                entry object@{b}

SYNOPSIS@{ub}
        success = @{"AddCustomField" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 335}(addr, field, data)
        D0                                      A0       A1             A2

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"AddCustomField" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 335}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} addr, @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} field, @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} data);@{b}

FUNCTION@{ub}
        Adds the specified data, which currently must be a string,
        to the address entry object, identifiable by the field name.
        Field names must be unique.@{b}

RETURNS@{ub}
        success - 1 if successful, 0 otherwise.@{b}

SEE ALSO@{ub}
        @{"RemCustomField()" LINK "RemCustomField"}, @{"GetCustomFieldData()" LINK "GetCustomFieldData"}

@ENDNODE
@NODE "AddFolderArticle" "aml.library/AddFolderArticle()"
@{b}

NAME@{ub}
        @{"AddFolderArticle" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 120} -- Adds an article to a folder@{b}

SYNOPSIS@{ub}
        success = @{"AddFolderArticle" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 120}(folder, type, data)
        D0                                              A0       D0      A1

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"AddFolderArticle" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 120}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} folder, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} type, @{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} data);@{b}

FUNCTION@{ub}
        This function attempts to add the the specified article
        to the folder.

        The type parameter specifies how the article data will
        be interpreted.  The following values are valid:

                @{"ARTICLE_TYPE_FILE" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 245} - data is a filename of
                        an RFC822/MIME formatted message.
                @{"ARTICLE_TYPE_ARTICLE" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 246} - data is a pointer to
                        an allocated article object.
                @{"ARTICLE_TYPE_BUFFER" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 247} - data is a pointer to a
                        buffer, which contains an RFC822/MIME
                        style message.@{b}

INPUTS@{ub}
        folder - Pointer to a folder object
        type - One of the types listed above
        data - Pointer to the article data@{b}

RETURNS@{ub}
        success - 1 if successful, 0 otherwise@{b}

SEE ALSO@{ub}
        @{"RemFolderArticle()" LINK "RemFolderArticle"}

@ENDNODE
