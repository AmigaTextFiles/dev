NAME@{ub}
        @{"SetServerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 46} -- Set attributes for a server object
        @{"SetServerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 51} -- Varargs stub for @{"SetServerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 51}@{b}

