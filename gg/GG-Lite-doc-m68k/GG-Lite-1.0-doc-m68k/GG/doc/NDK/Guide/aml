@NODE "CreateServerA" "aml.library/CreateServerA()"
@{b}

NAME@{ub}
        @{"CreateServerA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 33} -- Create a server object
        @{"CreateServer" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 38} -- Varargs stub for @{"CreateServerA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 33}@{b}

SYNOPSIS@{ub}
        server = @{"CreateServerA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 33}(tags)
        D0                                      A0

        @{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} @{"CreateServerA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 33}(@{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *tags);

        @{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} @{"CreateServer" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 38}(@{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} tag1, ...);@{b}

FUNCTION@{ub}
        This function creates a new server object, for use
        as a POP or SMTP server, an address book, or whatever
        else.

        Note that when using a server object with folders or
        address entries, the server must remain valid for the life
        of the associated object.@{b}

INPUTS@{ub}
        tags - An array of TagItems@{b}

TAGS@{ub}
        See @{"SetServerAttrsA()" LINK "SetServerAttrsA"} for details.@{b}

RETURNS@{ub}
        server - Pointer to a server object@{b}

SEE ALSO@{ub}
        @{"DisposeServer()" LINK "DisposeServer"}, @{"SetServerAttrsA()" LINK "SetServerAttrsA"}

@ENDNODE
@NODE "Decode" "aml.library/Decode()"
@{b}

NAME@{ub}
        Decode -- Decode a stream/buffer@{b}

SYNOPSIS@{ub}
        length = Decode(decoder, type)
        D0                              A0       A1

        @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} Decode(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} decoder, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} type);@{b}

FUNCTION@{ub}
        Decodes data from a input stream or buffer to
        an output stream or buffer.  The corresponding
        stream/buffer must be set up when the decoder is
        created via @{"CreateDecoderA()" LINK "CreateDecoderA"}.@{b}

INPUTS@{ub}
        decoder - Pointer to a decoder object.
        type - One of the following values, that specifies
                how to decode the input stream/buffer:

                @{"DECODE_COPY" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 311} - Do nothing to the stream.
                @{"DECODE_QP" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 312} - Decode the stream as quoted-printable
                        data.  See the MIME RFCs for details.
                @{"DECODE_B64" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 313} - Decode the stream as base64.
                @{"DECODE_UU" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 314} - Decode the stream as uuencoded.
                @{"DECODE_ROT" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 316} - Decode the stream as ROT-13 style.@{b}

RETURNS@{ub}
        length - The resulting length of the output stream or
                buffer.@{b}

SEE ALSO@{ub}
        @{"CreateDecoderA()" LINK "CreateDecoderA"}

@ENDNODE
