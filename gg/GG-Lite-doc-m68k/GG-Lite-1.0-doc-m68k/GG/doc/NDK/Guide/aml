@NODE "GetServerArticles" "aml.library/GetServerArticles()"
@{b}

NAME@{ub}
        @{"GetServerArticles" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 68} -- Get articles from a server@{b}

SYNOPSIS@{ub}
        value = @{"GetServerArticles" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 68}(server, incoming, hook, flags);
        D0                                       A0             A1              A2      D0

        @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} @{"GetServerArticles" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 68}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} server, @{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} incoming,
                        @{"struct Hook" LINK "gg:doc/NDK/Guide/Include/utility/hooks.h/File" 28} *hook, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} flags);@{b}

FUNCTION@{ub}
        This function tells the server to retrieve its
        articles.

        If a hook is provided, it will be called for each article
        retrieved, with the server object passed in A2 and an
        ArticleDisposition structure in A1.  This gives the
        application a chance to parse the article and determine its
        final destination, by setting the ad_Folder pointer.  If this
        pointer is @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}, or no hook is provided, the folder object
        specified by incoming will be used as the destination.  If
        that is not provided, the article is discarded.@{b}

INPUTS@{ub}
        server - Pointer to a server object
        incoming - Pointer to the default incoming folder object
        hook - A hook used for progress callbacks
        flags - One or more of the following:

                SF_USESPOOL   - Receive messages to server spool vs. folder.
                SF_DELARTICLE - Delete articles from the server
                        after successful retrieval.
                SF_BYPASSHOOK - Don't use the progress hook.@{b}

RETURNS@{ub}
        value - 0 for success, -1 or a DOS error code for failure@{b}

SEE ALSO@{ub}
        @{"GetServerHeaders()" LINK "GetServerHeaders"}, @{"CreateServerA()" LINK "CreateServerA"}

@ENDNODE
@NODE "GetServerAttrsA" "aml.library/GetServerAttrsA()"
@{b}

NAME@{ub}
        @{"GetServerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 55} -- Get attributes on a server object
        @{"GetServerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 60} -- Varargs stub for @{"GetServerAttrs" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 60}@{b}

SYNOPSIS@{ub}
        value = @{"GetServerAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 55}(server, tags);
        D0                                      A0         A1

