@NODE "SetAddressEntryAttrsA" "aml.library/SetAddressEntryAttrsA()"
@{b}

NAME@{ub}
        @{"SetAddressEntryAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 295} -- Set attributes of an address entry object
        @{"SetAddressEntryAttrs" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 300} -- Varargs stub for @{"SetAddressEntryAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 295}@{b}

SYNOPSIS@{ub}
        value = @{"SetAddressEntryAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 295}(addr, tags)
        D0                                              A0       A1

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"SetAddressEntryAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 295}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} addr, @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *tags);

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"SetAddressEntryAttrs" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 300}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} addr, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} tag1, ...);@{b}

FUNCTION@{ub}
        Sets the requested attributes from the specified
        address entry object.@{b}

INPUTS@{ub}
        addr - Pointer to a previously-created address entry object.
        tags - Pointer to an array of TagItems.@{b}

TAGS@{ub}
        @{"ADDRESSA_Group" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 263}, @{"ADDRESSA_Name" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 254}, @{"ADDRESSA_Address" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 255},
        @{"ADDRESSA_City" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 256}, @{"ADDRESSA_State" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 257}, @{"ADDRESSA_Zip" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 258},
        @{"ADDRESSA_Country" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 259}, @{"ADDRESSA_Phone" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 260}, @{"ADDRESSA_Fax" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 261},
        @{"ADDRESSA_Comments" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 262}, @{"ADDRESSA_URL" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 265}, @{"ADDRESSA_EMail" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 266},
        @{"ADDRESSA_Flags" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 264}, @{"ADDRESSA_ICQ" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 269}, @{"ADDRESSA_FTP" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 270},
        @{"ADDRESSA_Company" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 271}, @{"ADDRESSA_JobTitle" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 272}, @{"ADDRESSA_MobilePhone" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 273},
        @{"ADDRESSA_ImageFile" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 274} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67})
                The meanings of these tags should be obvious, we hope. :)

        @{"ADDRESSA_EntryType" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 275} (@{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52})
                What type of address entry this is; See <@{"libraries/aml.h" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File"}> for details.@{b}


RETURNS@{ub}
        value - The number of attributes successfully set.@{b}

