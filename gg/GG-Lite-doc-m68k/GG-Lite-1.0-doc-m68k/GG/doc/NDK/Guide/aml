@NODE "SetArticleAttrsA" "aml.library/SetArticleAttrsA()"
@{b}

NAME@{ub}
        @{"SetArticleAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 173} -- Set article attributes
        @{"SetArticleAttrs" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 178} -- Varargs stub for @{"SetArticleAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 173}@{b}

SYNOPSIS@{ub}
        value = @{"SetArticleAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 173}(article, tags)
        D0                                      A0               A1

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"SetArticleAttrsA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 173}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} article, @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *)

        @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52} @{"SetArticleAttrs" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 178}(@{"APTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 49} article, @{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52}, ...)@{b}

FUNCTION@{ub}
        Sets attributes on an article object@{b}

INPUTS@{ub}
        article - The article object to set attributes on
        tags - An array of TagItems containing attributes to set@{b}

TAGS@{ub}
        @{"ARTICLEA_To" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 204}, @{"ARTICLEA_Cc" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 205}, @{"ARTICLEA_Bcc" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 206}, @{"ARTICLEA_From" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 207},
        @{"ARTICLEA_ReplyTo" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 208}, @{"ARTICLEA_Subject" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 211}, @{"ARTICLEA_ContentType" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 213},
        @{"ARTICLEA_ContentSubType" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 214}, @{"ARTICLEA_ContentEncoding" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 212},
        @{"ARTICLEA_Groups" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 221}, @{"ARTICLEA_XFace" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 218}, @{"ARTICLEA_MessageID" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 219},
        @{"ARTICLEA_Reference" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 220}, @{"ARTICLEA_SigFile" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 224} (all @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) - Each tag
                corresponds to an appropriate message/newsarticle header
                as defined by RFC 822; 2045, et. al.  Note that these
                tags are @{"NOT" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 1500} cumulative, i.e., specifying the
                ADDRESSA_To tag twice will not add the next address to
                the "To" list, but replace it.  Please see the
                appropriate RFC for details on the proper formation
                of each header's data.

        @{"ARTICLEA_CustomHeaders" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 225} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}) - A set of valid article headers,
                each terminated by a "\\r\\n" sequence.

        @{"ARTICLEA_Date" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 209} (@{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}), @{"ARTICLEA_DateVal" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 210} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) - Allows you to
                set the Date: header using either a previous header's
                value (_Date), or a 32-bit integer value based off the epoch
                (_DateVal).

        @{"ARTICLEA_Flags" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 216} (@{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51}) - Sets appropriate flags for the
                article, like unread, reply, forward, etc. status.  See the
                library's header file for valid values.

        @{"ARTICLEA_ReturnReceipt" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File" 223} (@{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91}) - Sets whether the appropriate
                return receipt headers should be generated.  (Set only.)@{b}

RETURNS@{ub}
        value - The number of attributes successfully set.@{b}

SEE ALSO@{ub}
        @{"libraries/aml.h" LINK "gg:doc/NDK/Guide/Include/libraries/aml.h/File"}

@ENDNODE
