@{b}

NAME@{ub}
        @{"SetArticlePartDataA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 252} -- Sets an article part's data
        @{"SetArticlePartData" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 257} -- Varargs stub for @{"SetArticlePartDataA" LINK "gg:doc/NDK/Guide/Include/inline/aml.h/File" 252}@{b}

