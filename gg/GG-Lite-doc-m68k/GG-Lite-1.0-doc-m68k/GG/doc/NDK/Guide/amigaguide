@NODE "SendAmigaGuideCmdA" "amigaguide.library/SendAmigaGuideCmdA()"
@{b}

   NAME@{ub}
        @{"SendAmigaGuideCmdA" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 98} - Send a command string to AmigaGuide (V34)@{b}

   SYNOPSIS@{ub}
        success = @{"SendAmigaGuideCmdA" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 98} (handle, cmd, attrs );
          d0                            a0    d0   d1

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"SendAmigaGuideCmdA" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 98} (@{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87}, @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}, @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *) ;

        success = @{"SendAmigaGuideCmd" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 103} (handle, cmd, tag1, ...);

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"SendAmigaGuideCmd" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 103} (@{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87}, @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67}, Tag);@{b}

   FUNCTION@{ub}
        This function sends a command string to an AmigaGuide system.  The
        command can consist of any valid AmigaGuide action command.

        The following are the currently valid action commands:

        ALINK <name> - Load the named node into a new window.

        LINK <name> - Load the named node.

        RX <macro> - Execute an ARexx macro.

        RXS <cmd> - Execute an ARexx string file.  To display a picture,
            use 'ADDRESS COMMAND DISPLAY <picture name>', to
            display a text file 'ADDRESS COMMAND MORE <doc>'.

        @{"CLOSE" LINK "gg:doc/NDK/Guide/Include/intuition/iobsolete.h/File" 98} - Close the window (should only be used on windows
            that were started with ALINK).

        QUIT - Shutdown the current database.@{b}

   INPUTS@{ub}
        handle - Handle to an AmigaGuide system.

        cmd - Command string.

        attrs - Future expansion, must be set to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} for now.@{b}

   TAGS@{ub}
        @{"AGA_Context" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 69} (@{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52}) - Data is used as an index into nag_Context
            array.  This is used to build and send a LINK command.@{b}

   EXAMPLE@{ub}

        /* bring up help on a particular subject */
        @{"SendAmigaGuideCmd" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 103}(handle, "LINK MAIN", @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101});@{b}

   RETURNS@{ub}
        Returns @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if the message was sent, otherwise returns @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.@{b}

   BUGS@{ub}
        ALINK does not open a new window when using V39.@{b}

   SEE ALSO@{ub}

@ENDNODE
@NODE "SendAmigaGuideContextA" "amigaguide.library/SendAmigaGuideContextA()"
