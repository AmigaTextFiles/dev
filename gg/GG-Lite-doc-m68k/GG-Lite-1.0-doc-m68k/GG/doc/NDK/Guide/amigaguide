        #define HELP_FILEREQ   1
        #define HELP_PRINT     2
        #define HELP_ABOUT     3

        ...

        @{"struct NewAmigaGuide" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 103} nag = {@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}};

        /* initialize the context table */
        nag.nag_Context = ContextArray;

        ...

        /* bring up help on a particular subject */
        @{"SetAmigaGuideContext" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 85}(handle, HELP_ABOUT, @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101});@{b}

   RETURNS@{ub}
        success - Returns @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if a valid context ID was passed,
            otherwise returns @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.@{b}

   SEE ALSO@{ub}
        @{"SendAmigaGuideContextA()" LINK "SendAmigaGuideContextA"}, @{"SendAmigaGuideCmdA()" LINK "SendAmigaGuideCmdA"}

