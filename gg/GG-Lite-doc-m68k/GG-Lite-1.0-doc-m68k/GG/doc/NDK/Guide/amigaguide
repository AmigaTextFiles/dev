        @{"STRPTR" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 67} ContextArray[] =
        {
            "MAIN",
            "FILEREQ",
            "PRINT",
            "ABOUT",
            @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}
        };

        /* quickie defines */
