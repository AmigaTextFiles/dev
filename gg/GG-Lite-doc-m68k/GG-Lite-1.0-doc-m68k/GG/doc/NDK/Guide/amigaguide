    SYNOPSIS@{ub}
        key = @{"LockAmigaGuideBase" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 38} (@{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87} handle);
