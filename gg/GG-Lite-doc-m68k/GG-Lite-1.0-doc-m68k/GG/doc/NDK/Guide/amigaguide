        d0                                 a0      d0

        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"SendAmigaGuideContextA" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 89} (@{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87}, @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *);

