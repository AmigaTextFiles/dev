        @{"LONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 51} @{"LockAmigaGuideBase" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 38} (@{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87});@{b}

