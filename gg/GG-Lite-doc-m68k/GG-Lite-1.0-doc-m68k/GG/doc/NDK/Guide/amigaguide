        @{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87} @{"OpenAmigaGuideAsyncA" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 55} (@{"struct NewAmigaGuide" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 103} *,
