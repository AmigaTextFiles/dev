@NODE "GetAmigaGuideMsg" "amigaguide.library/GetAmigaGuideMsg()"
@{b}

   NAME@{ub}
        @{"GetAmigaGuideMsg" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 72} - Receive async AmigaGuide message.    (V34)@{b}

   SYNOPSIS@{ub}
        msg = @{"GetAmigaGuideMsg" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 72} (handle);
        d0                       a0

        @{"struct AmigaGuideMsg" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 89} *@{"GetAmigaGuideMsg" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 72} (@{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87});@{b}

   FUNCTION@{ub}
        This function returns a SIPC message from the AmigaGuide system,
        if there is a message available.@{b}

   INPUTS@{ub}
        handle - Handle to a AmigaGuide system.@{b}

   EXAMPLE@{ub}

        @{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87} handle;
        @{"struct AmigaGuideMsg" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 89} *agm;

        /* get a AmigaGuide message */
        while (agm = @{"GetAmigaGuideMsg" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 72}(handle))
        {
            /* process the event */
            switch (agm->agm_Type)
            {
                case @{"ToolCmdReplyID" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 60}: /* a command has completed */
                    if (agm->agm_Pri_Ret)
                    {
                        /* An error occurred, the reason is in agm_Sec_Ret.
                         * The command string is in agm_Data
                         */
                    }
                    break;

                case @{"ToolStatusID" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 58}: /* status message */
                    if (agm->agm_Pri_Ret)
                    {
                        /* an error occurred, the reason is in agm_Sec_Ret */
                    }
                    break;

                default:
                    break;
            }

            /* reply to the AmigaGuide message */
            @{"ReplyAmigaGuideMsg" LINK "ReplyAmigaGuideMsg"}(agm);
        }@{b}

   RETURNS@{ub}
        msg -- Pointer to a SIPC message or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if no message was
            available.@{b}

   SEE ALSO@{ub}
        @{"OpenAmigaGuideAsyncA()" LINK "OpenAmigaGuideAsyncA"}, @{"AmigaGuideSignal()" LINK "AmigaGuideSignal"}, @{"ReplyAmigaGuideMsg()" LINK "ReplyAmigaGuideMsg"}

@ENDNODE
