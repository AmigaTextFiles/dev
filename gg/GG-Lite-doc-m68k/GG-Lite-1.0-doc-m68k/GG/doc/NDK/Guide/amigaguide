        @{"BOOL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 91} @{"SendAmigaGuideContext" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 94} (@{"AMIGAGUIDECONTEXT" LINK "gg:doc/NDK/Guide/Include/libraries/amigaguide.h/File" 87}, Tag, ...);@{b}

   FUNCTION@{ub}
        This function is used to send a message to an AmigaGuide system to
        align it on the current context ID.

        This function effectively does a:

            @{"SendAmigaGuideCmd" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 103}(handle 'LINK ContextArray[contextID]', @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101});@{b}

   INPUTS@{ub}
        handle - Handle to an AmigaGuide system.
        future - Future expansion, must be set to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} for now.@{b}

   EXAMPLE@{ub}

        @{"struct IntuiMessage" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 763} *imsg;

        ...

        case @{"RAWKEY" LINK "gg:doc/NDK/Guide/Include/intuition/iobsolete.h/File" 119}:
            switch (imsg->Code)
            {
                case 95:
                    /* bring up help on a particular subject */
                    @{"SendAmigaGuideContext" LINK "gg:doc/NDK/Guide/Include/inline/amigaguide.h/File" 94}(handle, @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101});
                    break;
                ...
            }
            break;

        ...@{b}

   RETURNS@{ub}
        success -- Returns @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} if the message was sent, otherwise returns
            @{"FALSE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 98}.@{b}

   SEE ALSO@{ub}
        @{"SetAmigaGuideContextA()" LINK "SetAmigaGuideContextA"}, @{"SendAmigaGuideCmdA()" LINK "SendAmigaGuideCmdA"}

