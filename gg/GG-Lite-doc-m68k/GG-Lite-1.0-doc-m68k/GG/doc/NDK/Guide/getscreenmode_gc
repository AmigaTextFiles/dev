       @{"GM_HITTEST" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 298} -- Overrides superclass, returns @{"GMR_GADGETHIT" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 368} if within
                 the popscreenmode button.

       @{"GM_HELPTEST" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 314} -- Overrides superclass, returns @{"GMR_HELPHIT" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 371} if within
                 the domain of the gadget.

       @{"GM_GOACTIVE" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 305} -- Passed to superclass, passed to popfile/drawer button.

       @{"GM_HANDLEINPUT" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 308} -- Overrides superclass, all input processed.

       @{"GM_GOINACTIVE" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 311} -- Passed to superclass, popfile/drawer button deactivated.

       @{"GM_DOMAIN" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 323} -- Returns @{"GDOMAIN_MINIMUM" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 486} and @{"GDOMAIN_MAXIMUM" LINK "gg:doc/NDK/Guide/Include/intuition/gadgetclass.h/File" 492} dimensions.

       GM_KEYACTIVE -- Private method for keyboard control.

       GM_KEYINACTIVE -- Private method for keyboard control.

       @{"GSM_REQUEST" LINK "gg:doc/NDK/Guide/Include/gadgets/getscreenmode.h/File" 121} -- getscreenmode gadget method for invoking the screenmode requester.

