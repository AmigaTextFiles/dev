@NODE "ListBrowserSelectAll" "listbrowser_gc/ListBrowserSelectAll()"
@{b}

    NAME@{ub}
        @{"ListBrowserSelectAll" LINK "gg:doc/NDK/Guide/Include/inline/listbrowser.h/File" 61} -- Select all nodes in a multiselect ListBrowser.@{b}

    SYNOPSIS@{ub}
        @{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} @{"ListBrowserSelectAll" LINK "gg:doc/NDK/Guide/Include/inline/listbrowser.h/File" 61}(@{"struct List" LINK "gg:doc/NDK/Guide/Include/exec/lists.h/File" 20} *);
        @{"ListBrowserSelectAll" LINK "gg:doc/NDK/Guide/Include/inline/listbrowser.h/File" 61}(list);@{b}

    FUNCTION@{ub}
        Sets the @{"LBNA_Selected" LINK "gg:doc/NDK/Guide/Include/gadgets/listbrowser.h/File" 115} bit to @{"TRUE" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 95} in all nodes in list.  This is a
        quick way to select all nodes in a multi-select ListBrowser.

        Note that as with any modifications to a ListBrowser list, the list
        MUST be detached to the listbrowser at the time of modification.@{b}

    INPUTS@{ub}
        list - a list of ListBrowser nodes, not currently attached to a
            ListBrowser.

@ENDNODE
@NODE "HideAllListBrowserChildren" "listbrowser_gc/HideAllListBrowserChildren()"
