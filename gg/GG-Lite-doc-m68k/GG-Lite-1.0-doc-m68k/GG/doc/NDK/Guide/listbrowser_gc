    NAME@{ub}
        @{"HideAllListBrowserChildren" LINK "gg:doc/NDK/Guide/Include/inline/listbrowser.h/File" 77} -- Hide all children in a ListBrowser.@{b}

    SYNOPSIS@{ub}
        @{"VOID" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 24} @{"HideAllListBrowserChildren" LINK "gg:doc/NDK/Guide/Include/inline/listbrowser.h/File" 77}(@{"struct List" LINK "gg:doc/NDK/Guide/Include/exec/lists.h/File" 20} *);
