@NODE "CreateContext" "gadtools.library/CreateContext()"
@{b}

   NAME@{ub}
        @{"CreateContext" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 110} -- create a place for GadTools context data. (V36)@{b}

   SYNOPSIS@{ub}
        gad = @{"CreateContext" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 110}(glistpointer);
        D0                  A0

        @{"struct Gadget" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 214} *@{"CreateContext" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 110}(@{"struct Gadget" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 214} **);@{b}

   FUNCTION@{ub}
        Creates a place for GadTools to store any context data it might
        need for your window.  In reality, an unselectable invisible
        gadget is created, with room for the context data.
        This function also establishes the linkage from a glist type
        pointer to the individual gadget pointers.  Call this function
        before any of the other gadget creation calls.@{b}

   INPUTS@{ub}
        glistptr - address of a pointer to a Gadget, which was previously
                   set to @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}.  When all the gadget creation is done, you may
                   use that pointer as your NewWindow.FirstGadget, or
                   in @{"intuition.library/AddGList()" LINK "gg:doc/NDK/Guide/intuition/AddGList"},
                   @{"intuition.library/RefreshGList()" LINK "gg:doc/NDK/Guide/intuition/RefreshGList"}, @{"FreeGadgets()" LINK "FreeGadgets"}, etc.@{b}

   RESULT@{ub}
        gad - pointer to context gadget, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101} if failure.@{b}

   EXAMPLE@{ub}

        @{"struct Gadget" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 214} *gad;
        @{"struct Gadget" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 214} *glist = @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101};
        gad = @{"CreateContext" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 110}(&glist);
        /*  Other creation calls go here */
        if (gad)
        {
            myNewWindow.FirstGadget = glist;
            if ( myWindow = @{"OpenWindow" LINK "gg:doc/NDK/Guide/intuition/OpenWindow"}(&myNewWindow) )
            {
                @{"GT_RefreshWindow" LINK "GT_RefreshWindow"}(win,@{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101});
                /* other stuff */
                @{"CloseWindow" LINK "gg:doc/NDK/Guide/intuition/CloseWindow"}(myWindow);
            }
        }
        @{"FreeGadgets" LINK "FreeGadgets"}(glist);

@ENDNODE
@NODE "CreateGadgetA" "gadtools.library/CreateGadgetA()"
@{b}

   NAME@{ub}
        @{"CreateGadgetA" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 29} -- allocate and initialize a gadtools gadget. (V36)
        @{"CreateGadget" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 34} -- varargs stub for @{"CreateGadgetA" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 29}(). (V36)@{b}

   SYNOPSIS@{ub}
        gad = @{"CreateGadgetA" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 29}(kind, previous, newgad, tagList)
        D0                  D0    A0        A1      A2

        @{"struct Gadget" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 214} *@{"CreateGadgetA" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 29}(@{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52}, @{"struct Gadget" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 214} *,
                                     @{"struct NewGadget" LINK "gg:doc/NDK/Guide/Include/libraries/gadtools.h/File" 83} *, @{"struct TagItem" LINK "gg:doc/NDK/Guide/Include/utility/tagitem.h/File" 32} *);

        gad = @{"CreateGadget" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 34}(kind, previous, newgad, firsttag, ...)

        @{"struct Gadget" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 214} *@{"CreateGadget" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 34}(@{"ULONG" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 52}, @{"struct Gadget" LINK "gg:doc/NDK/Guide/Include/intuition/intuition.h/File" 214} *,
                                    @{"struct NewGadget" LINK "gg:doc/NDK/Guide/Include/libraries/gadtools.h/File" 83} *, Tag, ...);@{b}

   FUNCTION@{ub}
        @{"CreateGadgetA" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 29}() allocates and initializes a new gadget of the
        specified kind, and attaches it to the previous gadget.  The
        gadget is created based on the supplied kind, NewGadget structure,
        and tags.@{b}

   INPUTS@{ub}
        kind - kind of gadget is to be created, one of the XXX_KIND values
               defined in <@{"libraries/gadtools.h" LINK "gg:doc/NDK/Guide/Include/libraries/gadtools.h/File"}>
        previous - pointer to the previous gadget that this new gadget
                   is to be attached to. This function will fail if this value
                   is @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}
        newgad - a filled in NewGadget structure describing the desired
                 gadget's size, position, label, etc.
        tagList - pointer to an array of tags providing optional extra
                  parameters, or @{"NULL" LINK "gg:doc/NDK/Guide/Include/exec/types.h/File" 101}@{b}

   TAGS@{ub}
        All kinds:
        @{"GT_Underscore" LINK "gg:doc/NDK/Guide/Include/libraries/gadtools.h/File" 331} - Indicates the symbol that precedes the character in
            the gadget label to be underscored.  This can be to indicate
            keyboard equivalents for gadgets (note that GadTools does not
            process the keys - it just displays the underscore).  For example,
            to underscore the "M" in "Mode":
                ng.ng_GadgetText = "_Mode:";
                gad = @{"CreateGadget" LINK "gg:doc/NDK/Guide/Include/inline/gadtools.h/File" 34}(..._KIND, &ng, prev,
                        @{"GT_Underscore" LINK "gg:doc/NDK/Guide/Include/libraries/gadtools.h/File" 331}, '_',
                        ...
                        );
            (V37)

