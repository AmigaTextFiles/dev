;see docs/CACHE.DOC
setenv M2OBJCACHE ram:m2cache
IF NOT EXISTS $M2OBJCACHE
  makedir $M2OBJCACHE
ENDIF
resident modula:bin/m2l pure
resident dcc:bin/dcc pure
resident dcc:bin/dlink pure
IF NOT EXISTS ram:dlib
  makedir ram:dlib
ENDIF
IF NOT EXISTS ram:mlib
  makedir ram:mlib
ENDIF
copy mlib:#? ram:mlib
copy dlib:#? ram:dlib
assign mlib: ram:mlib
assign dlib: ram:dlib

