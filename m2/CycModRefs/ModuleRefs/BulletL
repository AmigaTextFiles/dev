DEFINITION MODULE BulletL {"bullet.library",33};

(*$ Implementation- *)


FROM SYSTEM IMPORT ADDRESS;
IMPORT ed:ExecD,Reg,df:DiskfontD;


VAR
 BulletBase : ed.LibraryPtr;


CONST

 bulletMinVersion=33;
 bulletName="bullet.library";


PROCEDURE OpenEngine():df.GlyphEnginePtr; CODE -30;

PROCEDURE CloseEngine(glyphEngine{Reg.A0}:df.GlyphEnginePtr); CODE -36;

PROCEDURE SetInfoA(glyphEngine{Reg.A0}:df.GlyphEnginePtr;tagList{Reg.A1}:ADDRESS):LONGCARD; CODE -42;

PROCEDURE ObtainInfoA(glyphEngine{Reg.A0}:df.GlyphEnginePtr;tagList{Reg.A1}:ADDRESS):LONGCARD; CODE -48;

PROCEDURE ReleaseInfoA(glyphEngine{Reg.A0}:df.GlyphEnginePtr;tagList{Reg.A1}:ADDRESS):LONGCARD; CODE -54;


END BulletL.

@endnode
