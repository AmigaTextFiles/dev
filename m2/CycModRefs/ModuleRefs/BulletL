@database BulletL
@node Main "BulletL"

Procedures

   @{" CloseEngine " link Modul 36}                 @{" ObtainInfoA " link Modul 40}                 @{" OpenEngine " link Modul 34}
   @{" ReleaseInfoA " link Modul 42}                @{" SetInfoA " link Modul 38}                 

Constants

   @{" bulletMinVersion " link Modul 28}            @{" bulletName " link Modul 31}               

Variables

   @{" BulletBase " link Modul 24}               

@endnode
@node Modul "am2:ModuleRefs/BulletL.def"
