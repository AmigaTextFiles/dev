   @{" diskfontName " link Modul 28}                @{" dotSize " link Modul 153}                     @{" eBullet " link Modul 298}
   @{" emboldenX " link Modul 194}                   @{" emboldenY " link Modul 195}                   @{" engine " link Modul 297}
   @{" errBadData " link Modul 446}                  @{" errBadFace " link Modul 449}                  @{" errBadGlyph " link Modul 451}
   @{" errBadTag " link Modul 444}                   @{" errFailure " link Modul 436}                  @{" errNoFace " link Modul 448}
   @{" errNoGlyph " link Modul 450}                  @{" errNoMemory " link Modul 447}                 @{" errNoRotate " link Modul 453}
   @{" errNoShear " link Modul 452}                  @{" errSuccess " link Modul 443}                  @{" errTooSmall " link Modul 454}
   @{" errUnknownGlyph " link Modul 455}             @{" errUnknownTag " link Modul 445}               @{" family " link Modul 301}
   @{" fchId " link Modul 52}                       @{" fileIdent " link Modul 294}                   @{" glyphCode " link Modul 204}
   @{" glyphCode2 " link Modul 209}                  @{" glyphMap " link Modul 232}                    @{" glyphWidth " link Modul 216}
   @{" hCompressed " link Modul 361}                 @{" hCondensed " link Modul 362}                  @{" hExpanded " link Modul 365}
   @{" hExtraCompressed " link Modul 360}            @{" hExtraExpanded " link Modul 366}              @{" hNormal " link Modul 363}
   @{" hSemiExpanded " link Modul 364}               @{" hUltraCompressed " link Modul 359}            @{" horizStyle " link Modul 358}
   @{" iName " link Modul 307}                       @{" indirect " link Modul 131}                    @{" inhibitAlgoStyle " link Modul 377}
   @{" isFixed " link Modul 326}                     @{" level0 " link Modul 123}                      @{" level1 " link Modul 125}
   @{" level2 " link Modul 127}                      @{" level3 " link Modul 129}                      @{" maxAvailSizes " link Modul 381}
   @{" maxFontName " link Modul 69}                 @{" maxFontPath " link Modul 26}                 @{" oTagList " link Modul 228}
   @{" oTagPath " link Modul 227}                    @{" ofchID " link Modul 55}                      @{" otulBroken " link Modul 273}
   @{" otulDoubleSolid " link Modul 274}             @{" otulNone " link Modul 271}                    @{" otulSolid " link Modul 272}
