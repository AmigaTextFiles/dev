   @{" sSemiBold " link Modul 343}                   @{" sSemiLight " link Modul 340}                  @{" sThin " link Modul 336}
   @{" sUltraBlack " link Modul 349}                 @{" sUltraThin " link Modul 334}                  @{" sUpright " link Modul 353}
   @{" serifFlag " link Modul 329}                   @{" setFactor " link Modul 167}                   @{" shearCos " link Modul 177}
