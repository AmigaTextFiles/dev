DEFINITION MODULE AmigaGuideL {"amigaguide.library",33};

(*$ Implementation- *)

FROM SYSTEM IMPORT ADDRESS;
IMPORT Reg,ag:AmigaGuideD,ud:UtilityD,DosD;


VAR
 AmigaGuideBase : ag.AmigaGuideBasePtr;


CONST

 amigaguideMinVersion=33;
 amigaguideName="amigaguide.library";


PROCEDURE LockAmigaGuideBase(handle{Reg.A0}:ag.AGContext):LONGINT; CODE -36;

PROCEDURE UnlockAmigaGuideBase(key{Reg.D0}:LONGINT); CODE -42;

PROCEDURE OpenAmigaGuideA(nag{Reg.A0}:ag.NewAmigaGuidePtr;attrs{Reg.A1}:ADDRESS):ag.AGContext; CODE -54;

PROCEDURE OpenAmigaGuideAsyncA(nag{Reg.A0}:ag.AGContext; attrs{Reg.D0}:ADDRESS):LONGINT; CODE -60;

PROCEDURE CloseAmigaGuide(cl{Reg.A0}:ag.AGContext):LONGINT; CODE -66;

PROCEDURE AmigaGuideSignal(cl{Reg.A0}:ag.AGContext):LONGINT; CODE -72;

PROCEDURE GetAmigaGuideMsg(cl{Reg.A0}:ag.AGContext):LONGINT; CODE -78;

PROCEDURE ReplyAmigaGuideMsg(amsg{Reg.A0}:ag.AmigaGuideMsgPtr):LONGINT; CODE -84;

PROCEDURE SetAmigaGuideContextA(cl{Reg.A0}:ag.AGContext;id{Reg.D0}:LONGCARD;attrs{Reg.D1}:ADDRESS):BOOLEAN; CODE -90;

PROCEDURE SendAmigaGuideContextA(cl{Reg.A0}:ag.AGContext;attrs{Reg.D0}:ADDRESS):BOOLEAN; CODE -96;

PROCEDURE SendAmigaGuideCmdA(cl{Reg.A0}:ag.AGContext;cmd{Reg.D0},attrs{Reg.D1}:ADDRESS):BOOLEAN; CODE -102;

PROCEDURE SetAmigaGuideAttrsA(cl{Reg.A0}:ag.AGContext;attrs{Reg.A1}:ADDRESS):LONGINT; CODE -108;

PROCEDURE GetAmigaGuideAttr(tag{Reg.D0}:ud.Tag;cl{Reg.A0}:ag.AGContext;storage{Reg.A1}:ADDRESS); CODE -114;

PROCEDURE LoadXRef(lock{Reg.A0}:DosD.FileLockPtr;name{Reg.A1}:ADDRESS):LONGINT; CODE -126;

PROCEDURE ExpungeXRef():LONGINT; CODE -132;

PROCEDURE AddAmigaGuideHostA(h{Reg.A0}:ud.HookPtr;name{Reg.D0}:ADDRESS;attrs{Reg.A1}:ADDRESS):ag.AmigaGuideHostPtr; CODE -138;

PROCEDURE RemoveAmigaGuideHostA(hh{Reg.A0}:ag.AmigaGuideHostPtr;attrs{Reg.A1}:ADDRESS):LONGINT; CODE -144;

PROCEDURE GetAmigaGuideString(id{Reg.D0}:LONGCARD):ADDRESS; CODE -210;


END AmigaGuideL.

@endnode
