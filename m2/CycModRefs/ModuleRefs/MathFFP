PROCEDURE Fix(x{0}:REAL):LONGINT; CODE -30;
PROCEDURE Flt(x{0}:LONGINT):REAL; CODE -36;
PROCEDURE Cmp(x{1},y{0}:REAL):LONGINT; CODE -42;
PROCEDURE Tst(x{1}:REAL):LONGINT; CODE -48;
PROCEDURE Abs(x{0}:REAL):REAL; CODE -54;
PROCEDURE Neg(x{0}:REAL):REAL; CODE -60;
PROCEDURE Add(x{0},y{1}:REAL):REAL; CODE -66;
PROCEDURE Sub(x{0},y{1}:REAL):REAL; CODE -72;
PROCEDURE Mul(x{0},y{1}:REAL):REAL; CODE -78;
PROCEDURE Div(x{0},y{1}:REAL):REAL; CODE -84;
PROCEDURE Floor(x{0}:REAL):REAL; CODE -90;
PROCEDURE Ceil(x{0}:REAL):REAL; CODE -96;

END MathFFP.

