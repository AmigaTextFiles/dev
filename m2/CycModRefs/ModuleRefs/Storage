FROM SYSTEM IMPORT ADDRESS;

PROCEDURE ALLOCATE(VAR Adr:ADDRESS; size:LONGINT);
PROCEDURE DEALLOCATE(VAR Adr:ADDRESS; size:LONGINT);

END Storage.

