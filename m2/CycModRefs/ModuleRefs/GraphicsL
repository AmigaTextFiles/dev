(*------ BitMap primitives ------*)

PROCEDURE BltBitMap(
           srcBitMap{Reg.A0}:GraphicsD.BitMapPtr;
           srcX{Reg.D0}:INTEGER;
           srcY{Reg.D1}:INTEGER;
           dstBitMap{Reg.A1}:GraphicsD.BitMapPtr;
           dstX{Reg.D2}:INTEGER;
           dstY{Reg.D3}:INTEGER;
           sizeX{Reg.D4}:INTEGER;
