   @{" ack " link Modul 8}                         @{" bel " link Modul 8}                         @{" bs " link Modul 9}
   @{" can " link Modul 13}                         @{" cr " link Modul 10}                          @{" csi " link Modul 15}
   @{" dc1 " link Modul 11}                         @{" dc2 " link Modul 11}                         @{" dc3 " link Modul 11}
   @{" dc4 " link Modul 12}                         @{" del " link Modul 15}                         @{" dle " link Modul 11}
   @{" em " link Modul 13}                          @{" enq " link Modul 8}                         @{" eof " link Modul 16}
   @{" eol " link Modul 17}                         @{" eot " link Modul 8}                         @{" esc " link Modul 13}
