LEX= flex
CFLAGS= DEF __GNU_LIBRARY__ OPT PARM r
OBJS= m2latex.o version.o getopt1.o getopt.o

m2latex: $(OBJS)
	sc link $(OBJS) LIB:fl.lib ICON

p2latex:
	makelink $@ m2latex
	makelink $@.info m2latex.info

version.o: version.c

m2latex.o: m2latex.c main.c getopt.h

getopt1.o: getopt1.c getopt.h

getopt.o: getopt.c

#.l.c:
#	$(LEX) -o$@ $<


demo: demo.mi
	m2latex -h -F it $<
	latex $<.tex

clean:
	-delete $(OBJS) m2latex.lnk p2latex demo.mi.(tex|log|aux)

archive: clean
	lha -r u RAM:m2latex // m2latex m2latex.info
