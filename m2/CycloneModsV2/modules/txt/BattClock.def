DEFINITION MODULE BattClock;

FROM SYSTEM IMPORT ADDRESS;

IMPORT Reg;

CONST
 battclockName="battclock.resource";

PROCEDURE ResetBattClock(base{Reg.A6}:ADDRESS); CODE -6;
PROCEDURE ReadBattClock(base{Reg.A6}:ADDRESS):LONGCARD; CODE -12;
PROCEDURE WriteBattClock(base{Reg.A6}:ADDRESS;time{Reg.D0}:LONGCARD); CODE -18;


END BattClock.
