PROCEDURE Sin(x{0}:FFP):FFP; CODE -36;

PROCEDURE Sincos(x{0}:FFP; VAR cos{1}:FFP):FFP; CODE -54;

PROCEDURE Sinh(x{0}:FFP):FFP; CODE -60;

PROCEDURE Sqrt(x{0}:FFP):FFP; CODE -96;

PROCEDURE Tan(x{0}:FFP):FFP; CODE -48;

