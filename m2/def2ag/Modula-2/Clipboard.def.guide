TYPE
 @{" SatisfyMsg " link HYPER:Modula-2/Clipboard.def.guide/Modul 40}=RECORD
  msg:@{" Message " link HYPER:Modula-2/ExecD.def.guide/Modul 376};
  unit:CARDINAL;
  clipID:LONGINT;
 END;
 @{" SatisfyMsgPtr " link HYPER:Modula-2/Clipboard.def.guide/Modul 46}=POINTER TO @{" SatisfyMsg " link HYPER:Modula-2/Clipboard.def.guide/Modul 40};

 (*36*)@{" ClipHookMsg " link HYPER:Modula-2/Clipboard.def.guide/Modul 48}=RECORD
        type:LONGCARD;
        changeMd:LONGINT;
        clipID:LONGINT;
       END;
 (*36*)@{" ClipHookMsgPtr " link HYPER:Modula-2/Clipboard.def.guide/Modul 53}=POINTER TO @{" ClipHookMsg " link HYPER:Modula-2/Clipboard.def.guide/Modul 48};

END Clipboard.noimp

