 clipboardName="clipboard.device";
 post=nonstd+0;
 currentReadId=nonstd+1;
 currentWriteId=nonstd+2;
 (*36*)changeHook=nonstd+3;
 obsoleteId=1;

TYPE
 @{" ClipboardUnitPartial " link HYPER:Modula-2/Clipboard.def.guide/Modul 14}=RECORD
