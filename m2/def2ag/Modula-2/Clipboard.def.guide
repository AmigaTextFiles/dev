FROM ExecD IMPORT nonstd,@{" Node " link HYPER:Modula-2/ExecD.def.guide/Modul 18},@{" Message " link HYPER:Modula-2/ExecD.def.guide/Modul 376},@{" DevicePtr " link HYPER:Modula-2/ExecD.def.guide/Modul 420},@{" IOFlagSet " link HYPER:Modula-2/ExecD.def.guide/Modul 431},@{" UnitPtr " link HYPER:Modula-2/ExecD.def.guide/Modul 429};

