# makefile for GNU make (automatically generated by makemake)
# NOTE: indent with TAB in GNU makefiles!

# paths are relative to the project directory (current directory during make)

EXE    = bin/test
OBJDIR = o/

# compiler and linker

CC = gcc
LD = gcc

INCLUDES=-I Work:Development/Tutorials/libmkv-0.6.5.1/test/
gccprefs=-I Work:Development/Tutorials/libmkv-0.6.5.1/test/ -I- 




# target 'all' (default target)

all : $(EXE)

$(OBJDIR)test.o : test.c
	$(CC) $(gccprefs) -c -o $(OBJDIR)test.o test.c $(INCLUDES)

OBJS = $(OBJDIR)test.o

$(EXE) : $(OBJS)
	$(LD) $(OBJS) -noixemul -lmkv -LWork:Development/Tutorials/libmkv-0.6.5.1/test/ $(gccprefs) -o $(EXE)

# target 'strip'

strip:
	strip --remove-section=.comment $(EXE)

# target 'clean'

clean:
	-delete $(EXE)
	-delete $(OBJDIR)\*.o
