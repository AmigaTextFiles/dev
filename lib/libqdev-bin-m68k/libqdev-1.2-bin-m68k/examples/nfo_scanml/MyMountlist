/*
 * This entry should be detected as a filesystem even
 * though it lacks all necessary information, such as
 * Device, Unit, Flags, DosType, MaxTransfer, Mask.
 * Additionally two keys address incorrect values!
*/
MYDEV99:
Priority 	= xxx
Surfaces 	= 16
BlockSize 	= 512
BlocksPerTrack 	= 255
Reserved 	= 2
Interleave 	= 0
LowCyl 		= 1
HighCyl 	= xxx
StackSize 	= 4096
Buffers 	= 80
BufMemType 	= 0
GlobVec 	= -1
Activate 	= 1
#

/*
 * This should be detected as a simple handler.
*/
MYHAN99:
Handler 	= L:myown-handler
StackSize 	= 4000
Priority 	= 10
GlobVec 	= -1
#

