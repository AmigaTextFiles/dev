.KEY INFILE/A,OUTFILE/A,OPTIONS/F
.BRA «
.KET »

; research
; by megacz
;
;    t_dictmerge d1ct_all.txt
;    t_research d1ct_all.txt hashlab.dump ;ne .255 tc 2
;

FAILAT 5
SET hl_batch    "t:___hashlab.batch"
SET hl_maxline  258
IF not exists "«OUTFILE»"
  ECHO " /// doing research on all your hashing routines..."
  ECHO >«OUTFILE» ""
  list >${hl_batch} #?.hl lformat "hashlab >>«OUTFILE» «INFILE» %s no bl ${hl_maxline} «OPTIONS»*necho >>«OUTFILE» *"*""
  execute ${hl_batch}
ENDIF
t_showtable «OUTFILE»
UNSETENV ${hl_batch}
UNSET hl_batch
UNSET hl_maxline
