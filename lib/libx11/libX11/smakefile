LIBOBJS=  X11events.o X11windows.o text.o  X11display.o xemul2.o  X11colormaps.o drawing.o clipping.o  Xtoolkit.o XmDrawing.o fonts.o images.o cursors.o filling.o xemul3.o xemul4.o timing.o resources.o resource_list.o X11c2p.o X11p2c.o context.o visuals.o regions.o Xmui.o lists.o alloca.o tcp.o class.o gc.o unix.o xtmui.o

# xtoolkit2.o pixmaps.o

RM= delete quiet

libX11.lib: $(LIBOBJS)
	-$(RM) libX11.lib
	oml libX11.lib r $(LIBOBJS)

000: $(LIBOBJS)
	-$(RM) libX11_000std.lib
	oml libX11_000std.lib r $(LIBOBJS)

alloca.o: alloca.c
	sc PARAMS=STACK alloca.c
