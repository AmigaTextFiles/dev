echo "Joining .c files"
join #?.c #?.doc to t:ad.temp
echo "Creating ram:extras.doc"
autodoc -C -I t:ad.temp >ram:extras.doc
cd ram:
echo "Converting to Guide"
ad2ag ram:extras.doc xreffile s:autodoc.xref to ram:
copy ram:extras.doc(|.info)   sc:programs/extraslib/docs/
copy ram:extras(|.info)       sc:programs/extraslib/docs/
copy ram:extras(|.info)       sc:doc/
echo Done
