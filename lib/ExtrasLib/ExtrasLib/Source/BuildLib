makedir >nil: libobjects
makedir >nil: objects

echo >t:newline ""
echo >libcode.h "#define NOTLIBCODE"
scopts OBJECTLIB="lib:extras.lib" PARM=B NOLIBCODE OBJECTNAME="objects/"
smake <t:newline -w

echo >libcode.h "#define LIBCODE"
scopts OBJECTLIB="lib:extras_libcode.lib" PARM=R BATCH LIBCODE OBJECTNAME="libobjects/"
smake <t:newline -w

echo >libcode.h "#define NOTLIBCODE"
scopts OBJECTLIB="lib:extras.lib" PARM=B BATCH NOLIBCODE  OBJECTNAME="objects/"

