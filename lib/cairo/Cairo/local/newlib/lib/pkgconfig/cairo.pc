prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: cairo
Description: Multi-platform 2D graphics library
Version: 0.9.2

Requires.private: fontconfig   
Libs: -L/usr/local/lib -lfreetype -lz -L${libdir} -lcairo 
Cflags: -I/usr/local/include/freetype2 -I/usr/local/include -I${includedir}/cairo
