
SUBMAKEFILE = 1

include ../Makefile.aros

CFLAGS += -I../src

SOURCES = \
	cairo-boilerplate.c \
	cairo-boilerplate-ps.c \
	cairo-boilerplate-svg.c \
	cairo-boilerplate-pdf.c \
	cairo-boilerplate-aros.c \
	xmalloc.c \
	$(NULL)

#	cairo-boilerplate-win32.c \

OBJECTS = $(subst .c,.o,$(SOURCES))

all: libboiler.a

libboiler.a: $(OBJECTS)
	$(AR) rs $@ $(OBJECTS)

clean:
	rm -f *.o *.a
