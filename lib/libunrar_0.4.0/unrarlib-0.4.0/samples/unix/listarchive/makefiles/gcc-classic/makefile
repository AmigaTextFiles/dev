# makefile for GNU make (automatically generated by makemake)
# NOTE: indent with TAB in GNU makefiles!

# paths are relative to the project directory (current directory during make)

EXE    = bin/gcc-classic/class3
OBJDIR = o/gcc-classic/

# compiler and linker

CC = gcc
LD = gcc

# target 'all' (default target)

all : $(EXE)

$(OBJDIR)Class3.o : Class3.c
	$(CC) $(shell gccprefs) -c -o $(OBJDIR)Class3.o Class3.c

OBJS = $(OBJDIR)Class3.o

$(EXE) : $(OBJS)
	$(LD) $(OBJS) $(shell gccprefs) -o $(EXE)

# target 'strip'

strip:
	strip --remove-section=.comment $(EXE)

# target 'clean'

clean:
	-delete $(EXE)
	-delete $(OBJDIR)\*.o
