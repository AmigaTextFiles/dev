# makefile for GNU make (automatically generated by makemake)
# NOTE: indent with TAB in GNU makefiles!

# paths are relative to the project directory (current directory during make)

EXE    = bin/gcc-morphos-stable/getfile
OBJDIR = o/gcc-morphos-stable/

# compiler and linker

CC = gcc
LD = gcc

INCLUDES=-I Work:Development/Tutorials/unrarlib-0.4.0/samples/unix/getfile/includes
gccprefs=-I Work:Development/Tutorials/unrarlib-0.4.0/samples/unix/getfile -I- 




# target 'all' (default target)

all : $(EXE)

$(OBJDIR)getfile.o : getfile.c
	$(CC) $(gccprefs) -c -o $(OBJDIR)getfile.o getfile.c $(INCLUDES)

OBJS = $(OBJDIR)getfile.o

$(EXE) : $(OBJS)
	$(LD) $(OBJS) -noixemul -lunrar -LWork:Development/Tutorials/unrarlib-0.4.0/samples/unix/getfile/includes/lib $(gccprefs) -o $(EXE)

# target 'strip'

strip:
	strip --remove-section=.comment $(EXE)

# target 'clean'

clean:
	-delete $(EXE)
	-delete $(OBJDIR)\*.o
