	:kd=\EOB:ke=\E[?1l:kl=\EOD:kr=\EOC:ks=\E[?1h:ku=\EOA:\
	:le=\010:ll=\E[24E:mb=\E[5m:md=\E[1m:me=\E[m\017\E["q:\
	:mh=\E[2m:mk=\E[8m:mp=\E[1"q:mr=\E[7m:nd=\E[C:nw=\EE:\
	:pf=\E[4i:po=\E[5i:ps=\E[?19h:\
	:r2=\E[61"p\E[40h\E[?6l\E[1r\E[2;3;4;13;20;34;39;36l\E[12;16;34h\E[?1;3;4;5;10;18l\E[?7;8;25h\E>\E[?5W\E(B\017\E[24E\E[4i:\
	:r3=\E[?5l:rc=\E8:\
	:..sa=\E[0%?%p6%t;1%;%?%p2%t;4%;%?%p1%p3%O%t;7%;%?%p4%t;5%;%?%p5%t;2%;%?%p7%t;8%;m\E[%?%p8%t1%;"q%?%p9%t\016%e\017%;:\
	:sc=\E7:se=\E[27m:sf=\n:so=\E[7m:sr=\EM:st=\EH:ta=^I:\
	:ue=\E[24m:up=\EM:us=\E[4m:vb=\E[?5h\E[?5l:\
