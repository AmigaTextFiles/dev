	:ch=\E[%i%d`:cl=\E[H\E[J:cm=\E[%i%d;%dH:cr=^M:\
	:cs=\E[%i%d;%dr:ct=\E[3g:cv=\E[%i%dd:dc=\E[P:dl=\E[M:\
	:do=^J:ds=\E[0$~:eA=\E)0:ec=\E[%dX:ei=\E[4l:fs=\E[0$}:\
	:ho=\E[H:i1=\E[?5W:i2=\E>\E(B\E)0\017\E[m:im=\E[4h:ip=:\
	:is=\E[2;4;20;30l\E[?1;4;10;16l\E[12h\E[?7;8;25;67h:\
	:k0=\E[21~:k1=\E[11~:k2=\E[12~:k3=\E[13~:k4=\E[14~:\
