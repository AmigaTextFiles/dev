	:F9=\E[33~:FA=\E[34~:IC=\E[%d@:K1=\EOw:K2=\EOy:K3=\EOu:\
	:K4=\EOq:K5=\EOs:LE=\E[%dD:RA=\E[?7l:RI=\E[%dC:SA=\E[?7h:\
	:UP=\E[%dA:\
	:ac=``aaffggjjkkllmmnnooppqqrrssttuuvvwwxxyyzz{{||}}~~:\
	:ae=^O:al=\E[L:as=^N:bl=^G:bt=\E[Z:cb=\E[1K:cd=\E[J:ce=\E[K:\
	:ch=\E[%i%d`:cl=\E[H\E[J:cm=\E[%i%d;%dH:cr=^M:\
	:cs=\E[%i%d;%dr:ct=\E[3g:cv=\E[%i%dd:dc=\E[P:dl=\E[M:\
	:do=^J:ds=\E[0$~:eA=\E)0:ec=\E[%dX:ei=\E[4l:fs=\E[0$}:\
	:ho=\E[H:i1=\E[?5W:i2=\E>\E(B\E)0\017\E[m:im=\E[4h:ip=:\
	:is=\E[2;4;20;30l\E[?1;4;10;16l\E[12h\E[?7;8;25;67h:\
	:k1=\EOP:k2=\EOQ:k3=\EOR:k4=\EOS:k6=\E[17~:k7=\E[18~:\
