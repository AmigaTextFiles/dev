If NumPars<>1 Then End

l$=Par$(1)

If ReadFile(0,l$)
  FileSeek 0,0
  Fields 0,n.b
  Get 0,37
  CloseFile 0
  Select -1
  Case n<10
    n$="000"+Str$(n)
  Case (n<100 AND n>9)
    n$="00"+Str$(n)
  Case (n<1000 AND n>99)
    n$="0"+Str$(n)
  Case (n<10000 AND n>999)
    n$=Str$(n)
  End Select
  Print n$
Else
  Print "Lib not found!!!"
EndIf

End


