NoCli
quit=False
path$="DH0:Progs/Blitz2/HotShots/Shapes/shape."
shp=0 : fil$="000"

AutoCookie On
Repeat
  shp+1
  fil$=Right$("000"+Str$(shp),3)
  d$=path$+fil$
  If Exists(d$) Then LoadShape shp,d$ Else quit=True
  If Exists(d$) Then MidHandle shp
Until quit=True
SaveShapes 1,shp-1,"DH0:Progs/Blitz2/HotShots/HotShotsData/objects"
End

