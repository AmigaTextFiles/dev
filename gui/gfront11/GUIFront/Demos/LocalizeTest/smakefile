#.SILENT:

# This file makes the localized FramePrefs demo
#
all: demoprogram

# Files which go into the preferences program
#
OBJECTS = obj/LocalizeTest.o obj/frameprefs.o

# Catalogs
#
CATOBJ = catalogs/dansk/LocalizeTest.catalog

# Build demo program executable
#
demoprogram: catalogs LocalizeTest

LocalizeTest: $(OBJECTS)
	slink with <<
FROM LIB:c.o $(OBJECTS)
TO "LocalizeTest"                
LIB LIB:sc.lib LIB:amiga.lib
SMALLCODE                   
SMALLDATA                   
NOICONS                     
DEFINE @__chkabort=@__dummy 
DEFINE __CXM33=__UCXM33     
DEFINE __CXD33=__UCXD33     
DEFINE __CXM22=__UCXM22     
DEFINE __CXD22=__UCXD22     
MAP ggprefs.map,h,s
<

# Compile preferences code
#
.c.o:
   sc $* objname=obj/

# Module dependancies
#
obj/localizetest.o	 : localizetest.c strings.h
obj/frameprefs.o	 : frameprefs.c strings.h

# Maintain catalog strings
#
strings.h: catalogs/LocalizeTest.cd
	CatComp catalogs/LocalizeTest.cd CFILE strings.h CTFILE catalogs/LocalizeTest.ct

catalogs: $(CATOBJ)

catalogs/dansk/LocalizeTest.catalog: catalogs/dansk/dansk.ct
	CatComp catalogs/LocalizeTest.cd catalogs/dansk/dansk.ct CATALOG catalogs/dansk/LocalizeTest.catalog
