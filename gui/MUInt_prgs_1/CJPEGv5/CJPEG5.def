stack 8000
;     SCRIPT FILENAME: CPEG5.def
;     DATE: 12-28-95



echo  >ram:CJbuttons "                      Convert SOURCE -->TO JPEG                         ;execute C.convertLR.scp"
echo >>ram:CJbuttons "    DELETE LEFT    ;execute C.deleft.scp"
echo >>ram:CJbuttons "     VIEW LEFT     ;execute C.viewerL.scp"
echo >>ram:CJbuttons "     VIEW RIGHT    ;execute C.viewerR.scp"
echo >>ram:CJbuttons "    DELETE RIGHT   ;execute C.delright.scp"
echo >>ram:CJbuttons "   COPY Src->Dest  ;execute C.copyL2R.scp"
echo >>ram:CJbuttons "       HELP        ;Advice *"CJPEG Help*" C.help.txt"
echo >>ram:CJbuttons "   COPY Dest->Src  ;execute C.copyR2L.scp"
echo >>ram:CJbuttons "       QUIT        "

echo >>ram:CJcheckbox "SMOOTH 50;-smooth 50"
echo >>ram:CJcheckbox "DEBUG    ;-debug"

echo  >ram:CJradio "OUTFILE;COLOR|GRAY;-gr"
echo >>ram:CJradio "INFILE;GIF;|TARGA;-targa|BMP;|PBM PLUS;"
echo >>ram:CJradio "DCT METHOD;Integer;|Fast Integer;-fast|Floating Pt;-float"
echo >>ram:CJradio "QUALITY;75   ;|90;-quality 90|50;-quality 50|20;-quality 20"


;SET source "work:graphics/pictures/GIF"

;========= Put your own directories here ========
;=========  This is the conversion path  ========

SET source "ram:"
SET dest "ram:"


echo  >ram:CJ.cl "BEGIN"
echo >>ram:CJ.cl "TITLE *"CJPEGv5 Converts pictures to JPEGs*"" 
echo >>ram:CJ.cl "LEFT SOURCE;$source"
echo >>ram:CJ.cl "RIGHT DESTINATION;$dest"
echo >>ram:CJ.cl "BODY C.jpeg.man"
echo >>ram:CJ.cl "BUTTONS ram:CJbuttons"
echo >>ram:CJ.cl "RADIO ram:CJradio"
echo >>ram:CJ.cl "CHECKBOX ram:CJcheckbox"
echo >>ram:CJ.cl "WIDTH 80"
echo >>ram:CJ.cl "NAME CJPEG5"
echo >>ram:CJ.cl "END"



MUInt ram:CJ.cl

DELETE ram:CJ#? quiet

quit


