; SCRIPT FILENAME: COPYL2R.SCP 


IF EXISTS >nil: env:left
  IF EXISTS >nil: env:rightpath

    rtEZrequest "MUInt Advice:" BODY "Do you want to COPY:\n$left\nTO\n$rightpath" BUTTONTEXT "Copy|Cancel" NORETURNKEY POS CENTERSCR CENTERTEXT
    SET result $RC

    IF $Result EQ 1
      COPY $left TO $rightpath
    ENDIF

    QUIT 0

  ENDIF
ENDIF

rtEZrequest "MUInt Advice:" BODY "ERROR: No File/Directory Selected" BUTTONTEXT "_Continue"  POS CENTERSCR

quit 