stack 8000
;     SCRIPT FILENAME: RADIO.def
;     DATE: 12-30-94 


;=================   WRITE THE RADIO FILE TO RAM: ====================
;======= FORMAT: <FILENAME> "<TITLETEXT>;<LABELTEXT>;<FLAG>" =========

echo  >ram:DEMOradio "Computer ;1200   ;-5|4000  ;-4|2200  ;-22|2000  ;-2"
echo >>ram:DEMOradio "Lifestyle;Laidback;-l|Hyper;-h|Normal;-n"
echo >>ram:DEMOradio "Interests;Graphics;-g|Video;-v|COMAL;-c|DOS;-d "


;================   WRITE THE BUTTON FILE TO RAM: ====================

echo  >ram:DEMObuttons "HELP;advice *"This is a DEMO*" GUIDE/demo.txt" POS CENTERSCR
echo >>ram:DEMObuttons "QUIT"

;================   WRITE THE DEFINITION FILE TO RAM: ====================

echo   >ram:DEMO.cl "BEGIN"
echo  >>ram:DEMO.cl "TITLE *"The Radio Gadget-  by BOTH Software (12-20)*"" 
echo  >>ram:DEMO.cl "RADIO ram:DEMOradio"
echo  >>ram:DEMO.cl "BUTTONS ram:DEMObuttons"
echo  >>ram:DEMO.cl "END"


RUN >NIL: MUInt ram:DEMO.cl 

DELETE ram:DEMO#? quiet

QUIT


