STACK 8000


;     SCRIPT FILENAME: MAN.def            
;     DATE           : 02-14-95 
;     AUTHOR         : Colin Thompson
;     DESCRIPTION    : A quick reference guide for MUInt/DOSRT   

IF NOT EXISTS ENV:DOSRTPATH

   IF EXISTS rtEZrequest
     rtEZrequest "ERROR:" "DOSReqTools has not been properly installed.\nSee DOSrt.guide for instructions"
     QUIT
   ELSE
     ECHO "DOSReqTools has not been properly installed. See DOSrt.guide for instructions"
     QUIT
   ENDIF

ENDIF

;================   WRITE THE BUTTON FILE TO RAM: ====================

echo  >ram:MAN.buttons "MUInt Keywords;RUN >NIL: SYS:utilities/Amigaguide $muintpath/Muint.guide document muin "
echo >>ram:MAN.buttons "MUInt Extensions;RUN >NIL: SYS:utilities/Amigaguide $muintpath/Muint.guide document exts"
echo >>ram:MAN.buttons "rtEZrequest;RUN >NIL: SYS:utilities/AmigaGuide $DOSRTPATH/DOSrt.guide DOCUMENT rtez"
echo >>ram:MAN.buttons "rtGetString;RUN >NIL: SYS:utilities/AmigaGuide $DOSRTPATH/DOSrt.guide DOCUMENT rtgs"
echo >>ram:MAN.buttons "rtGetLong;RUN >NIL: SYS:utilities/AmigaGuide $DOSRTPATH/DOSrt.guide DOCUMENT rtgl"
echo >>ram:MAN.buttons "rtFileRequest;RUN >NIL: SYS:utilities/AmigaGuide $DOSRTPATH/DOSrt.guide DOCUMENT rtfl"
echo >>ram:MAN.buttons "rtPaletteRequest;RUN >NIL: SYS:utilities/AmigaGuide $DOSRTPATH/DOSrt.guide DOCUMENT rtpr"
echo >>ram:MAN.buttons "rtFontRequest;RUN >NIL: SYS:utilities/AmigaGuide $DOSRTPATH/DOSrt.guide DOCUMENT rtfo"
echo >>ram:MAN.buttons "DRT Extensions;RUN >NIL: SYS:utilities/AmigaGuide $DOSRTPATH/DOSrt.guide DOCUMENT dosx"
echo >>ram:MAN.buttons "Advice;RUN >NIL: SYS:utilities/AmigaGuide $MUINTPATH/Advice/advice.guide DOCUMENT ad "
echo >>ram:MAN.buttons "Textwin;RUN >NIL: SYS:utilities/AmigaGuide $MUINTPATH/TextWin/TextWin.guide DOCUMENT ad"
echo >>ram:MAN.buttons "QUIT"
 
;================   WRITE THE DEFINITION FILE TO RAM: ================

echo   >ram:MAN.cl "BEGIN"
echo  >>ram:MAN.cl "TITLE *"Quick Reference Guide For MUInt/DOSReqTools*"" 
echo  >>ram:MAN.cl "BUTTONS ram:MAN.buttons"
echo  >>ram:MAN.cl "END"


MUInt ram:MAN.cl 

DELETE ram:MAN.#? quiet

QUIT


