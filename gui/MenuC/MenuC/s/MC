.key infile/a,test/s
if exists "<infile>.men"
	MenuC <infile>.men <infile>.c <test>
else 
	echo "MC: File <infile>.men was not found*N"
	quit 20
endif
; modify here for your specific compiler
; below for SAS/C(r)

if "<test>" not eq ""
	lc -L <infile>
	<infile>		; run the test program
else
	lc <infile>
endif

