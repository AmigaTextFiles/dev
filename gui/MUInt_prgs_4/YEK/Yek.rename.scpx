

;     SCRIPT FILENAME: Yek.rename.scp            
;     DATE           : 12-31-95 
;     AUTHOR         : Colin Thompson
;     DESCRIPTION    : Renames the OUTFILE



IF EXISTS ENV:OUTFILE

filepart >env:temp $outfile
     SET RESULT $RC
     
     IF $Result EQ 0
          DELETE ENV:NEWNAME QUIET
          QUIT
     ELSE

          RENAME $OUTFILE $NEWNAME QUIET
          DELETE ENV:NEWNAME QUIET
          QUIT
     ENDIF

ENDIF
ECHO "xxxx"
QUIT

