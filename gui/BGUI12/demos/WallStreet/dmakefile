OD     = dtmp:WallStreet/
EXE    = WallStreet
CFLAGS = -proto -ms -mi -3.0 -mRR -I$(OD) -Ht:WallStreet.m=WallStreet.h
PROTOS = $(OD)WallStreet_protos.h

SRCS  = wallstreet.c
OBJS  = $(SRCS:"*.c":"$(OD)*.o")

all: $(PROTOS) $(EXE)

$(EXE) : $(OBJS)
    dcc $(CFLAGS) %(right) -o %(left) -lbgui

$(OBJS) : $(SRCS)
    dcc $(CFLAGS) %(right) -o %(left) -c

$(PROTOS) : $(SRCS)
    -delete t:WallStreet.m
    makeproto -o %(left) %(right)

