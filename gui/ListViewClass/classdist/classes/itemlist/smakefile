all: usemyic /itemlist.class

usemyic: usemyic.c
	sc link usemyic.c

/itemlist.class: itemlist.o
      slink with <<
LIBPREFIX _
LIBFD itemlist.fd
to /itemlist.class
FROM lib:libent.o lib:libinit.o itemlist.o
lib lib:sc.lib lib:debug.lib lib:amiga.lib
noicons
SD
libversion 1 librevision 0
<

itemlist.o: itemlist.c
      sc libcode nostackcheck itemlist.c
