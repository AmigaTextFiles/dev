all: /separatoritem.class

/separatoritem.class: class.o
      slink with <<
LIBPREFIX _
LIBFD class.fd
to /separatoritem.class
FROM lib:libent.o lib:libinit.o class.o
lib lib:sc.lib lib:amiga.lib lib:debug.lib
noicons
SD
libversion 1 librevision 0
<

class.o: class.c
      sc libcode nostackcheck class.c

