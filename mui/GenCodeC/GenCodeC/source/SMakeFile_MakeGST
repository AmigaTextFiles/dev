Gst = 	GenCodeC.gst \
		GenCodeCGUI.gst \
		GenCodeCMain.gst \
		Hook_utility.gst \
		TextField.gst \
		Tools.gst \
		WriteCatalogFiles.gst \
		WriteExternalFile.gst \
		WriteMainFile.gst

Target: $(Gst)

.c.gst:
	sc mgst=$*.gst ansi errorrexx verbose $*.c
	delete $*.o

clean:                                                                           
    delete $(Gst)                                                            
 
