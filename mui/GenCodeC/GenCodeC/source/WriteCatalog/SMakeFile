Objects = 	WriteCatalogGUI.o \
			WriteCatalogMain.o 

Options = ansi errorrexx verbose

Program =	//Module/WriteCatalog/WriteCatalog

.c.o:
	sc gst=$*.gst $(Options) includedir=/ $*.c

$(Program): $(Objects)
	sc link from $(Objects) \
		to $(Program) \
		lib /WriteCatalogFiles.o \
		lib /Tools.o \
		lib /TextField.o \
		verbose

clean:
	delete $(Objects)
