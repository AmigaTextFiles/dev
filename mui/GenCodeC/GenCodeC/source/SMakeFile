Objects = 	GenCodeC.o \
			GenCodeCGUI.o \
			GenCodeCMain.o \
			Hook_utility.o \
			TextField.o \
			Tools.o \
			WriteCatalogFiles.o \
			WriteExternalFile.o \
			WriteGUIFiles.o \
			WriteMainFile.o

Options = ansi errorrexx verbose

Program =	/Module/GenCodeC

$(Program): $(Objects)
	sc link from $(Objects) \
		to $(Program) \
		verbose
	copy Hook_Utility.o /Module/Hook_Utility.o.sasc
	copy Hook_Utility.h /Module/Hook_Utility.h

.c.o:
	sc gst=$*.gst $(Options) $*.c

WriteGUIFiles.o: WriteGUIFiles.c
	sc $(Options) $*.c

Hook_Utility.o:	Hook_Utility.c
	sc gst=$*.gst $(Options) nostackext nostackcheck $*.c

clean:
    delete $(Objects)

