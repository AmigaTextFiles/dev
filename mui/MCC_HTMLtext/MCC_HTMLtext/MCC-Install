; --- Custom initialisation ---------------------------------------------------

 ; Copyright
 (set #copyright "Copyright © 1997 Dirk Holtwick")

 ; Minimum MUI version required for installation
 (set #min_mui_version 16)
 (set #min_mui_revision 0)

 ; Bultin language
 ; Bit   0 = English
 ; Bit   1 = German
 ; Bit   2 = French
 ; Bit   3 = Spanish
 ; Bit   4 = Portuguese
 ; Bit   5 = Danish
 ; Bit   6 = Italian
 ; Bit   7 = Dutch
 ; Bit   8 = Norwegian
 ; Bit   9 = Swedish
 ; Bit 10 = Polish
 ; Bit 11 = Finnish
 ; Bit 12 = Hungarian
 ; Bit 13 = Greek
 ; Bit 14 = Esperanto
 ; Bit 15 = Turkish
 ; Bit 16 = Latin
 ; Bit 17 = Russian
 ; BIT 18 = Czech
 ; BIT 19 = Catalonian
 (set #language_mcc %11111111111111111111) ; LANGUAGE
 (set #language_mcp %00000000000000000001) ; LANGUAGE

; --- Custom installation procedures ------------------------------------------

 (procedure P_CustomExists
  ; Params:
  ;
  ; Results:
  ;
  ; Used Vars:
  ;

 )

 (procedure P_CustomInstall
  ; Params:
  ;
  ; Results:
  ;
  ; Used Vars:
  ;

 )

 (procedure P_CustomUninstall
  ; Params:
  ;
  ; Results:
  ;
  ; Used Vars:
  ;

 )

; -----------------------------------------------------------------------------
; --- Do not change anything below this line ----------------------------------
; -----------------------------------------------------------------------------

 (set @default-dest "MUI:")

 (if (= @installer-version 0)
   (
    ; old Installer
    (set #prop "")
   )
   (
    ; new Installer
    (set #prop "\x1b[2p")
   )
 )

; --- Installer script strings ------------------------------------------------

 (set #i 0)
 (while (<> @language (select #i "suomi" "ΓeΣtina" "polski" "svenska" "nederlands" "greek" "franηais" "italiano" "dansk" "deutsch" @language)) ; LANGUAGE
   (set #i (+ #i 1))
 )
 (if (= #i 10) ; LANGUAGE
   (set #language "english")
   (set #language @language)
 )

 (set #i 0)
 (set #language_default %10000000000000000000) ; LANGUAGE
 (while (<> #language (select #i "catalonian" "ΓeΣtina" "russian" "latina" "tόrkηe" "esperanto" "greek" "magyar" "suomi" "polski" "svenska" "norsk" "nederlands" "italiano" "dansk" "portuguκs" "espaρol" "franηais" "deutsch" #language)) ; LANGUAGE
   (
    (set #language_default (shiftright #language_default 1))
    (set #i (+ #i 1))
   )
 )
 (select #i ; LANGUAGE
   ( ; Catalonian
    (complete 0)
   )
   ( ; Czech translation by Vit Sindlar <xsindl00@stud.fee.vutbr.cz>
    (if (= @installer-version 0)
      (
       (set #help_askoptions "Pokud mΑte InstalΑtor do verze 42.6, mΚΪe se stΑt, Ϊe uvidΙte tlaΓΙtka se zatrΪenΙm bez popisu. Volba pomocΙ tΕchto tlaΓΙtek bude ignorovΑna.\n\n")
       (set #help_askchoice "Pokud mΑte InstalΑtor do verze 42.6, mΚΪe se stΑt, Ϊe uvidΙte radio-tlaΓΙtka bez popisu. Volba pomocΙ tΕchto tlaΓΙtek zpΚsobΙ peskok na nejbliΪΣΙ niΪΣΙ volbu.\n\n")
      )
    )
    (set #unknown     "neznΑmΩ")
    (set #choice_english    "AngliΓtina")
    (set #choice_german     "NΕmΓina")
    (set #choice_french     "FrancouzΣtina")
    (set #choice_spanish    "σpanΕlΣtina")
    (set #choice_portuguese "PortugalΣtina")
    (set #choice_danish     "DΑnΣtina")
    (set #choice_italian    "ItalΣtina")
    (set #choice_dutch      "HolandΣtina")
    (set #choice_norwegian  "NorΣtina")
    (set #choice_swedish    "σvΧdΣtina")
    (set #choice_polish     "PolΣtina")
    (set #choice_finnish    "FinΣtina")
    (set #choice_hungarian  "MaΔarΣtina")
    (set #choice_turkish    "TureΓtina")
    (set #choice_greek      "ςeΓtina")
    (set #choice_latin      "Latina")
    (set #choice_esperanto  "Esperanto")
    (set #choice_russian    "RuΣtina")
    (set #choice_czech      "γeΣtina")
    (set #choice_catalonian "KatalΑnΣtina"); LANGUAGE

    (set #choice_catalogs   "LokalizaΓnΙ katalogy")
    (set #choice_docs       "Dokumentace")
    (set #choice_developer  "MateriΑl pro programΑtory")
    (set #choice_source     "ZdrojovΩ kΟd")
    (set #choice_install    "Instalovat")
    (set #choice_update     "Obnovit instalaci")
    (set #choice_uninstall  "Odinstalovat")
    (set #prompt_askcpufpu    (cat "Zvolte konfiguraci VaΣeho poΓΙtaΓe (CPU/FPU), pro kterou chcete nainstalovat " @app-name ".mcc"))
    (set #prompt_info         (cat "\nInstalace " @app-name " MUI custom class\n" #copyright "\n\n"))
    (set #prompt_install_uninstall (cat #prompt_info "Pejete si instalovat nebo odinstalovat?"))
    (set #abort_nocpu       (cat "Je mi lΙto, ale " @app-name ".mcc nelze nainstalovat, protoΪe typ VaΣeho CPU nenΙ podporovΑn.\n"))
    (set #abort_nomui       (cat @app-name " MUI custom class je moΪno pouΪΙvat pouze s Magic User Interface (MUI), bez nΕj je custom class nepouΪitelnΩ!\n\n"
             "InstalΑtor nenaΣel " @default-dest ". NejdΙve musΙte nainstalovat MUI a pak mΚΪete zkusit instalaci " @app-name ".mcc!\n"
             )
    )
    (set #abort_oldmui (cat "K instalaci " @app-name ".mcc je nutnΧ MUI alespoΞ verze %s.\n\n"
             "Bylo nalezeno MUI verze %s.\n"
             )
    )
    (set #welcome_install (cat "VΙtejte pi instalaci.\n"
                "ProsΙm zvolte si typ instalace (podle VaΣich znalostΙ Amigy)."
           )
    )
    (set #welcome_uninstall (cat "VΙtejte pi odinstalovΑnΙ.\n"
             "ProsΙm zvolte si typ odinstalovΑnΙ (podle VaΣich znalostΙ Amigy)."
             )
    )
    (set #prompt_uninstall_parts (cat "KterΧ ΓΑsti si pejete odinstalovat?\n"
                  @app-name ".mcp odinstaluje takΧ pΙsluΣnΧ lokalizaΓnΙ katalogy.\n"
             )
    )
    (set #prompt_uninstall_all (cat "Pejete si odinstalovat vΣechny soubory patΙcΙ k " @app-name ".mcc?"))
    (set #help_uninstall_all "JestliΪe nechcete odinstalovat vΣe, budete dotΑzΑni, kterΧ ΓΑsti si pejete odinstalovat.")
    (set #prompt_uninstall_plangs "Podporu pro kterΧ programovacΙ jazyky si pejete odinstalovat?")
    (set #prompt_uninstall_mcp_catalogs (cat "KterΧ lokalizaΓnΙ katalogy pro " @app-name ".mcp si pejete odinstalovat?"))
    (set #prompt_uninstall_mcc_catalogs (cat "KterΧ lokalizaΓnΙ katalogy pro " @app-name ".mcc si pejete odinstalovat?"))
    (set #prompt_uninstall_agdocs "Pro kterΧ jazyky si pejete odinstalovat dokumentaci v AmigaGuide® ?")
    (set #prompt_uninstall_docs "KterΧ z dokumentΚ si pejete odinstalovat?")
    (set #abort_oldversion (cat "" @app-name ".mcc verze %ld.%ld je jiΪ nainstalovΑn.\n\n"
            "JestliΪe si pejete znovu nainstalovat starΣΙ verzi %ld.%ld, pak je nutno nejdΙve odinstalovat novΕjΣΙ verzi."
            )
    )
    (set #prompt_update (cat "KterΧ ΓΑsti " @app-name ".mcc si pejete pidat?"))
    (set #prompt_install_mcc_catalogs (cat "KterΧ katalogy pro " @app-name ".mcc si pejete instalovat?"))
    (set #prompt_install_mcp_catalogs (cat "KterΧ katalogy pro " @app-name ".mcp si pejete instalovat?"))
    (set #prompt_install_docs "KterΧ z dokumentΚ si pejete instalovat?")
    (set #prompt_install_agdocs "Pro kterΧ jazyky si pejete nainstalovat dokumentaci v AmigaGuide® ?")
    (set #prompt_install_demo "Pejete si nainstalovat dodΑvanΧ demo?")
    (set #help_install_demo (cat "Demo je malΩ program, kterΩ demonstruje pouΪitΙ " @app-name "custom class."))
    (set #prompt_install_plangs "Podporu pro kterΧ programovacΙ jazyky si pejete nainstalovat?")
    (set #prompt_install_source "Pejete si nainstalovat zdrojovΩ kΟd?")
    (set #help_install_source (cat "ZdrojovΩ kΟd " @app-name " custom class je moΪno pouΪΙt jako pΙklad pi vytvΑenΙ svΩch vlastnΙch programΚ.\nUpozornΕnΙ - zdrojovΩ kΟd mΚΪe bΩt v jakΧmkoliv jazyce!"))
    (set #prompt_copy "Pejete si nakopΙrovat nΑsledujΙcΙ soubor(y)?")
    (set #prompt_makedir "NΑsledujΙcΙ adresΑ nebyl nalezen, je nutno jej vytvoit:")

    (set #prompt_uninstall_asciidocs "Pro kterΧ jazyky si pejete odinstalovat dokumentaci v ASCII?")
    (set #prompt_install_asciidocs "Pro kterΧ jazyky si pejete nainstalovat dokumentaci v ASCII?")
    (set #exit_uninstall_finished (cat "OdinstalovΑnΙ probΕhlo ΥspΕΣnΕ!\n"
                   '"'
                   @app-name
                   '"'
                   " je odstranΕn z "
                   '"'
                   @default-dest
                   '".'
              )
    )
   )
   ( ; Russian translation by Oleg Sergeev <bigblack@neworder.spb.ru>
    (if (= @installer-version 0)
      (
       (set #help_askoptions "Δπκη ς Αaψa αδοπηώ ξονβοaλλϊ Installer λδμδδ 42.6, ρν λνεδρ ρaι πκςφηρϋπώ, φρν α ξονυδππδ ημπρaκκώυηη Αϊ ςαηγηρδ ιμνξιη-βaκνφιη ΰδζ ξνγξηπδθ. Μaεaρηδ μa ρaιηδ ιμνξιη ΰςγδρ ξονηβμνοηοναaμν.\n\n")
       (set #help_askchoice "Δπκη Αaψa αδοπηώ ξονβοaλλϊ Installer λδμδδ 42.6, ρν λνεδρ ρaι πκςφηρϋπώ, φρν α ξονυδππδ ημπρaκκώυηη Αϊ ςαηγηρδ οaγηνιμνξιη ΰδζ ξνγξηπδθ. Μaεaρηδ μa ρaιηδ ιμνξιη οaαμνζμaφμν μaεaρηύ μa πaλςύ μηεμύύ γνπρςξμςύ ηζ μητ.\n\n")
      )
    )
    (set #unknown     "μδηζαδπρμν")
    (set #choice_english    "Aμβκηθπιηθ")
    (set #choice_german     "Μδλδυιηθ")
    (set #choice_french     "Σοaμυςζπιηθ")
    (set #choice_spanish    "Ηπξaμπιηθ")
    (set #choice_portuguese "Ξνορςβaκϋπιηθ")
    (set #choice_danish     "Γaρπιηθ")
    (set #choice_italian    "Ηρaκϋώμπιηθ")
    (set #choice_dutch      "Βνκκaμγπιηθ")
    (set #choice_norwegian  "Μνοαδεπιηθ")
    (set #choice_swedish    "Ψαδγπιηθ")
    (set #choice_polish     "Ξνκϋπιηθ")
    (set #choice_finnish    "Σημπιηθ")
    (set #choice_hungarian  "Αδμβδοπιηθ")
    (set #choice_turkish    "Ρςοδυιηθ")
    (set #choice_greek      "Βοδφδπιηθ")
    (set #choice_latin      "Κaρημπιηθ")
    (set #choice_esperanto  "άπξδοaμρν")
    (set #choice_russian    "Οςππιηθ")
    (set #choice_czech      "Φδψπιηθ")
    (set #choice_catalonian "Ιaρaκνμπιηθ"); LANGUAGE

    (set #choice_catalogs   "Ιaρaκνβη")
    (set #choice_docs       "Γνιςλδμρϊ")
    (set #choice_developer  "Λaρδοηaκϊ γκώ ξονβοaλληπρνα")
    (set #choice_source     "Ηπτνγμϊδ ινγϊ")
    (set #choice_install    "Ημπρaκκώρνο")
    (set #choice_update     "Νΰμνακδμηδ")
    (set #choice_uninstall  "Γδημπρaκκώυηώ")
    (set #prompt_askcpufpu     (cat "ιaεηρδ ινμσηβςοaυηύ CPU/FPU, γκώ ινρνονθ Αϊ τνρηρδ ξονημπρaκκηοναaρϋ " @app-name ".mcc"))
    (set #prompt_info      (cat "\nΓνΰον ξνεaκναaρϋ α κηφμϊθ MUI-ικaππ " @app-name "\n" #copyright "\n\n"))
    (set #prompt_install_uninstall (cat #prompt_info "Αϊ τνρηρδ ημπρaκκηοναaρϋ ηκη γδημπρaκκηοναaρϋ γaμμϊθ κηφμϊθ ικaππ?"))
    (set #abort_nocpu ( cat "Ηζαημηρδ, μν γaμμϊθ ημπρaκκώρνο μδ λνεδρ ςπρaμναηρϋ " @app-name ".mcc, ρ.ι. ςιaζaμμaώ Αaλη ινμσηβςοaυηώ μδ ξνγγδοεηαaδρπώ.\n"))
    (set #abort_nomui (cat "Κηφμϊθ MUI-ικaππ " @app-name " ηπξνκϋζςδρπώ ρνκϋιν πναλδπρμν π Ανκψδΰμϊλ Ημρδοσδθπνλ Ξνκϋζναaρδκώ (MUI), ημaφδ γaμμϊθ κηφμϊθ ικaππ μδ γνπρςξδμ!\n\n"
         "Ξοδγνξοδγδκδμηδ λaοψοςρa " @default-dest " μδ μaθγδμν. Ξδοδγ ημπρaκκώυηδθ " @app-name ".mcc πμaφaκa ξονημπρaκκηοςθρδ MUI !\n"
          )
    )
    (set #abort_oldmui (cat "Γκώ ημπρaκκώυηη " @app-name ".mcc μδνΰτνγηλa ιaι λημηλςλ %s αδοπηώ MUI.\n\n"
          "Νΰμaοςεδμa ρνκϋιν αδοπηώ %s MUI.\n"
           )
    )
    (set #welcome_install (cat "Γνΰον ξνεaκναaρϋ α ημπρaκκώρνο κηφμνβν MUI-ικaππa.\n"
             "ιaεηρδ ιaι ξονηζανγηρϋ ημπρaκκώυηύ (α ζaαηπηλνπρη νρ Αaψητ ζμaμηθ)."
        )
    )
    (set #welcome_uninstall (cat "Γνΰον ξνεaκναaρϋ α γδημπρaκκώρνο κηφμνβν MUI-ικaππa.\n"
         "ιaεηρδ ιaι ξονηζανγηρϋ γδημπρaκκώυηύ (α ζaαηπηλνπρη νρ Αaψητ ζμaμηθ)."
          )
    )
    (set #prompt_uninstall_parts (cat "Ιaιηδ φaπρη μδνΰτνγηλν γδημπρaκκηοναaρϋ?\n"
              @app-name ".mcp γδημπρaκκηοςδρ η ιaρaκνβη ρνεδ.\n"
         )
    )
    (set #prompt_uninstall_all (cat "Αϊ τνρηρδ γδημπρaκκηοναaρϋ απ½, παώζaμμνδ π " @app-name ".mcc?"))
    (set #help_uninstall_all "Δπκη Αϊ τνρηρδ ςγaκηρϋ απ½, ρν Αϊ ΰςγδρδ ξδοδπξονψδμϊ ξν ιaεγνθ φaπρη.")
    (set #prompt_uninstall_plangs "Ξνγγδοεις ιaιητ ώζϊινα ξονβοaλληοναaμηώ Αϊ τνρηρδ γδημπρaκκηοναaρϋ?")
    (set #prompt_uninstall_mcp_catalogs (cat "Ιaιηδ ιaρaκνβη ι " @app-name ".mcp Αϊ τνρηρδ γδημπρaκκηοναaρϋ?"))
    (set #prompt_uninstall_mcc_catalogs (cat "Ιaιηδ ιaρaκνβη ι " @app-name ".mcc Αϊ τνρηρδ γδημπρaκκηοναaρϋ?"))
    (set #prompt_uninstall_agdocs "Γνιςλδμρaυηύ ιaιητ ώζϊινα α σνολaρδ AmigaGuide® Αϊ τνρηρδ γδημπρaκκηοναaρϋ?")
    (set #prompt_uninstall_docs "Ιaιηδ ρηξϊ γνιςλδμρνα Αϊ τνρηρδ γδημπρaκκηοναaρϋ?")
    (set #abort_oldversion (cat "Νΰμaοςεδμν, φρν αδοπηώ %ld.%ld " @app-name ".mcc ςεδ ξονημπρaκκηοναaμa.\n\n"
        "Δπκη Αϊ τνρηρδ ξδοδημπρaκκηοναaρϋ αδοπηύ %ld.%ld, ρν πμaφaκa γδημπρaκκηοςθρδ απδ γοςβηδ αδοπηη."
