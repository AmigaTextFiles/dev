SRCS  = MUI-Demo.c Pages.c WbMan.c EnvBrowser.c DFView.c ShowImg.c BoopsiDoor.c MUI-Exchange.c DVIprint.c Virtual.c Popup.c Font.c Class1.c Class2.c Class3.c ShowHide.c
PRGS  = $(SRCS:"*.c":"*")
FLAGS = -3.0 -mRR -mi -proto -ms -I/include -L/DLib -Ht:demo.m=demo.h
LIBS  = -lmui -lc -lamiga30s

all: $(PRGS)

$(PRGS) : $(SRCS)
      dcc $(FLAGS) -o %(left) %(right) $(LIBS)
