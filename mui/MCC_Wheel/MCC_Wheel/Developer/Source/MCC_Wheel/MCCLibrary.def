DEFINITION MODULE MCCLibrary;

FROM System IMPORT Regs, RegSet;

CONST
  SavedRegs = RegSet : {D2..D7,A2..A6};

TYPE
  TableType = ARRAY [6] OF ANYPTR;

PROCEDURE Open;
PROCEDURE Close;
PROCEDURE Expunge;
PROCEDURE Reserved;

GROUP
  ProcGrp = Open, Close, Expunge, Reserved;
  All     = ProcGrp,
            SavedRegs, TableType;

END MCCLibrary.
