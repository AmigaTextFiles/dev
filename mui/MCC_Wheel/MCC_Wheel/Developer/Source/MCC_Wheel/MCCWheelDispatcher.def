DEFINITION MODULE MCCWheelDispatcher;

(*
  dispatcher for Wheel group
  to build in in an external class or Cluster module

  next tag    $F9F8020D
  next method $F9F80200
  next config $F9F80208
*)

VAR
  wheelMCC   : DEFERRED POINTER MuiO.CustomClassPtr;

CONST
  cfgWheelNotches       = $F9F80200;
  cfgWheelNotchWidth    = $F9F80201;
  cfgWheelSlantWidth    = $F9F80202;
  cfgWheelBackgroundPen = $F9F80203;
  cfgWheelShinePen      = $F9F80204;
  cfgWheelNotchPen      = $F9F80205;
  cfgWheelShadowPen     = $F9F80206;
  cfgWheelBuffered      = $F9F80207;

  defaultNotches        = 20;
  defaultNotchWidth     = $4000;
  defaultSlantWidth     = $1000;

GROUP
  ConfigGrp =
    cfgWheelNotches,   cfgWheelNotchWidth,
    cfgWheelSlantWidth,
    cfgWheelBackgroundPen, cfgWheelNotchPen,
    cfgWheelShinePen,  cfgWheelShadowPen,
    cfgWheelBuffered,
    defaultNotches,    defaultNotchWidth,
    defaultSlantWidth;

END MCCWheelDispatcher.
