    (set #choice_slovak       "Slovaΰki")
    (set #choice_slovenian    "Slovenaΰki")
    (set #choice_portuguesebr "Portugalski (Br)")

    (set #choice_catalogs   "Kataloge")
    (set #choice_docs	    "Dokumentaciju")
    (set #choice_developer  "Materijal za programere")
    (set #choice_source     "Izvorni kod")
    (set #choice_images     "Slike")
    (set #choice_install    "Instalacija")
    (set #choice_update     "Osveδavanje")
    (set #choice_uninstall  "Uklanjanje")
    (set #prompt_askcpufpu	   (cat "Molim Vas odaberite kombinaciju CPU/FPU za koju δelite da instalirate " @app-name ".mcc"))
    (set #prompt_info		   (cat "\nDobrodoγli u " @app-name " MUI kastom klasu\n" #copyright "\n\n"))
    (set #prompt_install_uninstall (cat #prompt_info "Da li δelite da instalirate ili uklonite kastom klasu?"))
