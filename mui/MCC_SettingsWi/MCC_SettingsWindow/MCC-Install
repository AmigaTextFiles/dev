 ; Minimum OS version required for installation
 (set #min_os_version 37)
 (set #min_os_revision 0)

 ; Minimum MUI version required for installation
 (set #min_mui_version 14)
 (set #min_mui_revision 0)

 ; Bultin language
 ; Bit	0 = English
 ; Bit	1 = German
 ; Bit	2 = French
 ; Bit	3 = Spanish
 ; Bit	4 = Portuguese (European)
 ; Bit	5 = Danish
 ; Bit	6 = Italian
 ; Bit	7 = Dutch
 ; Bit	8 = Norwegian
 ; Bit	9 = Swedish
 ; Bit 10 = Polish
 ; Bit 11 = Finnish
 ; Bit 12 = Hungarian
 ; Bit 13 = Greek
 ; Bit 14 = Esperanto
 ; Bit 15 = Turkish
 ; Bit 16 = Latin
 ; Bit 17 = Russian
 ; Bit 18 = Czech
 ; Bit 19 = Catalan
 ; Bit 20 = Croatian
 ; Bit 21 = Serbian
 ; Bit 22 = Slovak
 ; Bit 23 = Slovenian
 ; Bit 24 = Portuguese (Brazilian)

 (set #language_mcc %1111111111111111111111111) ; LANGUAGE
 (set #language_mcp %0000000000000000000000001) ; LANGUAGE

; --- Custom installation procedures ------------------------------------------

 (procedure P_CustomExists

 )

 (procedure P_CustomInstall

 )

 (procedure P_CustomUninstall

 )

; -----------------------------------------------------------------------------
; --- Do not change anything below this line ----------------------------------
; -----------------------------------------------------------------------------

 (set @default-dest "MUI:")

 (if (= @installer-version 0)
   (
    (set #prop "")
   )
   (
    (set #prop "\x1b[2p")
   )
 )

 (set #i 0)
 (while (<> @language (select #i "srpski" "russian" "suomi" "ΓeΣtina" "polski" "svenska" "nederlands" "greek" "franηais" "italiano" "dansk" "deutsch" @language))
   (set #i (+ #i 1))
 )
 (if (= #i 12)
   (set #language "english")
   (set #language @language)
 )

 (set #i 0)
 (set #language_default %1000000000000000000000000)
 (while (<> #language (select #i "portuguκs-brasil" "slovensko" "Slovak" "srpski" "Croatian" "catalonian" "ΓeΣtina" "russian" "latina" "tόrkηe" "esperanto" "greek" "magyar" "suomi" "polski" "svenska" "norsk" "nederlands" "italiano" "dansk" "portuguκs" "espaρol" "franηais" "deutsch" #language))
   (
    (set #language_default (shiftright #language_default 1))
    (set #i (+ #i 1))
   )
 )
 (select #i
   (
    (complete 0)
   )
   (
    (complete 0)
   )
   (
    (complete 0)
   )
   (
    (if (= @installer-version 0)
      (
       (set #help_askoptions "Sa Installer-om ΰija je verzija manja od 42.6 moδe se desiti da vidite checkbox gadδete bez natpisa. Odabir ovih gadδeta biαe ignorisan.\n\n")
       (set #help_askchoice  "Sa Installer-om ΰija je verzija manja od 42.6 moδe se desiti da vidite radio gadδete bez natpisa. Odabir ovih gadδeta imaαe za rezulat odabir prve niδe vrednosti.\n\n")
      )
    )
    (set #unknown	      "nepoznat")
