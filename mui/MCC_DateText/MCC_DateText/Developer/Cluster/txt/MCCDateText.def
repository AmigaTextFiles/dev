FROM SYSTEM    IMPORT SysStringPtr;
FROM Intuition IMPORT MsgRoot;
FROM Utility   IMPORT HookPtr;
FROM MuiO      IMPORT AreaRec, MuiTags, MethodID;


CONST
  cDateText        = "DateText.mcc"

TYPE
  DateTextRec    = RECORD OF AreaRec END;
  DateTextObject = POINTER TO DateTextRec;

  DateTextTags = TAGS OF MuiTags
    datestringFormat		= $81EE0059 : SysStringPtr;
  END;

  DateTextTagAPtr = POINTER TO ARRAY OF DateTextTags;


PROCEDURE MakeDateTextObjectA(tags : DateTextTagAPtr) : DateTextObject;
PROCEDURE MakeDateTextObject(tags : LIST OF DateTextTags) : DateTextObject;

GROUP
  Essential =
    cDateText,
    DateTextRec,        	DateTextObject,
    DateTextTags,		DateTextTagAPtr,
    MakeDateTextObject,	MakeDateTextObjectA;

  All = Essential;

END MCCDateText.
