
all: Date.mcc

Date.mcc: Date.o
	slink Date_mcc.o to Date.mcc noicons sc sd lib lib:amiga.lib lib:sc.lib

Date.o: Date_mcc.h Date_mcc.c MCCLib.c
	sc opt strict ansi commentnest noicons nolink math=standard \
	nostackcheck nostkext strmer memsize=huge def=__MakeLib \
	objname=Date_mcc.o Date_mcc.c

install:
	copy Date.mcc MUI:Libs/MUI quiet

clean:
	-delete \#?.o \#?.mcc quiet
