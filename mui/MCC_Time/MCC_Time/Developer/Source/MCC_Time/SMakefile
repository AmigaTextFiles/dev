
all: Time.mcc

Time.mcc: Time.o
	slink Time_mcc.o to Time.mcc noicons sc sd lib lib:amiga.lib lib:sc.lib

Time.o: Time_mcc.h Time_mcc.c MCCLib.c
	sc opt strict ansi commentnest noicons nolink math=standard \
	nostackcheck nostkext strmer memsize=huge def=__MakeLib \
	objname=Time_mcc.o Time_mcc.c

install:
	copy Time.mcc MUI:Libs/MUI quiet

clean:
	-delete \#?.o \#?.mcc quiet
