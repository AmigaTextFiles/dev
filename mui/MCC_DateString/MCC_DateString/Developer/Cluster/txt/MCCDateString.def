FROM SYSTEM    IMPORT SysStringPtr;
FROM Intuition IMPORT MsgRoot;
FROM Utility   IMPORT HookPtr;
FROM MuiO      IMPORT AreaRec, MuiTags, MethodID;


CONST
  cDateString        = "DateString.mcc"

TYPE
  DateStringRec    = RECORD OF AreaRec END;
  DateStringObject = POINTER TO DateStringRec;

  DateStringTags = TAGS OF MuiTags
    datestringFormat		= $81EE0047 : SysStringPtr;
  END;

  DateStringTagAPtr = POINTER TO ARRAY OF DateStringTags;


PROCEDURE MakeDateStringObjectA(tags : DateStringTagAPtr) : DateStringObject;
PROCEDURE MakeDateStringObject(tags : LIST OF DateStringTags) : DateStringObject;

GROUP
  Essential =
    cDateString,
    DateStringRec,        	DateStringObject,
    DateStringTags,		DateStringTagAPtr,
    MakeDateStringObject,	MakeDateStringObjectA;

  All = Essential;

END MCCDateString.
