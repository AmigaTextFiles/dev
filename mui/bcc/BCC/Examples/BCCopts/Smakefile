OBJS= initcl.o Popdir.o App.o App_lsopts.o MUI_main.o
PROG= C:Tools/BCCOpts

.bc.c:
	BCC $*.bc

.c.o:
	sc $*.c

.asm.o:
	GenAm $*.asm

$(PROG): $(OBJS)
	sc link $(OBJS) to $(PROG)


App.o: App.c
App.c: App.bc App.bh

App_lsopts.o: App_lsopts.c
App_lsopts.c: App_lsopts.bc

Popdir.o: Popdir.c
Popdir.c: Popdir.bc Popdir.bh

