/* Defines that help adjusting to any compiler */
#ifdef _DCC
#define REG(x) __ ## x
#define ASM
#define SAVEDS __geta4
#else
#define REG(x) register __ ## x
#ifdef _STORM
#define ASM
#define SAVEDS __saveds
#else
#if defined __MAXON__ || defined __GNUC__
#define ASM
#define SAVEDS
#else
#define ASM	__asm
#define SAVEDS __saveds
#endif
#endif
#endif

