(************************)
(* Window Event Handler *)
(************************)

TYPE

(* flags for ehn_Flags *)

  EHNFlags = (alwaysKeys, ehnf1, ehnf2,  ehnf3,  ehnf4,  ehnf5,  ehnf6,  ehnf7,
              ehnf8,      ehnf9, ehnf10, ehnf11, ehnf12, ehnf13, ehnf14, ehnf15
             ) ;
  EHNFlagSet = SET OF EHNFlags ;

(* other values reserved for future use *)

  mEventHandlerNodePtr = POINTER TO mEventHandlerNode ;
  mEventHandlerNode = RECORD
                        node     : ED.MinNode;
                        reserved : SHORTINT;        (* don't touch! *)
                        priority : SHORTINT;        (* event handlers are inserted according to their priority. *)
                        flags    : EHNFlagSet;      (* certain flags, see below for definitions. *)
                        object   : ID.ObjectPtr;    (* object which should receive MUIM_HandleEvent. *)
                        class    : ID.IClassPtr;    (* if !=NULL, MUIM_HandleEvent is invoked on exactly this class with CoerceMethod(). *)
                        events   : ID.IDCMPFlagSet; (* one or more IDCMP flags this handler should react on. *)
  END ;

(* return values for MUIM_HandleEvent (bit-masked, all other bits must be 0) *)
CONST eventHandlerRCEat = S.SHIFT(1,0) ; (* stop MUI from calling other handlers *)



(**********************)
(* List Position Test *)
(**********************)

TYPE
    mListTestPosFlags   = (above, below, left,   right,  ltpf4,  ltpf5,  ltpf6,  ltpf7,
                           ltpf8, ltpf9, ltpf10, ltpf11, ltpf12, ltpf13, ltpf14, ltpf15) ;
    mListTestPosFlagSet = SET OF mListTestPosFlags ;

    mListTestPosResultPtr = POINTER TO mListTestPosResult ;
    mListTestPosResult   = RECORD
                             entry   : LONGINT;               (* number of entry, -1 if mouse not over valid entry *)
                             column  : INTEGER;               (* numer of column, -1 if no valid column *)
                             flags   : mListTestPosFlagSet;   (* see above *)
                             xoffset,                         (* x offset of mouse click relative to column start *)
                             yoffset : INTEGER;               (* y offset of mouse click from center of line
                                                                 (negative values mean click was above center,
                                                                  positive values mean click was below center) *)
                           END ;

