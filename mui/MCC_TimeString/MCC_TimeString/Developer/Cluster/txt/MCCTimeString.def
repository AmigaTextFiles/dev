FROM SYSTEM    IMPORT SysStringPtr;
FROM Intuition IMPORT MsgRoot;
FROM Utility   IMPORT HookPtr;
FROM MuiO      IMPORT CBOOLEAN, AreaRec, MuiTags, MethodID;


CONST
  cTimeString        = "TimeString.mcc"

TYPE
  TimeStringRec    = RECORD OF AreaRec END;
  TimeStringObject = POINTER TO TimeStringRec;

  TimeStringTags = TAGS OF MuiTags
    timestringTimeFormat	= $81EE008A : SysStringPtr;
  END;

  TimeStringTagAPtr = POINTER TO ARRAY OF TimeStringTags;


PROCEDURE MakeTimeStringObjectA(tags : TimeStringTagAPtr) : TimeStringObject;
PROCEDURE MakeTimeStringObject(tags : LIST OF TimeStringTags) : TimeStringObject;

GROUP
  Essential =
    cTimeString,
    TimeStringRec,        	TimeStringObject,
    TimeStringTags,		TimeStringTagAPtr,
    MakeTimeStringObject,	MakeTimeStringObjectA;

  All = Essential;

END MCCTimeString.
