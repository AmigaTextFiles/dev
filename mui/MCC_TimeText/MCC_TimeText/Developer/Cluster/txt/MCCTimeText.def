FROM SYSTEM    IMPORT SysStringPtr;
FROM Intuition IMPORT MsgRoot;
FROM Utility   IMPORT HookPtr;
FROM MuiO      IMPORT CBOOLEAN, AreaRec, MuiTags, MethodID;


CONST
  cTimeText        = "TimeText.mcc"

TYPE
  TimeTextRec    = RECORD OF AreaRec END;
  TimeTextObject = POINTER TO TimeTextRec;

  TimeTextTags = TAGS OF MuiTags
    timetextTimeFormat	= $81EE0098 : SysStringPtr;
  END;

  TimeTextTagAPtr = POINTER TO ARRAY OF TimeTextTags;


PROCEDURE MakeTimeTextObjectA(tags : TimeTextTagAPtr) : TimeTextObject;
PROCEDURE MakeTimeTextObject(tags : LIST OF TimeTextTags) : TimeTextObject;

GROUP
  Essential =
    cTimeText,
    TimeTextRec,        	TimeTextObject,
    TimeTextTags,		TimeTextTagAPtr,
    MakeTimeTextObject,	MakeTimeTextObjectA;

  All = Essential;

END MCCTimeText.
