(SET #startMsg (CAT "\nCe script va installer le catalogue français de SettingsWindows_mcc\n"
                    "\ndans le répertoire SYS:Locale/catalogs/français de votre Amiga.\n"
                    "\nTout sera fait de façon automatique dès que vous aurez cliqué sur continuer.\n"
                    "\n                                                              \n"
                    "\nVersion Française par Eric LUCZYSZYN - Juin 2000\n")
)



(MESSAGE #startMsg)

(COPYFILES (PROMPT "")
           (SOURCE "locale/catalogs/français/settingswindow_mcc.catalog")
           (DEST "SYS:locale/catalogs/français")
           (HELP "")
)
(SET @default-dest "SYS:")

(exit)
