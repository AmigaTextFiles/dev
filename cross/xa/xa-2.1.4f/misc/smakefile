
all: uncpk printcbm file65 reloc65 ld65

uncpk: uncpk.c
	sc link warn=all uncpk.c to uncpk

printcbm: printcbm.c
	sc link warn=all printcbm.c to printcbm

file65: file65.c
	sc link warn=all file65.c to file65

ld65: ld65.c
	sc link warn=all ld65.c to ld65

reloc65: reloc65.c
	sc link warn=all reloc65.c to reloc65

#mkrom.sh: mkrom.sh
#	cp mkrom.sh /mkrom.sh

lt1: lt1.a
	/src/xa -R -c -o lt1 lt1.a

lt2: lt2.a
	/src/xa -R -c -o lt2 lt2.a

lt: lt1 lt2
	ld65 -o lt lt1 lt2 

clean:
	delete *.o 


