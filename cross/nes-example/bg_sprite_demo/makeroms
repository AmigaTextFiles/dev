dd if=bg_demo.nes bs=16 skip=1 of=roms/prg_chr.bin
dd if=roms/prg_chr.bin bs=1k count=16 of=roms/prg.bin
dd if=roms/prg_chr.bin bs=1k skip=16 of=roms/chr.bin
cat roms/prg.bin roms/prg.bin > roms/prg32.bin
cat roms/chr.bin roms/chr.bin roms/chr.bin roms/chr.bin > roms/chr32.bin
rm roms/prg.bin roms/chr.bin roms/prg_chr.bin

