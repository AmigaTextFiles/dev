all: linkGB

linkGB: lkarea.o lkeval.o lkihx.o lklist.o lkrloc.o lksym.o lkdata.o \
			lkhead.o lklex.o lkmain.o lklibr.o lks19.o lkgb.o lkgg.o
	sc LINK	lkarea.o lkeval.o lkihx.o lklist.o lkrloc.o lksym.o lkdata.o \
			lkhead.o lklex.o lkmain.o lklibr.o lks19.o lkgb.o lkgg.o TO linkGB

clean:
	rm *.o *.lnk

lkarea.o: lkarea.c aslink.h
	sc lkarea.c
lkeval.o: lkeval.c aslink.h
	sc lkeval.c
lkihx.o: lkihx.c aslink.h
	sc lkihx.c
lklist.o: lklist.c aslink.h
	sc lklist.c
lkrloc.o: lkrloc.c aslink.h
	sc lkrloc.c
lksym.o: lksym.c aslink.h
	sc lksym.c
lkdata.o: lkdata.c aslink.h
	sc lkdata.c
lkhead.o: lkhead.c aslink.h
	sc lkhead.c
lklex.o: lklex.c aslink.h
	sc lklex.c
lkmain.o: lkmain.c aslink.h
	sc lkmain.c
lklibr.o: lklibr.c aslink.h
	sc lklibr.c
lks19.o: lks19.c aslink.h
	sc lks19.c
lkgb.o: lkgb.c aslink.h
	sc lkgb.c
lkgg.o: lkgg.c aslink.h
	sc lkgg.c
