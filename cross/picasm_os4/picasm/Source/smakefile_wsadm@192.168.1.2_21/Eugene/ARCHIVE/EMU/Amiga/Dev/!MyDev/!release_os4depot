#
# Makefile for picasm (Amiga SAS/C 6.x)
#

DPS = picasm.h SCOPTIONS

OBJS = picasm.o devices.o config.o token.o symtab.o expr.o \
       pic12bit.o pic14bit.o

picasm: $(OBJS)
	sc link $(OBJS) to $@

picasm.o: picasm.c $(DPS)
devices.o: devices.c $(DPS)
config.o: config.c $(DPS)
token.o: token.c $(DPS)
symtab.o: symtab.c $(DPS)
expr.o: expr.c $(DPS)
pic12bit.o: pic12bit.c $(DPS)
pic14bit.o: pic14bit.c $(DPS)
