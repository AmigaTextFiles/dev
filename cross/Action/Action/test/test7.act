MODULE

BYTE V

PROC APROC(BYTE A,B,C,D,E)
	V = A + B + C
RETURN

PROC CPROC(CARD A,B)
	A = B
RETURN

BYTE FUNC AFUNC(BYTE A,B)
RETURN A+B

CARD FUNC CFUNC(CARD L,M)
RETURN L+M

PROC BPROC()
BYTE E,F,G
CARD CA,CB,CC,CD

	APROC(E,F,G+V,E-F,V-D)
	CPROC(CA,CB+CD)
	CC = CFUNC(CB+CD,CA)
	E = AFUNC(F,G)
RETURN