MODULE

BYTE A,B,C

PROC APROC(BYTE D)
BYTE E

E = D + A -C
IF E <= B THEN
	E = E - B
	C  = A RSH B-D
ELSEIF B = D THEN
	C = A LSH D- B
ELSE
	D = B RSH C + A - B
FI
WHILE A <= B + C DO
	A = A - E
	B = A + C
OD
FOR B = A + C TO E STEP D DO
	C = A & B
OD

DO
	A = A + C
	IF A = B THEN EXIT FI
	UNTIL C = A - B
OD
RETURN
