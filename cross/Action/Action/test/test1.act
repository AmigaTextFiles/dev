PROC PROCONE(BYTE AP, BP)
BYTE LONE

IF LONE >= BP - AP THEN 
  LONE = BP + AP
  BP = AP * LONE
FI

DO
	BONE = BTWO + LONE * (6 + 7)
	LONE = BTWO LSH BONE
	UNTIL BONE = BP
OD
RETURN

