.key FROM/K,SCREEN,DEPTH,WINDOW,PORT
.bra [
.ket ]

;$VER: ShellTerm_Script 1.02 (21.05.93) (C)opyright Tomi Blinnikka 1993


if "[FROM]" eq ""
 set FROM "S:ShellTerm.PREFS"
else
 set FROM "[FROM]"
endif

if "[SCREEN]" eq ""
 set SCREEN "ShellTerm Screen"
else
 set SCREEN "[SCREEN]"
endif

if "[DEPTH]" eq ""
 set DEPTH "1"
else
 set DEPTH "[DEPTH]"
endif

if "[WINDOW]" eq ""
 set WINDOW "ShellTerm Window"
else
 set WINDOW "[WINDOW]"
endif

if "[PORT]" eq ""
 set PORT "ShellTerm_Port"
else
 set PORT "[PORT]"
endif


PSX OPEN "$SCREEN" HIRES DEPTH "$DEPTH"
NewShell FROM S:ShellTerm-StartUp "CON:0/10/724/272/$WINDOW/NOCLOSE/Screen$SCREEN"
WatchPort "$PORT"
PSX CLOSE "$SCREEN" HIRES DEPTH 1
