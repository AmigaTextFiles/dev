/*
**  $VER: InsertAsmtext 1.0 (1.11.96)
**
**  By Kenneth C. Nilsen. Public Domain.
**
**  Will convert a text to assembler code
**
*/

options results

endtext	  = "',10" || "0A"X

address	"rexx_ced"

Status NUMLINES
LinePos=RESULT
CurLine=0

'beg of file'

DO UNTIL CurLine=LinePos

	text "09"X || "dc.b" || "09"X || '"'
	'end of line'
	text '",10'
	status 'LINENUMBER'
	'jump to line' RESULT+2
	'beg of line'
	CurLine=CurLine+1
End

exit 0
