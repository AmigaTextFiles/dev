/* Quit ASX                        */
/* 01.08.93 : Daniel Weber         */

ADDRESS 'asx_rexx'
OPTIONS RESULTS

IsTherePort
ret = rc
IF ret~=5 THEN DO
  port = result
  DO WHILE (port ~='' & ret ~=5)
	ADDRESS (''||port)
	EndOfAssembly
	ADDRESS 'asx_rexx'
	IsTherePort
	port = result
	ret  = RC
	END
END

Quit

EXIT(0)
