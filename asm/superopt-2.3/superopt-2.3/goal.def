DEF_GOAL (CMPBYTES,	2, "cmpbytes",
  {
    union { word w; char b[4]; } __r1;
    union { word w; char b[4]; } __r2;
    __r1.w = (v0); __r2.w = (v1);
    r = ((__r1.b[0] != __r2.b[0]) && (__r1.b[1] != __r2.b[1])
	 && (__r1.b[2] != __r2.b[2]) && (__r1.b[3] != __r2.b[3]));
  })

