	IFND	EXEC_LISTS_I
EXEC_LISTS_I	SET	1

	IFND	EXEC_NODES_I
	include	exec/nodes.i
	ENDC

	STRUCTURE	LH,0
		APTR	LH_HEAD
		APTR	LH_TAIL
		APTR	LH_TAILPRED
		UBYTE	LH_TYPE
		UBYTE	LH_PAD
		LABEL	LH_SIZE

	STRUCTURE	MLH,0
		APTR	MLH_HEAD
		APTR	MLH_TAIL
		APTR	MLH_TAILPRED
		LABEL	MLH_SIZE

NEWLIST	MACRO	* list
	MOVE.L	\1,(\1)
	ADDQ.L	#LH_TAIL,(\1)
	CLR.L	LH_TAIL(\1)
	MOVE.L	\1,(LH_TAIL+LN_PRED)(\1)
	ENDM

	ENDC ; EXEC_LISTS_I
