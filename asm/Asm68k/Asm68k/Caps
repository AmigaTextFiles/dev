   OPTION NL
SPC   MACRO
   FORMAT
   ENDM
FAIL  MACRO
   IFNE NARG
   END   \1
   ENDIF
   ENDM
OFFSET   MACRO
   FORMAT
   ENDM
IDNT  MACRO
   IDENTIFY \1
   ENDM
LIST  MACRO
   OPTION L
   ENDM
LLEN  MACRO
   FORMAT
   ENDM
PAGE  MACRO
   PAGEUP
   ENDM
PLEN  MACRO
   FORMAT
   ENDM
NOOBJ MACRO
   FORMAT
   ENDM
NOLIST   MACRO
   OPTION NL
   ENDM
NOPAGE   MACRO
   FORMAT
   ENDM
   OPTION L
