@VERSION 9,0

@REQUIRE "turbocalc"

turbocalc.Open_Turbocalc()


turbocalc.Import("Work:Development/Workshop/Turbocalc_hwp/farbe.TCD")


DebugPrint("Cols: "..turbocalc.N_Col().."   Rows: "..turbocalc.N_Row())

   DebugPrint("                              ")

DebugPrint("Properties Title: "..turbocalc.Properties(#TC_TITLE).." Author: "..turbocalc.Properties(#TC_AUTHOR).." Owner: "..turbocalc.Properties(#TC_OWNER).." Subject: "..turbocalc.Properties(#TC_SUBJECT))

   DebugPrint("                              ")

Local x 
Local y 

  For y = 1 To turbocalc.N_Row()

   For x = 1 To turbocalc.N_Col()


	   
	   DebugPrint("x = "..x.." y = "..y)
	   
       DebugPrint("Contents: "..turbocalc.Contents(x,y))
	   DebugPrint("Width: "..turbocalc.Width(x,y).."  Height: "..turbocalc.Height(x,y))
	   DebugPrint("Celltyp: "..turbocalc.Typ(x,y).."  "..turbocalc.Typ2Text(turbocalc.Typ(x,y)).." Is Expression: "..turbocalc.IsExpression(x,y).." Expression: "..turbocalc.Expression(x,y))
       DEbugprint("Color (0 = standart): apen: "..turbocalc.Apen(x,y).." bpen: "..turbocalc.Bpen(x,y))
       DebugPrint("Textstyle (0 = standart) Bold:"..turbocalc.Text_Bold(x,y).." Italic: "..turbocalc.Text_Italic(x,y).." Underline: "..turbocalc.Text_Underline(x,y).." Align: "..turbocalc.Text_Align(x,y))

       DebugPrint("Border (0 = standart): Top: "..turbocalc.Border_Top(x,y).." Bottom: "..turbocalc.Border_Bottom(x,y).." Left: "..turbocalc.Border_Left(x,y).." Right: "..turbocalc.Border_Right(x,y))
	   
	   
	   DebugPrint("                              ")
	   
    Next

  Next

  
  
  
  
  Local z
  
  For z = 1 To turbocalc.N_Colors()
	  
	  
	  
	  DebugPrint("Colorindex "..z.." Red: "..turbocalc.Color_Red(z).." Green: "..turbocalc.Color_Green(z).." Blue: "..turbocalc.Color_Blue(z))
	  
	  
	  
	  
  Next
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  

  
  turbocalc.Close_Turbocalc()

End

  
 