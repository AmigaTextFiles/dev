@VERSION 5,0

@REQUIRE "xmlparser"

s$ = FileToString("test.svg")

Function StartElement(p, name$, attrs)
	DebugPrint("*** Start Element:", name$)

	For i,v In Pairs(attrs)
		If GetType(i) = #STRING Then DebugPrint("-->", i .. "=" .. v)
	Next

EndFunction

Function EndElement(p, name$)
	DebugPrint("*** End Element:", name$)
EndFunction

Function Comment(p, comment$)
	DebugPrint("*** Comment:", comment$)
EndFunction

p = XMLParser.New({StartElement = StartElement, EndElement = EndElement, Comment = Comment})
Assert(p:Parse(s$))
p:Close()
End

