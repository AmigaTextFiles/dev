@VERSION 9,0

@REQUIRE "expression"





expression.InitVar  ;Init Variables






expression.Add_Var("Mor",36)  ; Add wariable "Mor" with the value "36"
expression.Add_Var("A1",100)
expression.Add_Var("B10",1000)





Debugprint("Result = "..expression.Calc("Mor + A1 * B10"))  ;Calculate the expression incl. variables





expression.Exit_Var   ;Free Variables


End
