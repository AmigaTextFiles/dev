<?xml version="1.0" encoding="iso-8859-1"?>
<application id="app" base="SHOWHIDE">
	<window title="Show &amp; Hide" muiid="SHHD" notify="closerequest">
		<hgroup>
			<vgroup frame="group">
				<hgroup weight="0">
					<checkmark selected="true" notify="selected" userdata="bt1"/>
					<checkmark selected="true" notify="selected" userdata="bt2"/>
					<checkmark selected="true" notify="selected" userdata="bt3"/>
					<checkmark selected="true" notify="selected" userdata="bt4"/>
					<checkmark selected="true" notify="selected" userdata="bt5"/>
				</hgroup>

				<vgroup>
					<button id="bt1">Button 1</button>
					<button id="bt2">Button 2</button>
					<button id="bt3">Button 3</button>
					<button id="bt4">Button 4</button>
					<button id="bt5">Button 5</button>
					<vspace height="0"/>
				</vgroup>
			</vgroup>
		</hgroup>
	</window>
</application>

