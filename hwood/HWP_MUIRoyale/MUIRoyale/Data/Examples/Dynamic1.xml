<window title="#%COUNT: Window" notify="closerequest" id="win%COUNT" defaultobject="lv%COUNT">
	<vgroup>
		<vgroup frame="group" frametitle="Window %COUNT">
			<listview id="lv%COUNT" cyclechain="1">
				<column>
					<item>One</item>
					<item>Two</item>
					<item>Three</item>
				</column>
			</listview>
			<hgroup>
				<button id="new%COUNT" notify="pressed">New window</button>
				<button id="dump%COUNT" notify="pressed">Dump windows</button>
				<button id="close%COUNT" notify="pressed">Close window %COUNT</button>
			</hgroup>
		</vgroup>
	</vgroup>
</window>

