@VERSION 9,0

@REQUIRE "turbocalc"

turbocalc.Open_Turbocalc()


turbocalc.Import("Work:Development/Workshop/Turbocalc_hwp/formel.TCD")


DebugPrint("Cols: "..turbocalc.N_Col().."   Rows: "..turbocalc.N_Row())



DebugPrint("Properties Title: "..turbocalc.Properties(#TC_TITLE).." Author: "..turbocalc.Properties(#TC_AUTHOR).." Owner: "..turbocalc.Properties(#TC_OWNER).." Subject: "..turbocalc.Properties(#TC_SUBJECT))

Local x
Local y

  For y = 1 To turbocalc.N_Row()

   For x = 1 To turbocalc.N_Col()



    DebugPrint("x = "..x.." y = "..y.." Contents: "..turbocalc.Contents(x,y).." Celltyp: "..turbocalc.Typ(x,y).."  "..turbocalc.Typ2Text(turbocalc.Typ(x,y)).." Expression: "..turbocalc.Expression(x,y))



    Next

  Next

turbocalc.Close_Turbocalc()

End
