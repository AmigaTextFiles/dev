<window title="#%COUNT: Window" id="win%COUNT" defaultobject="lv%COUNT" notify="closerequest">
	<vgroup>
		<vgroup frame="true" frametitle="Window %COUNT">
			<listview id="lv%COUNT">
				<column>
					<item>One</item>
					<item>Two</item>
					<item>Three</item>
				</column>
			</listview>
			<hgroup>
				<button id="new%COUNT">New window</button>
				<button id="close%COUNT">Close window %COUNT</button>
			</hgroup>
		</vgroup>
	</vgroup>
</window>

