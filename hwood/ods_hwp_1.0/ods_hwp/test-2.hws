@VERSION 9,0

@REQUIRE "ods"

ods.Open_Ods()


ods.Import("Work:Development/Workshop/Ods_hwp/ods/bold_italic_underline.ods")

DebugPrint("Properties Title: "..ods.Properties(#ODS_TITLE).." Author: "..ods.Properties(#ODS_AUTHOR).." Owner: "..ods.Properties(#ODS_OWNER).." Subject: "..ods.Properties(#ODS_SUBJECT))



DebugPrint("Cols: "..ods.N_Col(1).."   Rows: "..ods.N_Row(1))

DebugPrint("Sheets: "..ods.N_Sheet().." Styles: "..ods.N_Styles())

DEbugPrint("----------------------------------------------------------------------------------------------------------------------------------------------------------------")

DebugPrint("Default Style: Width: "..ods.Get_Default_Style(#ODSSTYLE_WIDTH).." \tHeight: "..ods.Get_Default_Style(#ODSSTYLE_HEIGHT).." \tFontColor: "..ods.Get_Default_Style(#ODSSTYLE_FONT_COLOR).." \tBG-Color: "..ods.Get_Default_Style(#ODSSTYLE_BG_COLOR).." \tFont: "..ods.Get_Default_Style(#ODSSTYLE_FONT))
DEbugPrint("----------------------------------------------------------------------------------------------------------------------------------------------------------------")


Local z = 1


DebugPrint("Sheet name: "..ods.Sheet_Name(z))



Local x
Local y

  For y = 1 To ods.N_Row(z)
	  
	  DebugPrint("\n")

   For x = 1 To ods.N_Col(z)



    DebugPrint("x = "..x.." y = "..y.." \tSheet = "..z.." \tContents: "..ods.Contents(x,y,z).." \tCelltyp: "..ods.Typ(x,y,z).." \tExpression: "..ods.Expression(x,y,z))
	
	DebugPrint(" ")

    DEbugPrint("Styles: Width:"..ods.Get_Style(x,y,z,#ODSSTYLE_WIDTH).." \tHeight: "..ods.Get_Style(x,y,z,#ODSSTYLE_HEIGHT).." \tBorder: Top: "..ods.Get_Style(x,y,z,#ODSSTYLE_BORDER_TOP).." \tBottom: "..ods.Get_Style(x,y,z,#ODSSTYLE_BORDER_BOTTOM).." \tLeft: "..ods.Get_Style(x,y,z,#ODSSTYLE_BORDER_LEFT).." \tRight: "..ods.Get_Style(x,y,z,#ODSSTYLE_BORDER_RIGHT))
	DEbugPrint("Fontcolor: "..ods.Get_Style(x,y,z,#ODSSTYLE_FONT_COLOR).." \tBackgroundcolor: "..ods.Get_Style(x,y,z,#ODSSTYLE_BG_COLOR).." \tFont: "..ods.Get_Style(x,y,z,#ODSSTYLE_FONT).." \tFontsize: "..ods.Get_Style(x,y,z,#ODSSTYLE_FONT_SIZE).." \tTextalign: "..ods.Get_Style(x,y,z,#ODSSTYLE_TEXT_ALIGN).." \tFontweight: "..ods.Get_Style(x,y,z,#ODSSTYLE_FONT_WEIGHT))   
	
	   
	   
	   
	 DebugPrint("---------------------------------------------------------------------------------------------------------------------------------------------------------------------") 
	   
	   
	   
	   

    Next

  Next

ods.Close_Ods()

End
