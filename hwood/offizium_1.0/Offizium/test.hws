
@VERSION 6,0

@REQUIRE "offizium"

DebugOutput(TRUE)


@DISPLAY {Width = 800, Height = 600,Color = #WHITE}


/*

Papersize:

0 = A3
1 = A4
2 = A5
3 = A6
4 = Letter
5 = Legal
6 = tolino
7 = 640x480
8 = 800x600
9 = 1024x768
10 = 1280x1024

*/

/*

Zoom:

100
125
150
175
200


*/

/* Office Mode
0 Text mode
1 Graphic mode
*/


offizium.Start_App(100,7)  ;offizium.Start_App(zoom,papersize)  
offizium.Open_Window()  

offizium.Office_Mode(1) ;offizium.Office_Mode(office mode)



offizium.Start_Inputhandler()  


offizium.Render_Brush(100)  ;offizium.Render_Brush(brush id)  



DisplayBrush(100,0,0)







WaitLeftMouse


offizium.End_App()  

End
