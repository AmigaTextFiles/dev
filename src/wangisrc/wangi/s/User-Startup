Alias . "Dir []~(#?.info)"
Alias , "List []~(#?.info)"
Alias mv "Run <>NIL: Multiview [] SCREEN"
Alias e "Run <>NIL: Ed []"
Alias ped "Run <>NIL: HSPASCAL:HSPascal []"
Alias del "Delete []"
Alias ls "List [] LFORMAT=%n"
Alias ll "List []"
Alias mlzx "Lzx -aerRxY []"
Alias mlha "LhA -aerRxZ []"
Alias enon "Run <>NIL: Enforcer SHOWPC STACKCHECK AREGCHECK DREGCHECK DATESTAMP VERBOSE FILE=CON://///AUTO ON"
Alias enoff "Enforcer OFF"
Alias getnode "GFindXXX n [] #? FULL QUIET"

copy s:rd.inf ram:disk.info QUIET

;enon

