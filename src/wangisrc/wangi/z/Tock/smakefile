#
# Makefile for Find#?
#
#

GST = Include:Tock.GST
OPTS = NOCHECKABORT

All: $(GST) Tock


Tock: Tock.o
	sc $(OPTS) link to $@ with <<
$@.o
<
	bumprev 1 $@

Tock.o: Tock.c scoptions $(GST)
	sc $(OPTS) NOLINK GST $(GST) Tock.c OBJECTNAME $@

$(GST): gst.c scoptions
	sc gst.c MAKEGST=$@ IGNORE=105 NOOPTIMIZE NOLINK NOPPONLY
	-delete gst.o
