#
# Makefile for GMulti
#
#

OBJS= GMulti.o

GMulti: $(OBJS) 
   sc NOCHECKABORT link to GMulti with <<
$(OBJS)
<
   Copy GMulti TO SYS:WBStartup

GMulti.o: GMulti.c scoptions INCLUDE:GMulti.gst
	sc GMulti.c GST=INCLUDE:GMulti.gst NOLINK

INCLUDE:GMulti.gst: gst.c scoptions smakefile
	sc gst.c MAKEGST=INCLUDE:GMulti.gst IGNORE=105 NOOPTIMIZE
	delete gst.o

dist:
	execute makedist

