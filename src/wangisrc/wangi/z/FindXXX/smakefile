#
# Makefile for Find#?
#
#

GST = Include:FindXXX.GST
OPTS = NOCHECKABORT

All: $(GST) FindXXX


FindXXX: FindXXX.o
	sc $(OPTS) link to $@ with <<
$@.o
<
	bumprev 1 $@
	Copy $@ TO MAIL:bin

FindXXX.o: FindXXX.c scoptions $(GST)
	sc $(OPTS) NOLINK GST $(GST) FindXXX.c OBJECTNAME $@

$(GST): gst.c scoptions
	sc gst.c MAKEGST=$@ IGNORE=105 NOOPTIMIZE NOLINK NOPPONLY
	-delete gst.o
