CC= sc:c/sc
TARGET= MidMoose
OBJS= MidMoose.o
CSRC= MidMoose.c
GST= INCLUDE:MidMoose.gst
GSTSRC= gst.c
OPTSFILE= scoptions

$(TARGET): $(OBJS) 
	$(CC) NOCHECKABORT link to $(TARGET) with <<
$(OBJS)
<

$(OBJS): $(CSRC) $(GST)
	$(CC) $(CSRC) NOLINK GST=$(GST)

$(GST): $(MAKEFILE) $(OPTSFILE) $(GSTSRC)
	$(CC) $(GSTSRC) MAKEGST=$(GST) NOLINK NOOPTIMIZE IGNORE=105




