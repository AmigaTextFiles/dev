#
# Makefile for GFindXXX
#
#

GST = Include:GFindXXX.GST
OPTS = NOCHECKABORT

All: $(GST) GFindXXX


GFindXXX: GFindXXX.o
	sc $(OPTS) link to $@ with <<
$@.o
<
	bumprev 1 $@
	Copy $@ TO MAIL:bin

GFindXXX.o: GFindXXX.c scoptions $(GST)
	sc $(OPTS) NOLINK GST $(GST) GFindXXX.c OBJECTNAME $@

$(GST): gst.c scoptions
	sc gst.c MAKEGST=$@ IGNORE=105 NOOPTIMIZE NOLINK NOPPONLY
	-delete gst.o
