Failat 21

Delete >NIL: T:Startup-Menu#? T:SM#? ALL QUIET
Delete dist/#?.unit

copy >NIL: dist t:Startup-Menu ALL clone
copy >NIL: dist.info t:Startup-Menu.info clone
copy >NIL: LIBS:reqtools.library t:Startup-Menu/reqtools.library
Makedir >NIL: T:SMSrc
copy >NIL: #? T:SMSrc ALL CLONE
Delete >NIL: T:SMSrc/reg

pcd t:Startup-Menu

Umentiler FILE_ID.DIZ QUIET
Umentiler Startup-Menu.guide QUIET
Umentiler Startup-Menu.readme QUIET
Umentiler T:SMSrc/SMSrc.readme T:SMSrc.readme QUIET

rename FILE_ID.DIZ /FILE_ID.DIZ
copy >NIL: Startup-Menu.readme /Startup-Menu.readme

cd t:

; Create archives
lha -aerRxZq a Startup-Menu.lha Startup-Menu/#? Startup-Menu.info FILE_ID.DIZ
lha2lzx Startup-Menu.lha Startup-Menu.lzx
lha -aerRxZq a SMSrc.lha SMSrc/#?
lha2lzx SMSrc.lha SMSrc.lzx

; cleanup
delete FILE_ID.DIZ Startup-Menu SMSrc Startup-Menu.info ALL QUIET

pcd
