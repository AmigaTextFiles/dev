.key file/a,cmd,desc
.bra {
.ket }
.def desc=file_id.diz
if {cmd} eq "read"
 unarj_fid "{file}" >nil:
endif
if {cmd} eq "execute"
 execute env:viewdiz/modules/.getdrawer
 cd "$dest"
 unarj x "{file}" >con:
endif
if {cmd} eq "info"
 setenv viewdiz/.minf "*"{file}*"" rx 6 _UnArj (#?.arj) ARJ-archive
endif
;$VER: ViewDIZ-ARJ.module 1.1 (1.9.98) Mikael Lund