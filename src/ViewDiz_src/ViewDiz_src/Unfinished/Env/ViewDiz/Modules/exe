.key file/a,cmd/a,desc
.def desc=t:file_id.diz
.bra {
.ket }
if {cmd} eq "read"
 exedescript x "{desc}" "{file}" >nil:
endif
if {cmd} eq "write"
 exedescript d "{file}" >nil:
 exedescript a "{desc}" "{file}" >nil:
endif
if {cmd} eq "delete"
 exedescript d "{file}" >nil:
endif
if {cmd} eq "execute"
 "{file}"
endif
if {cmd} eq "info"
 setenv viewdiz/.minf "*"{file}*"" rwxd 0 _Run (#?.exe) Executable
endif
;$VER: ViewDiz-Exe.module 1.0 (5.9.98) Mikael Lund
