.key file/a,cmd/a,desc
.def desc="t:file_id.diz"
.bra {
.ket }
if {cmd} eq "delete"
 filenote "{file}"
endif
if {cmd} eq "execute"
 execute env:viewdiz/modules/.getdrawer
 cd $dest
 gzip -dq "{file}"
endif
if {cmd} eq "info"
 setenv viewdiz/.minf "*"{file}*"" xd 9 _Decompress (#?.gzip|#?.gz) TarGz-archive
endif
;$VER: ViewDIZ-GZip.module 1.2 (1.9.98) Mikael Lund
