CFLAGS = optimize verbose


all: chunkyiclass.lib \
     test

chunkyiclass.lib: chunkyiclass.o
   oml $@ r $<

test: test.o
   slink FROM LIB:c.o test.o \
         TO test \
         LIB LIB:sc.lib LIB:amiga.lib chunkyiclass.lib \
         STRIPDEBUG VERBOSE

myinclude:chunkyiclass.h: chunkyiclass.h
   copy $< $@

chunkyiclass.o: chunkyiclass.c chunkyiclass.h

test.o: test.c
