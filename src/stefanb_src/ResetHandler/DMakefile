PRE   = T:ResetHandler.m
FLAGS = -3.1 -proto -mRR -mi -ms -H$(PRE)=ResetHandler.h
PROGS = ResetHandler MyResetHandler
SRCS  = $(PROGS:"*":"*.c")

all : $(PROGS)

$(PROGS) : $(SRCS)
         dcc $(FLAGS) -o %(left) %(right)

$(PROGS) : $(PRE)

$(PRE) : ResetHandler.h DMakefile
         -Delete %(left)
