FLAGS  = -3.0 -mRR -mi -ms -proto -I /include -L /dlib

all: example showimage

example : example.c
      dcc $(FLAGS) -o %(left) %(right) -ltm

showimage : showimage.c
      dcc $(FLAGS) -o %(left) %(right) -ltm
