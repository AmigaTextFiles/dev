#
# DMakefile for UMS Debug routines
#
DBGDIR = t:umsuucp/umsdebug
CFLAGS = -3.1 -mRR -mi -proto -ms -r -c

SRCS   = umsdebug.c
OBJS   = $(SRCS:"*.c":"$(DBGDIR)/*.o")

all : $(DBGDIR) $(OBJS)

$(OBJS) : $(SRCS)
         dcc $(CFLAGS) -o %(left) %(right)

$(OBJS) : /ums_uucp.h DMakefile

$(DBGDIR) : $(DBGDIR)
         -makedir %(left)

clean :
         -delete $(DBGDIR) all quiet force
