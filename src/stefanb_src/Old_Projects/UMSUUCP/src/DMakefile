#
# DMakefile for the UMS_UUCP package
#
TMPDIR = t:umsuucp

DIRS = AUUCPLib debug uuxqt ums2uucp
TAGS = $(DIRS:"*":"*/compiled")

all : $(TMPDIR) $(TAGS)

$(TAGS) : $(DIRS)
         @cd %(right)
         @echo "making all in %(right)..."
         @dmake -q
         @cd /

$(TMPDIR) : $(TMPDIR)
         -makedir %(left)

clean :
         -delete $(TMPDIR) all quiet force
