FLAGS  = -3.1 -r -mRR -ms
CFLAGS = $(FLAGS) -proto -mi
LFLAGS = $(FLAGS)
LIBS   =

SRCS = freeze.c bitio.c debug.c decode.c default.c encode.c huf.c lz.c
OBJS = $(SRCS:"*.c":"t:*.o")

freeze : $(OBJS)
      dcc $(LFLAGS) -o %(left) %(right) $(LIBS)

$(OBJS) : $(SRCS)
      dcc $(CFLAGS) -o %(left) -c %(right)

$(OBJS) : freeze.h

$(OBJS) : compat.h

$(OBJS) : huf.h

$(OBJS) : bitio.h

$(OBJS) : patchlevel.h

$(OBJS) : lz.h

$(OBJS) : config.h

