#
# DMakefile  V1.0
#
# Makefile for dospath.library C examples
#
#  DOSPathTest  - Test dospath.library
#  DOSPathWhich - Implementation of the "which" program using dospath.library
#
# (c) 1996 Stefan Becker
#

FLAGS = -3.1 -mRR -mi -ms -r -proto -I /include -L /dlib
LIBS  = -ldospath

all : DOSPathTest DOSPathWhich

DOSPathTest : DOSPathTest.c
      dcc $(FLAGS) -o %(left) %(right) $(LIBS)

DOSPathWhich : DOSPathWhich.c
      dcc $(FLAGS) -o %(left) %(right) $(LIBS)
