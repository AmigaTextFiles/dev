   IF SHOWLIST('Ports','AmigaTeX')~=0 THEN LEAVE
   CALL DELAY 50
  END
  IF SHOWLIST('Ports','AmigaTeX')=0 THEN RETURN 10
END
ADDRESS 'AmigaTeX'
'ToBack'
'CD RAM:'
RETURN 0
