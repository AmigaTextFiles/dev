/* TeXSwitch.dme V2.00 18.12.1989 SB */
IF SHOWLIST('Ports','AmigaTeX')=0 THEN RETURN 10
PARSE ARG mode
mode=UPPER(LEFT(mode,1))
SELECT
 WHEN mode='F' THEN CALL ToFront
 WHEN mode='B' THEN CALL ToBack
 OTHERWISE
  res=GETCLIP('TeXDMEts')
  IF res='' THEN CALL ToFront
            ELSE CALL ToBack
END
RETURN 0

ToFront:
 ADDRESS 'AmigaTeX' 'ToFront'
 CALL SETCLIP 'TeXDMEts','1'
 RETURN 0

ToBack:
 ADDRESS 'AmigaTeX' 'ToBack'
 CALL SETCLIP 'TeXDMEts',''
 RETURN 0
