/* PreviewSwitch.dme V2.00 18.12.1989 SB */
IF SHOWLIST('Ports','TeXpreview')=0 THEN RETURN 10
PARSE ARG mode
mode=UPPER(LEFT(mode,1))
SELECT
 WHEN mode='F' THEN CALL ToFront
 WHEN mode='B' THEN CALL ToBack
 OTHERWISE
  res=GETCLIP('TeXDMEps')
  IF res='' THEN CALL ToFront
            ELSE CALL ToBack
END
RETURN 0

ToFront:
 ADDRESS 'TeXpreview' 'ToFront'
 CALL SETCLIP 'TeXDMEps','1'
 RETURN 0

ToBack:
 ADDRESS 'TeXpreview' 'ToBack'
 CALL SETCLIP 'TeXDMEps',''
 RETURN 0
