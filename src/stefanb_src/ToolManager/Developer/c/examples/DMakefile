DEVDIR = /
IDIRS  = -I$(DEVDIR)include
LDIRS  = -L$(DEVDIR)dlib
LIBS   = -ltoolmanager
FLAGS  = -3.1 -mRR -mi -ms -proto

all: ShowImage

ShowImage : ShowImage.c
      dcc $(FLAGS) $(IDIRS) -o %(left) %(right) $(LDIRS) $(LIBS)
