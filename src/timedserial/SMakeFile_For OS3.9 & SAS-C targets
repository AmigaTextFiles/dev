TimedSerialD.o : TimedSerial.c TimedSerial.h
  SC $(DOpts) objname=TimedSerialD.o TimedSerial.c
#
# -------------- FINAL TARGET: ------------------------------------
#
TimedSerial.o : TimedSerial.c TimedSerial.h
  SC $(COpts) TimedSerial.c
#
