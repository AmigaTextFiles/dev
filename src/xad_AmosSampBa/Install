(if (patmatch "(68040|68060)" (database "cpu"))
  (set #client "AmosSampBank.040")
  (if (patmatch "(68020|68030)" (database "cpu"))
    (set #client "AmosSampBank.020")
    (set #client "AmosSampBank.000")
  )
)

(copyfiles
  (help @copyfiles-help)
  (source #client)
  (dest "LIBS:xad")
  (newname "AmosSampBank")
)
