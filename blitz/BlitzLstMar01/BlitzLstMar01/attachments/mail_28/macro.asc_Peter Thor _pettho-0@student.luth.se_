
Macro printlabeladdress
  ?label`1
End Macro

;--

  NPrint !printlabeladdress{0}
  NPrint !printlabeladdress{1}
  NPrint !printlabeladdress{2}
  NPrint !printlabeladdress{3}

  Stop

End

;--

label0
  IncBin ram:0
endoflabel0:

label1
  IncBin ram:1
endoflabel1:

label2
  IncBin ram:2
endoflabel2:

label3
  IncBin ram:3
endoflabel3:


