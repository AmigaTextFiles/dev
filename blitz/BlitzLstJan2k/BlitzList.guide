;;             |   |  | |
;;             |   |  | |
;;             |   |  | |
;;             |   |  | l 0=3D absolute, ie from screen's top-left
;;             |   |  |
;;             |   |  l 0 =3D no mouse click
;;             |   |
;;             |   l y position =3D 128
;;             |
;;             l x position =3D 320

