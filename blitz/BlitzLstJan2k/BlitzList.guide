INCLUDE"tcpfuncs.bb2"         ; use latest blitz tcp extension

TCPbuflen=2048                ;Maximum data size to read at any time

If NumPars=0
  NPrint "TestTCP. Get the root document of selected host"
  NPrint ""
  NPrint "usage TestTCP host"
  NPrint "eg. TestTCP www.monrif.net"
  Goto fine
EndIf
;Stop
host$=Par$(1)

;host$="www.monrif.net"

;simple fuctions for parsing http headers...

;return true if a 200 status otherwise just false

;Function  .b exists_url{header$}
