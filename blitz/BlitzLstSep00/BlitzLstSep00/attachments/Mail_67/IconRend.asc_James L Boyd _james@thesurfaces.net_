iconname$="miami:miami"

FindScreen 0

Window 0,0,0,640,100,$20140f,"",1,2

*rp.RastPort=RastPort(0)

  *do.DiskObject = GetDiskObject_(&iconname$)

  If *do

      *im.Image = *do\do_Gadget\SelectRender

  FreeDiskObject_ *do
  Else Request "","No such icon!","OK"
  EndIf

  If *im
    DrawImage_ *rp,*im,20,20
  EndIf

MouseWait
End
