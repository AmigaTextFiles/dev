_DATATYPE
  redrawn=0
  lock.l=Lock_(&name$,#ACCESS_READ)
  If lock
    *dtn=ObtainDataTypeA_(#DTST_FILE,lock,0)
    If *dtn
      *dth=*dtn\dtn_Header
      GroupID$=Left$(NPeek$(&*dth\dth_GroupID),4)
      If GroupID$="pict"
        *obj.b=NewDTObjectA_(&name$,attrs(1))
        If *obj
          dtf\MethodID=#DTM_FrameBox
          dtf\dtf_ContentsInfo=&fri,&fri,SizeOf.FrameInfo
          DoDTMethodA_ *obj,0,0,&dtf
          gpl\MethodID=#DTM_PROCLAYOUT
          gpl\gpl_GInfo=0
          gpl\gpl_Initial=1
          DoDTMethodA_ *obj,0,0,&gpl
          GetAttr_ #PDTA_CRegs,*obj,&*cregs
          GetAttr_ #PDTA_BitMapHeader,*obj,&*bmhd
          GetAttr_ #PDTA_BitMap,*obj,&*bm
          For i.l=32 To 254
            j.l=i*12
            j+*cregs
            r.l=NPeekB(j  ) MOD 256
            g.l=NPeekB(j+4) MOD 256
            b.l=NPeekB(j+8) MOD 256
            AGAPalRGB #BMCM,i,r,g,b
          Next
          BltBitMapRastPort_ *bm,0,0,RastPort(#WIND),#PICX,#PICY,160,128,$c0
          ShowPalette #BMCM
          redrawn=1
          DisposeDTObject_ *obj
        End If
      End If
      ReleaseDataType_ *dtn
    End If
    UnLock_ lock
  End If
Return



