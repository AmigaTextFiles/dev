if not exists ram:fib_test_dir
   makedir ram:fib_test_dir
endif

fib_com.exe MC.RAW ram:fib_test_dir/MC_f.RAW f

join MC_f_hed.8svx ram:fib_test_dir/MC_f.RAW as ram:fib_test_dir/MC_f.8svx

fib_com.exe MC.RAW ram:fib_test_dir/MC_e.RAW e

join MC_e_hed.8svx ram:fib_test_dir/MC_e.RAW as ram:fib_test_dir/MC_e.8svx

echo ""
echo " Check out `ram:fib_test_dir'...!!!"
echo ""
