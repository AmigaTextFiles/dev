
; Create executable, and pass it two arguments...

DEFTYPE.RDArgs *rdargs
DEFTYPE.b *fl, *str
DEFTYPE.l rc

NEWTYPE.CustomArgs
  args0.l
  args1.l
  args2.l
End NEWTYPE

DEFTYPE.CustomArgs args

arg$="FILE/A,STRING/A"

*rdargs=ReadArgs_(&arg$, &args, 0)

If *rdargs=0

  PutStr_ "Wrong arguments: Test FILE/A,STRING/A"
  Goto quit

EndIf

fl$   = Peek$(args\args0)+Chr$(10)
sg$   = Peek$(args\args1)+Chr$(10)

PutStr_ &fl$
PutStr_ &sg$

quit
End
