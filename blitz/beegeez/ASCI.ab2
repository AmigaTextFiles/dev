
; Just a little programers util to get ascii key codes.

WBStartup:FindScreen 0
SizeLimits 95,30,175,30
Window 0,0,0,175,30,$140F,"ASCII CODE",0,1
WindowInput 0:WLocate 30,6:Print "Hit any key."
Repeat
  If WaitEvent=$200 Then End
  a=Asc(Inkey$)
  If a>0 Then WCls:WLocate 1,6:Print " DEC=",a,"   HEX=",StripLead$(Hex$(a),48)
Forever



