NEWTYPE.foo
    pad.w
    text.s
End NEWTYPE

Function$ RandomString{maxlength.l}
    tempstring$ = String$(Chr$(Rnd(26)+65),Rnd(maxlength)+1)
    Function Return tempstring$
End Function

Dim List blah.foo(0)
For i.w=0 To 99
    If AddItem(blah())
        blah()\text= RandomString{10}
    EndIf
Next

ResetList blah()
i.w=0
While NextItem(blah())
    NPrint i," ",blah()\text
    i+1
Wend

ClickMouse
End


