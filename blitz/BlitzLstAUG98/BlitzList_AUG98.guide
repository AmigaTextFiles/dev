SPR_COPLIST.b    =3D 0
BitMaps1_HEIGHT.w =3D 160
BitMaps2_HEIGHT.w =3D 90

LoadPalette 0,"demo.pal"
LoadShape 0,"demo.br"

GetaSprite 0,0

BitMap 0,320,256,8
BitMap 1,320,256,8

InitCopList 0,44,161,$10018,8,256,0
InitCopList 1,44+173,95,$10018,8,256,0

BLITZ

CreateDisplay 0,1

DisplayBitMap 0,0
DisplayPalette 0,0

DisplayBitMap 1,1

Mouse On

BitMapOutput 0
NPrint Centre$("My CopList 1",40)

BitMapOutput 1
NPrint Centre$("My CopList 2",40)

SetInt 5
     DisplaySprite SPR_COPLIST,0,MouseX,MouseY,4
End SetInt

While NOT Joyb(0)=3D2
     If Joyb(0)=3D1
       AMIGA
       DefaultOutput                                         ;
       NPrint "Now watch how the mousepointer is jumping!!!" ; do somethi=
ng in Amiga Mode
       VWait 200                                             ;
       BLITZ
     EndIf
     Gosub mousetest
     VWait
Wend

End

=2Emousetest
If MouseY>(BitMaps1_HEIGHT-2)
  DisplaySprite 0,0,MouseX,161,4
  SPR_COPLIST=3D1
  mpos=3D1
  MouseArea 0,3,575,5 : VWait
  MouseArea 0,3,575,81
Else
