item$(2)  =3D"It's really easy to do this!"
item$(3)  =3D"Even I can do it!"
item$(4)  =3D"That says a lot..."
item$(5)  =3D"Anyway..."

Dim List lname.lst (5)

For readem.w=3D1 To 5
  If AddItem(lname())
    lname()\string =3D  item$(readem) ; add item to list
  EndIf
Next readem

;----------------------------------------------------------------

FindScreen 0 ; get frontmost screen

  *scr.Screen=3DPeek.l(Addr Screen(0)) ; get screen info...
  hite.w=3D*scr\BarHeight+1            ; ...for the title bar height

; set messages to be received from listview :
  #LISTVIEWIDCMP=3D#GADGETUP|#GADGETDOWN|#INTUITICKS|#MOUSEBUTTONS|#MOUSE=
MOVE
  AddIDCMP #LISTVIEWIDCMP

; setup gadgets...keep numbers over 50 when using listviews! OS Bug!

GTButton 0,50,0,65,200,hite+5,"Change Listview",$10

  GTTags #GTLV_ShowSelected,0   ; keep highlighted (remove to check it)
  GTListView 0,51,0,0,400,50,"Listviews? Easy!",$8,lname()

; to put it into the very top-left of a GimmeZeroZero window,
; use x=3D-WleftOff and y=3D-hite. Use 0,0 if it's a non-GimmeZeroZero
; window...

Window 0,(ScreenWidth/2)-225,(ScreenHeight/2)-60,450,120,$140e,"ListView =
Demo",1,2

AttachGTList 0,0

loop
Select WaitEvent
  Case $40
    Select GadgetHit

      Case 50 ;  gadget hit

        GTChangeList 0,51         ; we wanna change the list
        ClearList lname()         ; clear list

        Dim item$ (15)

        item$(1)  =3D"Hello, this is a NEW ListView!"
        item$(2)  =3D"It's dead easy!"
        item$(3)  =3D"Blah de blah..."
        item$(4)  =3D"Something else"
        item$(5)  =3D"Scroll down..."
        item$(6)  =3D"Nearly there I think..."
        item$(7)  =3D"Unless you're using..."
        item$(8)  =3D"A REALLY BIG FONT!"
        item$(9)  =3D"But that would be stupid..."
        item$(10) =3D"And so on..."
        item$(11) =3D"Even more stuff down here now!"
        item$(12) =3D"I'm running out of ideas..."
        item$(13) =3D"Am I nearly there yet?"
        item$(14) =3D"I think so..."
        item$(15) =3D"Ah, that's it!"

        Dim List lname.lst (15) ; new list
        For readem.w=3D1 To 15
          If AddItem(lname())
            lname()\string =3D  item$(readem) ; add item to list
          EndIf
        Next readem

        GTChangeList 0,51,lname() ; update listview

      Case 51 ; listview hit

        item$=3Dlname(EventCode)\string ; eventcode tells us which line
        Request "",item$,"OK"

    End Select

  Case $200
    End

End Select

Goto loop

End


