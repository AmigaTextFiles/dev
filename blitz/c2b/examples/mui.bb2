; Global information for every object 

NEWTYPE .MUI_GlobalInfo 

   priv0.l                               ; ULONG   
   *mgi_ApplicationObject.Object         

   ; ... private data follows ... 
End NEWTYPE   


; Instance data of notify class 

NEWTYPE .MUI_NotifyData 

   *mnd_GlobalInfo.MUI_GlobalInfo        ; STRUCT  
   mnd_UserData.l                        ; ULONG   
   mnd_ObjectID.l                        ; ULONG   
   priv1.l                               ; ULONG   
   priv2.l                               ; ULONG   
   priv3.l                               ; ULONG   
   priv4.l                               ; ULONG   
End NEWTYPE   


; MUI_MinMax structure holds information about minimum, maximum
; and default dimensions of an object. 

NEWTYPE .MUI_MinMax 

   MinWidth.w                            ; WORD    
   MinHeight.w                           ; WORD    
   MaxWidth.w                            ; WORD    
   MaxHeight.w                           ; WORD    
   DefWidth.w                            ; WORD    
   DefHeight.w                           ; WORD    
End NEWTYPE   

#MUI_MAXMAX                    = 10000 ; use this if a dimension is not limited. 


; Hook message for custom layout 

NEWTYPE .MUI_LayoutMsg 

   lm_Type.l                             ; ULONG   ; type of message (see defines below)                      
   *lm_Children.MinList                  ; STRUCT  ; list of this groups children, traverse with NextObject() 
   lm_MinMax.MUI_MinMax                  ; STRUCT  ; results for MUILM_MINMAX                                 
   NEWTYPE .struct_#?1 

      Width.l                            ; LONG    
      Height.l                           ; LONG    
      priv5.l                            ; ULONG   
      priv6.l                            ; ULONG   
   End NEWTYPE  lm_Layout  ; size (and result) for MUILM_LAYOUT                       
End NEWTYPE   

#MUILM_MINMAX                  = 1 ; MUI wants you to calc your min & max sizes 
#MUILM_LAYOUT                  = 2 ; MUI wants you to layout your children      

#MUILM_UNKNOWN                 = -1 ; return this if your hook doesn't implement lm_Type 


; (partial) instance data of area class 

NEWTYPE .MUI_AreaData 

   *mad_RenderInfo.MUI_RenderInfo        ; STRUCT  ; RenderInfo for this object 
   priv7.l                               ; ULONG   
   *mad_Font.TextFont                    ; STRUCT  ; Font 
   mad_MinMax.MUI_MinMax                 ; STRUCT  ; min/max/default sizes 
   mad_Box.IBox                          ; STRUCT  ; position and dimension 
   mad_addleft.b                         ; BYTE    ; frame & innerspacing left offset 
   mad_addtop.b                          ; BYTE    ; frame & innerspacing top offset  
   mad_subwidth.b                        ; BYTE    ; frame & innerspacing add. width  
   mad_subheight.b                       ; BYTE    ; frame & innerspacing add. height 
