--BOUNDARY.1748332912.1
Content-Disposition: attachment; filename="EvenByteWidth.asc"

Repeat
  Print "Enter pixel Width "
  width.w= Edit(5)

  ebw.w=(width/16)
  temp.q=Frac(width/16)
  If temp
    ebw+1
  EndIf
  ebw*16
  NPrint "Even byte width = ",ebw
  NPrint " "

Until width=0
End


