
; echo version
; echo ROX its ROM inbuilt on 2.x+. ;)

Statement CPrint {text$}
   qt$=Chr$(34) : exe$="echo >* "+qt$+text$+qt$+" NOLINE" : Execute_ &exe$,0,0
End Statement

Statement CNPrint {text$}
   qt$=Chr$(34) : exe$="echo >* "+qt$+text$+qt$ : Execute_ &exe$,0,0
End Statement


CNPrint {"This text will NOT re-direct"}

NPrint "This text WILL re-direct"

End





