
#W=1006 : prout$="*" : pout.l=Open_(&prout$,#W)

Statement exit {}
   SHARED pout : Close_(pout) : End
End Statement

Statement CPrint {text$}
   SHARED pout : Write_ pout, &text$, Len(text$)
End Statement

Statement CNPrint {text$}
   SHARED pout : text$=text$+Chr$(10) : Write_ pout, &text$, Len(text$)
End Statement


CNPrint {"This text will NOT re-direct"}

NPrint "This text WILL re-direct"


exit{}




