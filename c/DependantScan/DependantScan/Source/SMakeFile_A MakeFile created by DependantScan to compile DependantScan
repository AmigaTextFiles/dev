obj = Object/

/DependantScan: $(obj)ProcessDirectory.o $(obj)LocaleDpS.o $(obj)DependantScan.o $(obj)ArgumentsDpS.o\
$(obj)RequesterError.o $(obj)LocaleSupport.o
	$(LD) <WITH <
From LIB:c.o + $(obj)ProcessDirectory.o + $(obj)LocaleDpS.o + $(obj)DependantScan.o + $(obj)ArgumentsDpS.o +\
$(obj)RequesterError.o + $(obj)LocaleSupport.o

To /DependantScan
Library lib:sc.lib + lib:amiga.lib
<

$(obj)ProcessDirectory.o: ProcessDirectory.c ProcessDirectory.h
	$(CC) $*.c

$(obj)LocaleDpS.o: LocaleDpS.c DependantScan.h
	$(CC) $*.c

$(obj)DependantScan.o: DependantScan.c ProcessDirectory.h DependantScan.h RequesterError.h LocaleSupport.h
	$(CC) $*.c

$(obj)ArgumentsDpS.o: ArgumentsDpS.c DependantScan.h ProcessDirectory.h RequesterError.h LocaleSupport.h
	$(CC) $*.c

$(obj)RequesterError.o: RequesterError.c RequesterError.h LocaleSupport.h
	$(CC) $*.c

$(obj)LocaleSupport.o: LocaleSupport.c LocaleSupport.h
	$(CC) $*.c