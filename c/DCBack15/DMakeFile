
all:    makelib makereglib

makelib:
    dcc -c dcback.c -o dcback.o
    dcc -c versiontag.c -o versiontag.o
    das wbmain.a -o wbmain.o
    join dcback.o wbmain.o versiontag.o to dcbacks.lib

makereglib:
    dcc -mRR -c dcback.c -o dcbackr.o
    dcc -mRR -c versiontag.c -o versiontagr.o
    das wbmain.a -o wbmainr.o
    join dcbackr.o wbmainr.o versiontagr.o to dcbacksr.lib
