   XDEF  _asmCodeException

   XREF  _CodeException

   CSECT asmCodeException

; empiler les arguments de droite à gauche

_asmCodeException:
   move.l   a1,-(sp) ; base de TacheStd
   move.l   d0,-(sp) ; EnsSig qui a provoqué l'exception
   jsr      _CodeException ; renvoie EnsSig, pour réautoriser l'exception
   addq.l   #8,sp
   rts

   END

