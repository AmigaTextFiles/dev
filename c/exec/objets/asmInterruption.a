   XDEF  _asmPrologue

   XREF  _geta4
   XREF  __Interruption_prologue
;   XREF  _Debug

   CSECT asmInterruption

_asmPrologue:
   movem.l  d2-d7/a2-a4,-(sp) ; registres à ne pas écraser
   jsr      _geta4   ; charge l'@ du segment de donnée

   bchg     #1,$bfe001

   move.l   a1,-(sp)
   jsr      __Interruption_prologue
   addq.l   #4,sp
   movem.l  (sp)+,d2-d7/a2-a4
   moveq    #0,d0 ; continuer avec la prochaine INT ds la chaine
   rts

   END
