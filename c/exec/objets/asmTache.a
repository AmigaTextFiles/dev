   XDEF  _asmPrologueExc

   XREF  _geta4
   XREF  __Tache_prologueExc
;   XREF  _Debug

   CSECT asmTache

_asmPrologueExc:
   move.l   d0,-(sp)
   jsr      _geta4   ; charge l'@ du segment de donnée

   bchg     #1,$bfe001

   move.l   a1,-(sp)
   jsr      __Tache_prologueExc
   addq.l   #4,sp
   move.l   (sp)+,d0 ; continuer avec la prochaine exception
   rts

   END
