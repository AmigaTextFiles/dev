
;  Link string object files into Hcc_String.lib, by Jason Petty.

; copy df1:Libr ram:

ram:libr -a ram:Hcc_String.dir bzero.o lmemcpy.o memcpy.o memmove.o memset.o
ram:libr -a ram:Hcc_String.dir strchr.o strcpy.o strrchr.o atol.o bsearch.o
ram:libr -a ram:Hcc_String.dir ctlcnv.o lmemmove.o ltoa.o memccpy.o memchr.o
ram:libr -a ram:Hcc_String.dir memcmp.o memicmp.o splitpat.o stradj.o strcat.o
ram:libr -a ram:Hcc_String.dir strcmp.o strcspn.o strdup.o stricmp.o strirpl.o
ram:libr -a ram:Hcc_String.dir stristr.o strlen.o strlwr.o strncat.o strncmp.o
ram:libr -a ram:Hcc_String.dir strncpy.o strnicmp.o strpbrk.o strpcpy.o
ram:libr -a ram:Hcc_String.dir strpos.o strrev.o strrpbrk.o strrpl.o strrpos.o
ram:libr -a ram:Hcc_String.dir strset.o strspn.o strstr.o strtok.o
ram:libr -a ram:Hcc_String.dir strtol.o strtrim.o strupr.o substr.o swab.o
