    struct BitMap *bitmap;
    struct Layer_Info *layerinfo;

    if( layer )
    {
        bitmap = layer->rp->BitMap;
        layerinfo = layer->LayerInfo;
        WaitBlit();
        DeleteLayer( NULL, layer );
        FreeBitMap( bitmap );
        DisposeLayerInfo( layerinfo );
    }
}

