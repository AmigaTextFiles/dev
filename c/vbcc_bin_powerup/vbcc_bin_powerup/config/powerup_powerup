-elf
-no-regnames
-no-multiple-ccs
-madd
-no-align-args
-use-commons
-cc=vbccppc_pup -quiet %s -o= %s %s -O=%ld -Ivincludepup:
-ccv=vbccppc_pup %s -o= %s %s -O=%ld -Ivincludepup:
-as=vasmppc_std_pup -quiet -Felf -opt-branch -no-regnames -nowarn=62 %s -o %s
-asv=vasmppc_std_pup -Felf -opt-branch -no-regnames -nowarn=62 %s -o %s
-isc=vscppc_pup -quiet %s %s
-iscv=vscppc_pup %s %s
-rm=delete quiet %s
-rmv=delete %s
-ld=vlink_pup -belf32powerup -Cvbccelf -nostdlib vlibpup:startup.o %s %s -Lvlibpup: -lvc -lpowerup -o %s
-l2=vlink_pup -belf32powerup -Cvbccelf -nostdlib %s %s -Lvlibpup: -o %s
-ldv=vlink_pup -belf32powerup -t -Cvbccelf -nostdlib vlibpup:startup.o %s %s -Lvlibpup: -lvc -lpowerup -o %s
-l2v=vlink_pup -belf32powerup -t -Cvbccelf -nostdlib %s %s -Lvlibpup: -o %s
-ldnodb=-s -x
-ul=-l%s
-cf=-F%s
-ml=500
