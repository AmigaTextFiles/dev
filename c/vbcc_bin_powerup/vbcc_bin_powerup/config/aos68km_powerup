-l2=vlink_pup -bamigahunk -x -Bstatic -Cvbcc -nostdlib -mrel %s %s -Lvlibos3: -o %s
-ldv=vlink_pup -bamigahunk -t -x -Bstatic -Cvbcc -nostdlib -mrel vlibos3:minstart.o %s %s -Lvlibos3: -lvc -o %s
-l2v=vlink_pup -bamigahunk -t -x -Bstatic -Cvbcc -nostdlib -mrel %s %s -Lvlibos3: -o %s
