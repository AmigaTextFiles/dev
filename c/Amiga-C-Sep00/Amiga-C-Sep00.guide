    return 1L
}

// Dispatcher Class
SAVEDS ULONG Time_Dispatcher(REG(a0)struct IClass *Class, REG(a2) Object
*obj, REG(a1) Msg msg)
{
    switch(msg->MethodID)
    {
        case OM_NEW:
            return Time_New(Class,obj,(APTR)msg);
        case OM_DISPOSE:
            return Time_Dispose(Class,obj,(APTR)msg);
        case MUIM_Time_Trigger:
            return Time_Trigger(Class,obj,(APTR)msg);
        case MUIM_Setup:
            return Time_Setup(Class,obj,(APTR)msg);
