    if (!CheckIO( ((struct IORequest *)Data->request) ))
        AbortIO( ((struct IORequest *)Data->request) );
    WaitIO( ((struct IORequest *)Data->request) );

    return (DoSuperMethodA(Class,obj,msg));
}

ULONG Time_Trigger(struct IClass *Class, Object *obj, Msg msg)
{
