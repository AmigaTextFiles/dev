#!Sc:C/SMake
#
#   Simple Makefile to create Complex Numbers Link Library in C
#                       for SAS/C
#
#   Makefile by Nick d'Alterio
#
#   $VER: SMakefile 1.1 (10.07.95) $
#   $Id: SMakefile 1.1 1995/07/10 18:37:10 daltern Exp daltern $
#

OBJS    = ComAdd.o \
          ComSub.o \
          ComMul.o \
          ComDiv.o \
          ComArg.o \
          ComMod.o

LIBRARY = complex.lib

CC      = sc
CFLAGS  = 

all: $(OBJS) $(LIBRARY)

.c.o:
	$(CC) $(CFLAGS) $< OBJNAME=$@

ComAdd.o: ComAdd.c
ComSub.o: ComSub.c
ComMul.o: ComMul.c
ComDiv.o: ComDiv.c
ComMod.o: ComMod.c
ComArg.o: ComArg.c

$(LIBRARY): $(OBJS)
	$(CC) $(OBJS) OBJLIB=$(LIBRARY)
