CC= SC

SUFFIX=

CFLAGS=

# see SCOPTIONS for more flags

LDFLAGS= SC SD ND BATCH

LDLIBS= LIB:sc.lib LIB:scmieee.lib

LN= slink
RM= delete quiet
AR= oml

LIBOBJECTS= _snprintf.o _snprintf_noieee.o _snprintf_ieee.o _snprintf_unsafe.o

all: libsnp.lib

libsnp.lib: $(LIBOBJECTS)
        -$(RM) libsnp.lib
        $(AR) libsnp.lib r $(LIBOBJECTS)

clean:
        -$(RM) libsnp.lib
