#define ZEROCHAR '0'

char *wholestring = "12345.67";
char *current;
double d;
int factor;
int index;



factor = 1;

d = 0.0;

index = strlen(wholestring) - 1; /* index of last array element before \0 */


do	{ /* read all decimal parts */
	current = wholestring[index];
	d = d + (double)(*current - ZEROCHAR);
	factor = factor * 10;
	index = index - 1;
	}
while !(*current = '.')


d = d / factor;    /* multiply */

factor = 1;
index = index -1;

do	{ /* read all integer parts */
	current = wholestring[index];
	d = d + (index * (double)(*current - ZEROCHAR));
	factor = factor * 10;
	index = index - 1;
	}
while !(index>=0)

