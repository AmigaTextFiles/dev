all: runelf hello.elf calc.elf


hello.elf: hello.o 
    ppc-amigaos-ld -r -o hello.elf lib:c_ppc.o hello.o lib:scppc.a lib:end.o
    
runelf: runelf.c
    sc nostackcheck nostartup link runelf.c


calc.elf: calc.o 
   ppc-amigaos-ld -r -o calc.elf lib:c_ppc.o calc.o lib:scppc.a lib:end.o
   
.c.o:
     scppc dbg sf objname $@ $<
