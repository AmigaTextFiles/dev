
# SMake file for IconifyButtonClass and test program
# requires BCC 3.5 and SASC 6.5x to compile happily.
# (c) 1998 by Christian Buchner


# Executable name, Objects, and compiler options

PROG= test
OBJS= test.o initcl.o IconifyButtonClass.o
COPTS= PARMS=REGISTERS NOSTKCHK STRMER COMNEST UTILLIB


# Default rules

.bc.c:
	BCC $*.bc

.c.o:
	SC $*.c $(COPTS)

.asm.o:
	GenAm $*.asm


# Dependencies

$(PROG): $(OBJS)
	slink FROM $(OBJS) TO $(PROG) LIB LIB:sc.lib+LIB:amiga.lib+LIB:bcc.lib SC SD NOICONS

IconifyButtonClass.o: IconifyButtonClass.c
IconifyButtonClass.c: IconifyButtonClass.bc IconifyButtonClass.bh

test.o: test.c  IconifyButtonClass.h
test.c: test.bc IconifyButtonClass.bh
