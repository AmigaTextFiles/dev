-elf
-no-regnames
-no-multiple-ccs
-madd
-no-align-args
-use-commons
-cc=vbccppc -quiet %s -o= %s %s -O=%ld -Ivincludepup:
-ccv=vbccppc %s -o= %s %s -O=%ld -Ivincludepup:
-as=vasmppc_std -quiet -Felf -opt-branch -no-regnames -nowarn=62 %s -o %s
-asv=vasmppc_std -Felf -opt-branch -no-regnames -nowarn=62 %s -o %s
-isc=vscppc -quiet %s %s
-iscv=vscppc %s %s
-rm=delete quiet %s
-rmv=delete %s
-ld=vlink -belf32powerup -Cvbccelf -nostdlib vlibpup:startup.o %s %s -Lvlibpup: -lvc -lpowerup -o %s
-l2=vlink -belf32powerup -Cvbccelf -nostdlib %s %s -Lvlibpup: -o %s
-ldv=vlink -belf32powerup -t -Cvbccelf -nostdlib vlibpup:startup.o %s %s -Lvlibpup: -lvc -lpowerup -o %s
-l2v=vlink -belf32powerup -t -Cvbccelf -nostdlib %s %s -Lvlibpup: -o %s
-ldnodb=-s -x
-ul=-l%s
-cf=-F%s
-ml=500
