CFLAGS= opt math=68881
LDFLAGS= lib lib:gap.lib

.c.o:
	sc $(CFLAGS) $*.c

all: FuncOpt1 FuncOpt2 tutorial

clean:
	@delete \#\?\.\(lnk\|o\|info\|bak) FuncOpt1 FunOpt2 tutorial

FuncOpt1: FuncOpt1.o
	sc $(LDFLAGS) $*.o link

FuncOpt2: FuncOpt2.o
	sc $(LDFLAGS) $*.o link

tutorial: tutorial.o
	sc $(LDFLAGS) $*.o link
