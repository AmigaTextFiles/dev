#
# GAP-Lib (C)1998-1999 Peter Bengtsson
#
# Amiga SAS/C makefile
#

OBJS= Crossover.o Evolve.o Testbit.o Flip.o CreatePopulation.o Random.o \
      EnterGAP.o DeletePopulation.o Magic.o PopMember.o IRange.o HammingDist.o \
      DefaultFunctions.o Misc.o Filters.o

lib: $(OBJS)
	OML /lib/GAP.lib R $(OBJS)

all: veryclean
	@smake lib

clean:
	@delete \#\?\.\(info\|o\|bak\|lnk\)
	@delete mkver increv VString

veryclean: clean
	@delete /lib/GAP.lib

bump: increv
	increv

VString: mkver GAP_Version GAP_Revision
	mkver c \>VString

increv: increv.c
	sc increv.c link

mkver: mkver.c
	sc mkver.c link

Crossover.o: Crossover.c GAPLocal.h

Magic.o: Magic.c GAPLocal.h

Evolve.o: Evolve.c GAPLocal.h

IRange.o: IRange.c

Testbit.o: Testbit.c

Flip.o: Flip.c

CreatePopulation.o: CreatePopulation.c GAPLocal.h

PopMember.o: PopMember.c GAPLocal.h

Random.o: Random.c

EnterGAP.o: EnterGAP.c VString mkver

DeletePopulation.o: DeletePopulation.c

HammingDist.o: HammingDist.c

DefaultFunctions.o: DefaultFunctions.c

Misc.o: Misc.c GAPLocal.h

Filters.o: Filters.c GAPLocal.h

