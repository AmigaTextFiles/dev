PhoneLogScanner.o: PhoneLogScanner.rex StringMem.h Idents.h Positions.h General.h DynArray.h
	rex -cdnis PhoneLogScanner.rex
	sc PhoneLogScanner.c PhoneLogScannerSource.c
	delete >NIL: PhoneLogScannerDrv.c

PhoneLogParser.o: PhoneLogParser.lalr PhoneLog.h PhoneLogParserInterface.h Sets.h Memory.h System.h Idents.h DynArray.h
	lalr -c -d -b -v -e PhoneLogParser.lalr
	sc def BCOPY PhoneLogParser.c Errors.c
	-delete >NIL: _Debug
