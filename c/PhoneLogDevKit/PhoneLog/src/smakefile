PhoneLog: PhoneLogGenerator.o  PhoneLogParserInterface.o PhoneLogParser.o PhoneLogScanner.o

PhoneLogGenerator.o: PhoneLogGenerator.c PhoneLogGenerator.h PhoneLog.h
	sc PhoneLogGenerator.c

PhoneLogParserInterface.o: PhoneLogParserInterface.c PhoneLogParserInterface.h PhoneLog.h PhoneLogScanner.o PhoneLogParser.o
	sc PhoneLogParserInterface.c

