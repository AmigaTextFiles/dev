all: fft.elf pulse.elf sine.elf

fft.elf: ufft.o

         ppc-amigaos-ld -r LIB:c_ppc.o ufft.o hlp:ppc/libm/libm.a LIB:scppc.a lib:end.o -o fft.elf
         Protect fft.elf +e

pulse.elf: upulse.o

         ppc-amigaos-ld -r LIB:c_ppc.o upulse.o hlp:ppc/libm/libm.a LIB:scppc.a lib:end.o -o pulse.elf
         Protect pulse.elf +e

sine.elf: usine.o

         ppc-amigaos-ld -r LIB:c_ppc.o usine.o hlp:ppc/libm/libm.a LIB:scppc.a lib:end.o -o sine.elf
         Protect sine.elf +e

ufft.o: ufft.c

        SCPPC ufft.c

upulse.o: upulse.c

        SCPPC upulse.c

usine.o: usine.c

        SCPPC usine.c
