OPTIONS RESULTS

Main:
	SETCLIP("CEDNEW", "NO")

	ADDRESS "rexx_ced"

	'STATUS' 18
	if result > 0 then
		'SAVE'

	'STATUS' 21; WorkFile = result
	'STATUS' 20; WorkDir = result

	ADDRESS "COMMAND"

	if ~SHOW("PORTS", "SC_SCMSG") then
		'Run >NIL: <NIL: SC:C/scmsg'

	'C:Execute S:Compile.ced "'WorkDir'" "'WorkFile'"'

	'SYS:RexxC/rx REXX:NextError.ced'
return
