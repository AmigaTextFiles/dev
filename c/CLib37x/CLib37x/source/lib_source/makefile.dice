CC  = dcc
CFLAGS   = -3.0 -I/Include/ -c -// -no-env
LDFLAGS  = -l0
LDLIBS  =

OBJS    = StartUp.o LibInit.o SampleFuncs.o

example.library: $(OBJS)
    $(CC) $(LDFLAGS) -o %(left) $(OBJS) $(LDLIBS)

StartUp.o: StartUp.c
    $(CC) $(CFLAGS) %(right)

LibInit.o: LibInit.c
    $(CC) $(CFLAGS) %(right)

SampleFuncs.o: SampleFuncs.c
    $(CC) $(CFLAGS) %(right)
