CC	= vc
CFLAGS   = -O2 -+ -sc -DVBCC -c
LDFLAGS  = -nostdlib -lamiga
LDLIBS	=

OBJS	= StartUp.o LibInit.o SampleFuncs.o

example.library: $(OBJS)
	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(LDLIBS)

StartUp.o: StartUp.c
	$(CC) $(CFLAGS) StartUp.c

LibInit.o: LibInit.c
	$(CC) $(CFLAGS) LibInit.c

SampleFuncs.o: SampleFuncs.c
	$(CC) $(CFLAGS) SampleFuncs.c
