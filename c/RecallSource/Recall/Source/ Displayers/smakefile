# -------------------------------------
# SAS-C Makefile for the Recall modules
# -------------------------------------

MODULE		=Displayer
MODULEDIR	=Displayers

all: Browser

Browser: Browser.c
	@echo "*e[32mCompiling $* $(MODULE) module...*e[0m"
	@sc $*.c
	@revco Browser REVISION
 	@move Browser rec:/$(MODULEDIR)/Browser
