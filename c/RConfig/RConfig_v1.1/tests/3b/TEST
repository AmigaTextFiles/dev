; RConfig Test Script #3b

if not exists rlib:
    echo "RConfig Library Source Not Found"
    quit
endif

; Generate Replacement Library
cc RLIB:rlib.c -iRLIB: -oT:rlib.o -d__STKCHK_REPLACE -dSTKCHK_MIN_STACK=2768 -d__DYNASTACK_STKCHK -dSTKCHK_STACK_SIZE=8192 -dSTKCHK_CONTEXT_SIZE=128 -m0d -pl -sa -s0b -sf -sm -ss -sp -sn -r04 -r06 -sr
lb rlib.lib T:rlib.o

; Make Test Program
cc -bs -bd -at test3b.c
stkchker test3b.asm
as test3b.asm
ln test3b.o rlib.lib -lc

; Set-up Test Environment
stack 4000
test3b
