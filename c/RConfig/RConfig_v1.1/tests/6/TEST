; RConfig Test Script #6

if not exists rlib:
    echo "RConfig Library Source Not Found"
    quit
endif

; Generate Replacement Library
;cc RLIB:rlib.c -iRLIB: -oT:rlib.o -d__ALLOCA_REPLACE -d__SAFE_ALLOCA -d__SETJMP_REPLACE -m0d -pl -sa -s0b -sf -sm -ss -sp -sn -r04 -r06 -sr
;lb rlib.lib T:rlib.o

; Make Test Program
cc test6.c
ln test6.o rlib.lib -lc

; Set-up Test Environment
test6
