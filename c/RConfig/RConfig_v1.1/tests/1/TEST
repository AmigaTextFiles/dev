; RConfig Test Script #1

if not exists rlib:
    echo "RConfig Library Source Not Found"
    quit
endif

; Generate Replacement Library
cc RLIB:rlib.c -oT:rlib.o -m0d -pl -sa -s0b -sf -sm -ss -sp -sn -r04 -r06 -sr
as RLIB:crt0.a68 -o T:crt0.o
cc RLIB:_main.c -oT:_main.o -d__DETACH_MAIN -m0b -m0d -pl -sa -s0b -sf -sm -ss -sp -sn -r04 -r06 -sr
lb rlib.lib T:crt0.o T:_main.o T:rlib.o

; Make Test Program
cc test1.c -otest1.o
ln test1.o rlib.lib -lc

; Set-up Test Environment
test1
