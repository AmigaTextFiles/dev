# /bin/sh

echo "  /* -*- odump by Harry \"Piru\" Sintonen */"
echo "  static const ULONG $1[]={"

objdump -s $1.o |
  gawk '
    BEGIN {
      FS="  "
    }
    {
      print $1
    }' |
  gawk '
    / [0-9a-f]+ [0-9a-f]+/ {
      printf "    0x%08s,0x%08s,0x%08s,0x%08s,\n",$2,$3,$4,$5
    }'

echo "    0};"
echo "  /* -*- */"
