
LINKER=slink
LIBRARIES=xd.lib lib:sc.lib lib:amiga.lib
OBJECTS=xd.o
CFLAGS=GST=gst
LINKERFLAGS=

all:	xd.lib xdtest xd.h

gst:
	sc MGST=gst xd.h
	
xd.lib:	xd.o xd.h
	echo "" >xd.lib
	oml -n xd.lib a xd.o 

xdtest:	xdtest.o xd.lib xd.h
	$(LINKER) $(LINKERFLAGS)\
		FROM LIB:c.o xdtest.o $(OBJECTS)\
		LIB $(LIBRARIES)\
		TO xdtest

xd.o:	xd.c xd.h
	sc $(CFLAGS) xd.c

xdtest.o:	xdtest.c xd.h
	sc $(CFLAGS) xdtest.c



