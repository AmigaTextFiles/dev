OFILES  = main.o parse.o pr.o cppsetup.o ifparser.o include.o

all: makedepend

makedepend: $(OFILES)
    slink addsym from LIB:c.o $(OFILES) to makedepend LIB LIB:sc.lib LIB:small.lib \
    SC SD NOICONS STRIPDEBUG

depend:
    makedepend -A -Y $(OFILES)

install:
    copy makedepend WinTrans:TestBin/

clean:
    -delete \#?.o
    -delete makedepend


# DO NOT DELETE THIS LINE makedepend needs it

main.o: main.c def.h imakemdep.h
parse.o: parse.c def.h
pr.o: pr.c def.h
cppsetup.o: cppsetup.c def.h ifparser.h
ifparser.o: ifparser.c ifparser.h
include.o: include.c def.h
