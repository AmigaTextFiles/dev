#asm
    xref    _MUIMasterBase
    public    _MUI_MakeObjectA
_MUI_MakeObjectA:
    MOVE.L    _MUIMasterBase,a6
    MOVE.L    4(sp),d0
    LEA    8(sp),a0
    JSR    -$78(A6)
    RTS
#endasm

