TESTPRGS       =    apphome:TESTPRGS/
APPINCLUDE     =    appinclude:
LIBFILE        =    apphome:SASC++/APPLibrary/aplusplus.lib




.cxx.o:
   sc $*.cxx includedirectory=$(APPINCLUDE) objectname=/SASC++/

all: \
 TypeInfo_test\
