 PriorityList.o\
 RawKeyDecoder.o\
 RectObject.o\
 RowColumnGroup.o\
 ScreenC.o\
 SignalResponder.o\
 StdGadget.o\
 TextView.o\
 TimedMsgPort.o\
 TimerC.o\
 TypeInfo.o\
 WindowCV.o\


$(LIBRARYFILE): $(OBJS)
   oml $(LIBRARYFILE) R $?


.cxx.o:
   sc $*.cxx includedirectory=$(APPINCLUDE) objectname=/APPLibrary/


