#  Makefile for compiling and making the trace.lib under Manx.

CC = cc
LD = ln

CFLAGS   	=  +L
DBFLAGS		= -dDEBUG

LIBS     	=  -lm32 -lc32
LIB			=	trace.lib
TEST			=	test

all:     $(LIB) $(TEST)

trace.lib: trace.o
	lb trace.lib  trace.o

trace.o: trace.c debug.h trace.h
	$(CC) $(CFLAGS) trace.c

test: test.o trace.lib
	$(LD) test.o -o test $(LIB) $(LIBS)

test.o: test.c
	$(CC) $(CFLAGS) $(DBFLAGS) test.c
