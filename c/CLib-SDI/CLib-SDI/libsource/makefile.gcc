CFLAGS = -O2 -c -I ../include/C/ -D __NOLIBBASE__
LFLAGS = -noixemul -nostartfiles -s

DDIR = ../libs/# the directory, where destination library is stored

$(DDIR)example.library: libinitgcc.o examplefuncsgcc.o
	gcc $(LFLAGS) -o $@ libinitgcc.o examplefuncsgcc.o
	
libinitgcc.o: libinfo.h libinit.c
	gcc $(CFLAGS) -o $@ libinit.c

examplefuncsgcc.o: libinfo.h examplefuncs.c
	gcc $(CFLAGS) -o $@ examplefuncs.c

clean:
	delete examplefuncsgcc.o libinitgcc.o
