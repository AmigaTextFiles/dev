# Smakefile for SmakeOpts.c

LD=sc link # prefer this over slink
CP=copy
DEBUG=

all: SMakeOpts

SMakeOpts: SmakeOpts.o stringpatch.o
    $(LD) $< $(DEBUG) stringpatch.o lib=lib:bgui.lib

# stringpatch.c/strinpatch.h included in the bgb directory
stringpatch.o: sc:source/stringpatch.o
    $(CP) sc:source/stringpatch.o ""

# Use this for compiling stringpatch
#
#stringpatch.o: stringpatch.c
#   $(CC) stringpatch.c

.c.o:
    $(CC) $(CFLAGS) $(DEBUG) $<
