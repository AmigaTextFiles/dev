failat 20
echo ""
echo "Amiga Oberon 3.0 Demo Entpacker:"
echo ""
echo ""
ask "Bitte Diskette 1 in Laufwerk DF0: einlegen und <RETURN> drücken!"
echo ""
format drive df0: name OberonDemo_3.0_Disk1 noicons
assign Oberon3.0Gepackt: ""
cd OberonDemo_3.0_Disk1:
echo ""
echo "Entpacke Disk 1:"
echo ""
Oberon3.0Gepackt:lhunarc >NIL: Oberon3.0Gepackt:O3Demo1
protect ExeIcon +p
protect Oberon +p
protect ODebug +p
protect OEd +p
protect OErr +p
protect OLink +p
protect ResidentManager +p
protect Install +s
echo ""
echo "In DF0: liegt entpackte Diskette 1!"
echo ""
echo ""
ask "Bitte Diskette 2 in Laufwerk DF0: einlegen und <RETURN> drücken!"
echo ""
format drive df0: name OberonDemo_3.0_Disk2 noicons
cd OberonDemo_3.0_Disk2:
echo ""
echo "Entpacke Disk 2:"
echo ""
Oberon3.0Gepackt:lhunarc >NIL: Oberon3.0Gepackt:O3Demo2
assign Oberon3.0Gepackt:
echo ""
echo "In DF0: liegt entpackte Diskette 2!"
echo ""
echo ""
ask "<RETURN>"
