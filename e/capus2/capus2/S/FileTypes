##   Syntax
##   TYPE "Src Ada"
##       SUBTYPE "Text"
##       INSERTAFTER "Script"
##       ICONNAME "def_Src Ada"  # path internaly expanded to "ENV:Sys/def_Src Ada.info"
##       NAMEPATTERN *.ada
##   # or OPTNAMEPATTERN *.ada
##   # The 4 upper lines are optional but order MUST be kept.
##   # Lower lines are for information about syntax, and if used order MUST be kept.
##       # COMPAREBYTE OFFSET BYTES
##       COMPAREBYTE 12 $ABADCAFE    # hex bytes, offset decimal
##       COMPAREBYTE $23 "Coucou"    # string bytes, offset in hex
##       SEARCHBYTE  "Salut"
##       SEARCHBYTE  $DEADBEEF
##       SEARCHPATTERN [CASE] "ST-??"
##       MATCHPATTERN [CASE] 45 "ST-??"
##   ENDTYPE
########################################################
TYPE "Source E"
#> Source AmigaE
    SUBTYPE "Text"
    ICONNAME "def_E"
    NAMEPATTERN "#?.e"
ENDTYPE

TYPE "Include E"
#> Include AmigaE
    ICONNAME "def_ince"
    NAMEPATTERN "#?.m"
    COMPAREBYTE 0 "EMOD"
ENDTYPE

TYPE "Source C"
#> Source C
    SUBTYPE "Text"
    ICONNAME "def_C"
    NAMEPATTERN "#?.c"
ENDTYPE

TYPE "Include C"
#> Include C
    SUBTYPE "Text"
    INSERTAFTER "Source C"
    ICONNAME "def_H"
    NAMEPATTERN "#?.h"
ENDTYPE

TYPE "Asm"
#> Source Assembleur
    SUBTYPE "Text"
    ICONNAME "def_Asm"
    NAMEPATTERN "#?.(asm|s|a)"
ENDTYPE

TYPE "Incld Asm"
#> 
    SUBTYPE "Text"
    ICONNAME "def_I"
    NAMEPATTERN "#?.i"
ENDTYPE

TYPE "Doc"
#> 
    SUBTYPE "Text"
    ICONNAME "def_doc"
    NAMEPATTERN "#?.Doc#?"
ENDTYPE

TYPE "PP Doc"
#> 
    SUBTYPE "PP Data"
    ICONNAME "def_ppdoc"
    NAMEPATTERN "#?.(Doc#?.pp|Doc)"
ENDTYPE

TYPE "ReadMe"
#> 
    SUBTYPE "Text"
    INSERTAFTER "Doc"
    ICONNAME "def_doc"
    NAMEPATTERN "Read#?Me#?"
ENDTYPE

TYPE "Guide"
#> 
    SUBTYPE "Text"
    INSERTAFTER "ReadMe"
    ICONNAME "def_Guide"
    NAMEPATTERN "#?.(guide|doc)"
    COMPAREBYTE 0 "@database"
ENDTYPE

TYPE "Tex"
#> 
    ICONNAME "def_Tex"
    NAMEPATTERN "#?.Tex"
ENDTYPE

TYPE "Rexx"
#> Script Arexx
    SUBTYPE "Text"
    ICONNAME "def_Rexx"
    NAMEPATTERN "#?.(rexx|ced|ged|vert)"
ENDTYPE

TYPE "Basic"
#> 
    NAMEPATTERN "#?.bas"
ENDTYPE

TYPE "GFA"
#> 
    NAMEPATTERN "#?.GFA"
ENDTYPE

TYPE "Tex Font"
#> 
    COMPAREBYTE 0 $F7593647
ENDTYPE

TYPE "Tex DVI"
#> 
    OPTNAMEPATTERN "#?.dvi"
    COMPAREBYTE 0 $F7020183
ENDTYPE

TYPE "Warp"
#> 
    OPTNAMEPATTERN "#?.wrp"
    COMPAREBYTE 0 "Warp"
ENDTYPE

TYPE "Zoom"
#> 
    OPTNAMEPATTERN "#?.zoom"
    COMPAREBYTE 0 "ZOOM"
ENDTYPE

TYPE "ZIP"
#> 
    OPTNAMEPATTERN "#?.zip"
    COMPAREBYTE 0 $504B0304
ENDTYPE

TYPE "GST"
#> 
    OPTNAMEPATTERN "#?.gst"
    COMPAREBYTE 6 "GST"
ENDTYPE

TYPE "PPM V1.8"
#> 
    COMPAREBYTE 0 $07231988
ENDTYPE

TYPE "PPM V2.1"
#> 
    COMPAREBYTE 0 $07231988
ENDTYPE

TYPE "STSong"
#> 
    COMPAREBYTE 0 "PACK"
ENDTYPE

TYPE "ST Mod"
#> 
    NAMEPATTERN "#?(#?.mod|mod.#?)"
ENDTYPE

TYPE "Exe Lha"
#> Archive lha Auto-décompactante
    SUBTYPE "Exe"
    ICONNAME "def_lha"
    NAMEPATTERN "#?.run"
    COMPAREBYTE 0 $000003f3
    COMPAREBYTE $2c "SFX"
ENDTYPE

TYPE "Exe Lib"
#> Libraries système
    ICONNAME "def_library"
    NAMEPATTERN "#?.library"
ENDTYPE

TYPE "ILBM8"
#> Image Ilbm 8 bitplanes
    SUBTYPE "IFF"
    ICONNAME "def_ham8"
    NAMEPATTERN "#?.ham8"
    COMPAREBYTE 0 "FORM"
    COMPAREBYTE 8 "ILBM"
    COMPAREBYTE 28 "$08"
ENDTYPE

TYPE "JPEG"
#> Images Jpeg
    ICONNAME "def_jpeg"
    OPTNAMEPATTERN "#?.(jpeg|jpg)"
    COMPAREBYTE 0 $FFD8FFE0
ENDTYPE

TYPE "PCX"
#> Image Pcx
    ICONNAME "def_pcx"
    NAMEPATTERN "#?.pcx"
    COMPAREBYTE 0 $0a050108
ENDTYPE

TYPE "TGA"
#> Image TGA
    ICONNAME "def_tga"
    OPTNAMEPATTERN "#?.tga"
    COMPAREBYTE 0 $00000200
    COMPAREBYTE 16 $1820
ENDTYPE

TYPE "GIF"
#> Image Gif
    ICONNAME "def_gif"
    COMPAREBYTE 0 "GIF8"
ENDTYPE

TYPE "BMP"
#> Image BMP
    ICONNAME "def_bmp"
    COMPAREBYTE 0 "BM"
ENDTYPE

TYPE "Catalog"
#> Catalogue
    SUBTYPE "IFF"
    ICONNAME "def_catalog"
    NAMEPATTERN "#?.catalog"
    COMPAREBYTE 0 "FORM"
    COMPAREBYTE 8 "CTLG"
ENDTYPE

TYPE "Obj3D"
#> Objets 3D
    NAMEPATTERN "#?.(3dpro|pro|3D2|3D|comp|img)"
ENDTYPE

TYPE "Pov"
#> Script Pov
    SUBTYPE "Text"
    ICONNAME "def_pov"
    NAMEPATTERN "#?.pov"
ENDTYPE

TYPE "Vertex_o"
#> Objets 3D Vertex < v1.62
    SUBTYPE "Obj3D"
    ICONNAME "def_3d_vertex(old)"
    COMPAREBYTE 0 $56453344
ENDTYPE

TYPE "Vertex_n"
#> Objets 3D Vertex > 1.73
    SUBTYPE "Obj3D"
    ICONNAME "def_3d_vertex(new)"
    COMPAREBYTE 0 $56523344
ENDTYPE

TYPE "3Dpro"
#> Objets 3D 3Dpro
    SUBTYPE "Obj3D"
    ICONNAME "def_3d_3dpro"
    OPTNAMEPATTERN "#?.(pro|3dpro)"
    COMPAREBYTE 0 $435333444F4231
    SEARCHBYTE "CSOBJPALLETE1"
ENDTYPE

TYPE "3D2"
#> Objets 3D Cyber v2.0 (Atari)
    SUBTYPE "Obj3D"
    ICONNAME "def_3d_3d2"
    COMPAREBYTE 0 $3D02
ENDTYPE

TYPE "3D"
#> Objets 3D Cyber v1.0 (Atari)
    SUBTYPE "Obj3D"
    ICONNAME "def_3d_3d"
    NAMEPATTERN "#?.3D"
    COMPAREBYTE 0 $3D3D
ENDTYPE

