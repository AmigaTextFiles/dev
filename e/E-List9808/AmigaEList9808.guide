Frank Weber          E-Mail: frank@cc86.org
74076 Heilbronn
PGP key available on request

Amiga Shareware:    CDCat     AFiloFaxPro     AmigaTrainer     Cheater

CDCat Support site: http://www.amigaworld.com/support/cdcat

aMIGA rULZ :-)

- --BOUNDARY.1463444384.1
Content-Type: text/plain; charset=iso-8859-1; name="Muitest2.e"
Content-Disposition: attachment; filename="Muitest2.e"
Content-Transfer-Encoding: binary

OPT PREPROCESS

MODULE 'muimaster','libraries/mui','utility/tagitem','tools/boopsi'

DEF window,app,maingroup,str,butgroup,okbutton,cancelbutton,comstr,helpstr

ENUM NOMUI,ERRMUIOBJ,NOWINDOW,NOAPP

PROC main() HANDLE
  DEF sig,running=TRUE,res
  IF muimasterbase:=OpenLibrary('muimaster.library',11)
    createmuigroups()
    setupmethods()
    set(window,[MUIA_Window_Open,MUI_TRUE])

    WHILE running<>0
      res:=domethod(app,[MUIM_Application_Input,{sig}])
      SELECT res
      CASE -1
        running:=FALSE
      ENDSELECT
      IF running AND sig THEN Wait(sig)
    ENDWHILE
    set(window,[MUIA_Window_Open,FALSE])
    disposemuigroups()
    CloseLibrary(muimasterbase)
  ELSE
    Raise(NOMUI)
  ENDIF
EXCEPT
  disposemuigroups()
  WriteF('An error occured\nNr: \d\n\d',exception,butgroup)
  IF muimasterbase THEN CloseLibrary(muimasterbase)
ENDPROC

PROC createmuigroups()
  IF (app:=ApplicationObject,
          MUIA_Application_Base,'MUITEST',
          MUIA_Application_Author,'Frank',
          MUIA_Application_Title,'Muitest v1.0',
          MUIA_Application_Window,
          window:=WindowObject,
                    MUIA_Window_Title,'Testwin',
                    MUIA_Window_ID,"TEST",
                    MUIA_Window_RootObject,
                    VGroup, Child, str:=KeyString('Press close gadget to
quit', 100, "t"),
                            Child, HGroup,
                                     Child, okbutton:=KeyButton('OK',"o"),
                                     Child,
cancelbutton:=KeyButton('Cancel',"c"),
                                   End,
                    End,
                  End,
       End)=NIL THEN Raise(NOAPP)
ENDPROC

PROC setupmethods()
  domethod(window,[MUIM_Notify,MUIA_Window_CloseRequest,1,
                  app,2,MUIM_Application_ReturnID,-1])

ENDPROC

PROC disposemuigroups()
  IF app THEN Mui_DisposeObject(app)
ENDPROC

- --BOUNDARY.1463444384.1--

@endnode

