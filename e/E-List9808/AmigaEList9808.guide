OPT OSVERSION=3D36

MODULE ...'input','exec/io'...
..
DEF mp=3DNIL,io=3DNIL:PTR TO iostd,error=3D-1
..
mp:=3DCreateMsgPort()
io:=3DCreateIORequest(mp,SIZEOF iostd)
error:=3DOpenDevice('input.device',0,io,0)
inputbase:=3Dio.device
/* can call PeekQualifier() now */
..
IF error=3D0 THEN CloseDevice(io)
IF io THEN DeleteIORequest(io)
IF mp THEN DeleteMsgPort(mp)

