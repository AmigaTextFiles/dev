ENDOBJECT

OBJECT b
  b :[16]:ARRAY OF CHAR
ENDOBJECT

OBJECT xyz
  a[16]:ARRAY OF a
  b[16]:ARRAY OF b
  c[16]:ARRAY OF CHAR
