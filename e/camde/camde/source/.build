fd=                                     fd2module
ec=                                     ec quiet ignorecache
ic=                                     iconvert -p
mv=                                     rename quiet

start: /emodules/camd.m /emodules/midi/camd.m /emodules/midi/camdbase.m /emodules/midi/camddevices.m /emodules/midi/mididefs.m /emodules/midi/midiprefs.m

/emodules/camd.m:                        camd.fd
	$(fd) camd.fd
	$(mv) camd.m /emodules/

/emodules/midi/camd.m:                   camd.i
	$(ic) camd.i
	$(mv) camd.m /emodules/midi/

/emodules/midi/camdbase.m:               camdbase.i
	$(ic) camdbase.i
	$(mv) camdbase.m /emodules/midi/

/emodules/midi/camddevices.m:            camddevices.i
	$(ic) camddevices.i
	$(mv) camddevices.m /emodules/midi/

/emodules/midi/mididefs.m:               mididefs.i
	$(ic) mididefs.i
	$(mv) mididefs.m /emodules/midi/

/emodules/midi/midiprefs.m:              midiprefs.e
    $(ec) midiprefs.e
	$(mv) midiprefs.m /emodules/midi/
