INSOMNIA_E_SOURCE_EDITOR_TEMPLATE
PROC mHandleEvent(cl:PTR TO iclass, obj:PTR TO object, msg:PTR TO muip_handleevent)
   DEF muikey, imsg, class, code, data:PTR TO mydata
   
   data := INST_DATA(cl, obj)
   
   IF (muikey := msg.muikey)
      SELECT muikey
      ENDSELECT   
   ENDIF
   
   
   IF (imsg := msg.imsg)
      class := imsg.class
      code := imsg.code
      SELECT class
      ENDSELECT
   ENDIF

   
ENDPROC NIL




