OPT MODULE, PREPROCESS
OPT EXPORT

CONST MAXUSERS=100,
      MAXUTLEN=256

OBJECT utmp
  line:PTR TO CHAR
  name:PTR TO CHAR
  host:PTR TO CHAR
  time
ENDOBJECT

OBJECT utmpidle
  utmp:utmp
  idle
ENDOBJECT

OBJECT utmparr
  len
  val:PTR TO utmp
ENDOBJECT

OBJECT utmpidlearr
  len
  val:PTR TO utmpidle
ENDOBJECT

CONST RUSERSPROG=100002,
      RUSERSVERS_IDLE=1

ENUM RUSERSPROC_NUM=1, RUSERSPROC_NAMES, RUSERSPROC_ALLNAMES

CONST RUSERSVERS_ORIG=2
