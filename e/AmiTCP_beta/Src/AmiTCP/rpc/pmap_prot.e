OPT MODULE
OPT EXPORT

CONST PMAPPORT=111,
      PMAPPROG=100000,
      PMAPVERS=2,
      PMAPVERS_PROTO=2,
      PMAPVERS_ORIG=1

ENUM PMAPPROC_NULL, PMAPPROC_SET, PMAPPROC_UNSET, PMAPPROC_GETPORT,
     PMAPPROC_DUMP, PMAPPROC_CALLIT

OBJECT pmap
  prog
  vers
  prot
  port
ENDOBJECT

OBJECT pmaplist
  map:pmap
  next:PTR TO pmaplist
ENDOBJECT
