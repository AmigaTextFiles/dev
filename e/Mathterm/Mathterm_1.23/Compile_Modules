echo "Make sure you have ec in your path and emodules: assigned properly!"
assign emodules: "" add
cd tools

ec chars quiet
ec mathtermerrors quiet
ec mathtermerrorstrings quiet
ec singlesupport quiet
ec doublesupport quiet
ec mathterms quiet
ec mathtermd quiet

echo ""
echo "Modules compiled."
ask "Move them into EModules:tools/ ? (y/n) "
if warn
   copy #?.m emodules:tools/
endif
echo "Done."

