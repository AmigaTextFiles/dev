build: build.e .build
	failat 21
	ec $dep
	if not warn
	delete e:bin/build_old
	rename e:bin/build e:bin/build_old
	if not warn
	echo "Renamed previous build in E:bin/"
	endif
	copy build e:bin/
	if warn
	echo "Copying new build to E:bin/ failed"
	else
	echo "Copied new build to E:bin/"
	endif
	endif

clean:
	delete build
