;Installiert die Datei EManager.prefs in das
;benötigte Verzeichnis.

IF EXISTS ENVARC:
  echo "kopiere EManager.prefs nach ENVARC:"
  copy Einstellungen/EManager.prefs to ENVARC:
  echo "kopiere EManager.setup nach ENVARC:"
  copy Einstellungen/EManager.setup to ENVARC:
  echo "kopiere EManager.utils nach ENVARC:"
  copy Einstellungen/EManager.utils to ENVARC:
  echo "fertig"
ELSE
  echo "Das Assign «ENVARC:» MUSS existiern!"
  echo "Installation abgebrochen."
ENDIF
