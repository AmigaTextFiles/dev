.K args
set ec ecx.mos
set opts morphos showargs opti <args> quiet

$ec mystring $opts
$ec fractmisc $opts
$ec fractplugs/mand/mand $opts
$ec fractplugs/mand/plugin_mand $opts  DDIR fractplugs/
$ec fractplugs/flame/plugin_flame $opts DDIR fractplugs/
$ec mainmisc $opts
$ec zonegroup $opts
$ec zoneview $opts
$ec dispwin $opts
$ec fract_plugin $opts
$ec stuff/png $opts
$ec stuff/makepng2 $opts
$ec ignite $opts
