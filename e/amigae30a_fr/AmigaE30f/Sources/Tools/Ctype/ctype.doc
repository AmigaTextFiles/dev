ctype.m: fonctions généralement trouvé dans ctype.h

tolower(c)      converti n'importe quel caractère en minuscule
toupper(c)      converti n'importe quel caractère en majuscule

isspace(a)      " "
isupper(a)      "A" .. "Z"
islower(a)      "a" .. "z"
isalpha(a)      "a" .. "z", "A" .. "Z"
isalnum(a)      "a" .. "z", "A" .. "Z", "0" .. "9"
isxdigit(a)     "0" .. "9", "a" .. "f", "A" .. "Z"
isdigit(a)      "0" .. "9"
iscntrl(a)      0 .. 31, 128 .. 159
isprint(a)      32 .. 127, 160 .. 255
isgraph(a)      isprint - isspace
ispunct(a)      isgraph - isalnum
