@Node "select()" "field/select"

@{b}   NAME@{ub}
       select() of @{"field" Link "field/field()"} -- Selection of action.

@{b}   SYNOPSIS@{ub}
       field.select(LONG, LONG)

       field.select(optionlist, index)

@{b}   FUNCTION@{ub}
       Recognized token are:
           "len" -- length of the @{"field" Link "field/field()"}. The @{"field" Link "field/field()"} can contain up to that
               much bytes.

           "type" -- type of the @{"field" Link "field/field()"} (a char). Supported values are:
               "s" -- @{"string" Link "reqtools/string()"}
               "i" -- integer
               "f" -- float
               "d" -- date
               "b" -- binary

           "id" -- identifier of the @{"field" Link "field/field()"}.

           "desc" -- description of @{"field" Link "field/field()"}.

@{b}   INPUTS@{ub}
       optionlist:LONG -- Option list.

       index:LONG -- @{"index" Link "definitions/index()"} of option list.

@{b}   SEE ALSO@{ub}
       @{"field" Link "field/field()"}, object/select

@EndNode

@Node "setIdentifier()" "field/setIdentifier"

@{b}   NAME@{ub}
       setIdentifier() of @{"field" Link "field/field()"} -- Set @{"field" Link "field/field()"} identifier.

@{b}   SYNOPSIS@{ub}
       field.setIdentifier(PTR TO CHAR)

       field.setIdentifier(newIdentifier)

@{b}   FUNCTION@{ub}
       Sets the identifier of the @{"field" Link "field/field()"} to the specified @{"string" Link "reqtools/string()"} of
       characters. The identifier is the name the @{"field" Link "field/field()"} can be accessed
       by.

@{b}   INPUTS@{ub}
       newIdentifier:PTR TO CHAR -- String of characters that represent the
           @{"new" Link "resource/new()"} identifier. It is copied.

@{b}   SEE ALSO@{ub}
       @{"field" Link "field/field()"}, string/string.set

@EndNode

