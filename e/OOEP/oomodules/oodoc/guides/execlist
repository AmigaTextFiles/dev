@Node "end()" "execlist/end"

@{b}   NAME@{ub}
       end() of @{"execlist" Link "execlist/execlist()"} -- Global destructor.

@{b}   SYNOPSIS@{ub}
       execlist.end()

@{b}   FUNCTION@{ub}
       Disposes all nodes and the list. If the nodes have names you have to
       dispose them.

@{b}   SEE ALSO@{ub}
       @{"execlist" Link "execlist/execlist()"}

@EndNode

@Node "fromList()" "execlist/fromList"

@{b}   NAME@{ub}
       fromList() of @{"execlist" Link "execlist/execlist()"} -- Take items of @{"elist" Link "elist/elist()"} as node names.

@{b}   SYNOPSIS@{ub}
       execlist.fromList(LONG)

       execlist.fromList(list)

@{b}   FUNCTION@{ub}
       Creates a list. The items of the passed @{"elist" Link "elist/elist()"} are taken as names of
       the nodes. Therefore you may free the @{"elist" Link "elist/elist()"} but not the items.

@{b}   INPUTS@{ub}
       list:LONG -- E list of strings.

@{b}   SEE ALSO@{ub}
       @{"execlist" Link "execlist/execlist()"}

@EndNode

@Node "init()" "execlist/init"

@{b}   NAME@{ub}
       init() of @{"execlist" Link "execlist/execlist()"} -- Initialization of the object.

@{b}   SYNOPSIS@{ub}
       execlist.init()

@{b}   FUNCTION@{ub}
       Empty by now.

@{b}   SEE ALSO@{ub}
       @{"execlist" Link "execlist/execlist()"}

@EndNode

@Node "select()" "execlist/select"

@{b}   NAME@{ub}
       select() of @{"execlist" Link "execlist/execlist()"} -- Selection of action.

@{b}   SYNOPSIS@{ub}
       execlist.select(LONG, LONG)

       execlist.select(optionlist, index)

@{b}   FUNCTION@{ub}
       Recognized tags are:
           "list" -- take items as node names. See @{"fromList()" Link "fromList()"}.
@{b}   INPUTS@{ub}
       optionlist:LONG -- list of options

       index:LONG -- @{"index" Link "definitions/index()"} of option list

@{b}   SEE ALSO@{ub}
       @{"execlist" Link "execlist/execlist()"}, @{"fromList()" Link "fromList()"}

@EndNode

@Node "execlist()" "object/execlist"

@{b}   NAME@{ub}
       execlist of object -- List as used in the exec.library

@{b}   PURPOSE@{ub}
       Just a quick and small implementation of exec's list. Only useable
       for converting from an @{"elist" Link "elist/elist()"} (for listviews etc.)

@{b}   ATTRIBUTES@{ub}
       list:PTR TO lh -- exec's list header

       len:LONG -- number of nodes in the list

@{b}   SEE ALSO@{ub}
       object, exec

@EndNode

