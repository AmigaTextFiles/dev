OPTIONS RESULTS

Status DirName

path = RESULT

Status FileName

name = RESULT

SaveFile

PARSE VAR name new '.e'

IF (EXISTS(path || new)) THEN
  DO
    SAY 'Uruchamiam ' || path || new

    ADDRESS COMMAND path || new
  END
ELSE
  DO
    SAY 'Nie ma pliku ' || path || new
  END    