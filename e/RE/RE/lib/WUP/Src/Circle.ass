#rE
	.include	ppcmacros.pasm
	.text
	.sdreg	r2
	.align	2
	.global	_Circle
_Circle:
.set	Circle_x,20
.set	Circle_y,16
.set	Circle_r,12
.set	Circle_c,8
	link	a5,0
	push	a0
#IF stdrast
	law	a0,_stdrast
	lwz	d0,0(a0)
	tstw	d0
	beq	Circle_else1_0
#IF c<0 THEN c:=coloura
	lwz	d0,Circle_c(a5)
	cmpiw	d0,0
	bge	Circle_else2_0
#c:=coloura
	law	a0,coloura
	lwz	d0,0(a0)
	stw	d0,Circle_c(a5)
Circle_else2_0:
#SetAPen(stdrast,c)
	law	a0,_stdrast
	lwz	a1,0(a0)
	lwz	d0,Circle_c(a5)
	RUN_68K	_GfxBase,-342
#DrawEllipse(stdrast,x,y,r,r)
	law	a0,_stdrast
	lwz	a1,0(a0)
	lwz	d0,Circle_x(a5)
	lwz	d1,Circle_y(a5)
	lwz	d2,Circle_r(a5)
	lwz	d3,Circle_r(a5)
	RUN_68K	_GfxBase,-180
Circle_else1_0:
#ENDPROC
	pop	a0
	unlk	a5
	blr

	.type	_Circle,2
	.size	_Circle,$-_Circle

	.align	2
	.tocd
	.extern	_stdrast_
	.extern	coloura_
	.extern	_GfxBase_

	.tocd
	.extern	_PowerPCBase_
_Circle_:	.long	_Circle
