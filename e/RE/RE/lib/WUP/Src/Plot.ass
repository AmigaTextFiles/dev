#rE
	.include	ppcmacros.pasm
	.text
	.sdreg	r2
	.align	2
	.global	_Plot
_Plot:
.set	Plot_x,16
.set	Plot_y,12
.set	Plot_c,8
	link	a5,0
	push	a0
#IF stdrast
	law	a0,_stdrast
	lwz	d0,0(a0)
	tstw	d0
	beq	Plot_else1_0
#IF c<0 THEN c:=coloura
	lwz	d0,Plot_c(a5)
	cmpiw	d0,0
	bge	Plot_else2_0
#c:=coloura
	law	a0,coloura
	lwz	d0,0(a0)
	stw	d0,Plot_c(a5)
Plot_else2_0:
#SetAPen(stdrast,c)
	law	a0,_stdrast
	lwz	a1,0(a0)
	lwz	d0,Plot_c(a5)
	RUN_68K	_GfxBase,-342
#WritePixel(stdrast,x,y)
	law	a0,_stdrast
	lwz	a1,0(a0)
	lwz	d0,Plot_x(a5)
	lwz	d1,Plot_y(a5)
	RUN_68K	_GfxBase,-324
Plot_else1_0:
#ENDPROC
	pop	a0
	unlk	a5
	blr

	.type	_Plot,2
	.size	_Plot,$-_Plot

	.align	2
	.tocd
	.extern	_stdrast_
	.extern	coloura_
	.extern	_GfxBase_

	.tocd
	.extern	_PowerPCBase_
_Plot_:	.long	_Plot
