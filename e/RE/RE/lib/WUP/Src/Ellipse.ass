#rE
	.include	ppcmacros.pasm
	.text
	.sdreg	r2
	.align	2
	.global	_Ellipse
_Ellipse:
.set	Ellipse_x,24
.set	Ellipse_y,20
.set	Ellipse_w,16
.set	Ellipse_h,12
.set	Ellipse_c,8
	link	a5,0
	push	a0
#IF stdrast
	law	a0,_stdrast
	lwz	d0,0(a0)
	tstw	d0
	beq	Ellipse_else1_0
#IF c<0 THEN c:=coloura
	lwz	d0,Ellipse_c(a5)
	cmpiw	d0,0
	bge	Ellipse_else2_0
#c:=coloura
	law	a0,coloura
	lwz	d0,0(a0)
	stw	d0,Ellipse_c(a5)
Ellipse_else2_0:
#SetAPen(stdrast,c)
	law	a0,_stdrast
	lwz	a1,0(a0)
	lwz	d0,Ellipse_c(a5)
	RUN_68K	_GfxBase,-342
#DrawEllipse(stdrast,x,y,w,h)
	law	a0,_stdrast
	lwz	a1,0(a0)
	lwz	d0,Ellipse_x(a5)
	lwz	d1,Ellipse_y(a5)
	lwz	d2,Ellipse_w(a5)
	lwz	d3,Ellipse_h(a5)
	RUN_68K	_GfxBase,-180
Ellipse_else1_0:
#ENDPROC
	pop	a0
	unlk	a5
	blr

	.type	_Ellipse,2
	.size	_Ellipse,$-_Ellipse

	.align	2
	.tocd
	.extern	_stdrast_
	.extern	coloura_
	.extern	_GfxBase_

	.tocd
	.extern	_PowerPCBase_
_Ellipse_:	.long	_Ellipse
