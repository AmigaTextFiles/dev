I'm too lazy to inform You about this stuff. Just here are 2 procs from
my MUI program - You should know what is it about:

- --- cut ---

- -> picture is a full name of a file
- -> Prepares file

PROC initDTPicture(picture) OF dtpicture
    DEF width, height, bitmap

    self.dtobject:=3DNewDTObjectA(picture,[PDTA_REMAP, FALSE,
            DTA_GROUPID, GID_PICTURE, NIL, NIL])
    IF self.dtobject=3DNIL THEN Raise(DTER_FILE)

    GetDTAttrsA(self.dtobject,[PDTA_BITMAPHEADER, {bitmap},
            DTA_NOMINALHORIZ,{width},
            DTA_NOMINALVERT,{height},
            NIL, NIL])

    self.width:=3Dwidth
    self.height:=3Dheight

ENDPROC

/*

    =A3aduje bitmap=EA do pami=EAci, robi kopi=EA i wczytuje palet=EA
    Loads bitmap to memory,makes a copy of it and then loads the palette
*/

PROC allocDTBitmap(scaley=3DTRUE) OF dtpicture HANDLE
    DEF dtf=3DNIL:PTR TO dtframebox, fri=3DNIL:PTR TO frameinfo
    DEF bitmap:PTR TO bitmap, cregs, numcols, scale

    -> Get information 'bout pic

    dtf:=3DNew(SIZEOF dtframebox)
    fri:=3DNew(SIZEOF frameinfo)

    dtf.methodid:=3DDTM_FRAMEBOX
    dtf.frameinfo:=3Dfri
    dtf.contentsinfo:=3Dfri
    dtf.sizeframeinfo:=3DSIZEOF frameinfo

    IF DoDTMethodA(self.dtobject,NIL,NIL,dtf)=3DNIL THEN Raise(DTER_INFO)=

    -> Load to memory

    dtf.methodid:=3DDTM_PROCLAYOUT
    dtf.ginfo:=3DNIL

    IF DoDTMethodA(self.dtobject,NIL,NIL,dtf)=3DNIL THEN
Raise(DTER_LAYOUT)

    GetDTAttrsA(self.dtobject,[PDTA_BITMAP, {bitmap},
            PDTA_CREGS, {cregs},
            PDTA_NUMCOLORS, {numcols},
            NIL, NIL])
    WaitBlit()

    self.bitmap:=3Dbitmap
    self.palette:=3Dcregs
    self.depth:=3Dfri.depth
    self.colors:=3Dnumcols

EXCEPT DO

    IF dtf THEN Dispose(dtf)
    IF fri THEN Dispose(fri)
    IF exception THEN Raise(exception)

ENDPROC

- --- cut ---

