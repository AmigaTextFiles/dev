  OPTNAMEPATTERN "SDM.#?"
  COMPAREBYTE 58 "SIDMON II - THE MIDI VERSION"
ENDTYPE

TYPE "TFMX MOD"
  MATCHPATTERN 0 "TFMX-SONG"
ENDTYPE


