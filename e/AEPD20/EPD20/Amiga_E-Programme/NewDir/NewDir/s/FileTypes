  COMPAREBYTE $a0 $4eaefdd82940ffd0670000ea43fa0062
  COMPAREBYTE $0118 $2e6c696272617279006d617468666670
  COMPAREBYTE $0128 $2e6c69627261727900006100008a2e6c
  COMPAREBYTE $0138 $fffc4aacfff467182c6cffd4222cfff4
ENDTYPE


