
# List of file types which whatis.library reconizes without any FileType file:
#
# DOS_DEVICE, VOLUME, ASSIGN, DIR, EXECUTABLE, EXECPP40, EXECPP30, EXECPP,
# SCRIPT, TEXT, OBJECT, LIB, IFF, ILBM, ILBM24, ANIM, 8SVX, SMUS, FTXT, PREFS,
# TERM, ICON, IMPDATA, PPDATA, ZOO, LHARC, MEDMOD


### Programming languages #####################################################

TYPE "ABASIC"                   # AmigaBASIC
  NAMEPATTERN "#?.bas"
ENDTYPE

TYPE "GFA BASIC"
  OPTNAMEPATTERN "#?.gfa"
  COMPAREBYTE 2 "GFA-AMIGAB"
ENDTYPE


#-- ARexx ---------------------------------------------------------------------

TYPE "ARexx"
  SUBTYPE "Text"
  NAMEPATTERN "#?.rexx"
ENDTYPE

TYPE "DOpus RX"                 # DirOpus ARexx macro
  SUBTYPE "Text"
  NAMEPATTERN "#?.dopus"
ENDTYPE

TYPE "SD RX"                    # SuperDuper ARexx macro
  SUBTYPE "Text"
  NAMEPATTERN "#?.supdup"
ENDTYPE

TYPE "TSM RX"                   # TSMorph ARexx macro
  SUBTYPE "Text"
  NAMEPATTERN "#?.TSM"
ENDTYPE

TYPE "PP RX"                    # PowerPacker ARexx macro
  SUBTYPE "Text"
  NAMEPATTERN "#?.pprx"
ENDTYPE


#-- CanDo ---------------------------------------------------------------------

TYPE "CanDo dck"                # CanDo deck
  SUBTYPE "IFF"
  COMPAREBYTE 8 "DECK"
ENDTYPE

TYPE "CanDo var"                # CanDo variable
  SUBTYPE "IFF"
  COMPAREBYTE 8 "CDVR"
ENDTYPE


#-- C -------------------------------------------------------------------------

TYPE "C source"
  SUBTYPE "Text"
  NAMEPATTERN "#?.c"
ENDTYPE

TYPE "C header"
  SUBTYPE "Text"
  NAMEPATTERN "#?.h"
ENDTYPE


#-- Assembler -----------------------------------------------------------------

TYPE "Asm src"                  # Assembler source
  SUBTYPE "Text"
  NAMEPATTERN "#?.(asm|s)"
ENDTYPE

TYPE "Asm inc"                  # Assembler include
  SUBTYPE "Text"
  NAMEPATTERN "#?.i"
ENDTYPE


#-- AmigaE --------------------------------------------------------------------

TYPE "E source"
  SUBTYPE "Text"
  NAMEPATTERN "#?.e"
ENDTYPE

TYPE "E module"
  OPTNAMEPATTERN "#?.m"
  COMPAREBYTE 0 "EMOD"
ENDTYPE


#-- AMOS ----------------------------------------------------------------------

TYPE "AMOS src"                 # AMOS source
  OPTNAMEPATTERN "#?.amos"
  COMPAREBYTE 0 "AMOS Basic"
ENDTYPE

TYPE "AMOSPro"                  # AMOSPro source
  OPTNAMEPATTERN "#?.amos"
  COMPAREBYTE 0 "AMOS Pro"
ENDTYPE


### Packer ####################################################################

