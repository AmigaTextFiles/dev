@NODE "Example" "execTest.e"

/*
** Name......: execTest.e
** Version...: 0.1
** Author....: Jørgen 'Da' Larsen (Posse Pro. DK)
**
** History...: Version 0.1
**              o First release
**
*/

MODULE '*exec'

PROC main()
 autodetach()
 settaskpri()
 Delay(500)
 PrintF('Still here!!! - SPUNK\\n')
ENDPROC

PROC settaskpri()
 DEF exec:PTR TO exec
 NEW exec.new()
 exec.settaskpriority(5)
 PrintF('Task priority: \\d\\n',exec.gettaskpriority())
 END exec
ENDPROC

PROC autodetach()
 DEF exec:PTR TO exec
 NEW exec.new()
 exec.autodetach('execTest')
 END exec
ENDPROC

@ENDNODE
