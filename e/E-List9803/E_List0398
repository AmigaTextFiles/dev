EXPORT PROC getCPU()
DEF cpu, flags

   flags := exec.attnflags

   IF (flags AND $80)
      cpu := 68060
   ELSEIF (flags AND AFF_68040)
      cpu := 68040
   ELSEIF (flags AND AFF_68030)
      cpu := 68030
   ELSEIF (flags AND AFF_68020)
      cpu := 68020
   ELSEIF (flags AND AFF_68010)
      cpu := 68010
   ELSE
      cpu := 68000
   ENDIF
ENDPROC cpu



