
-- a QUEUE is a FIFO (first in, first out) collection.
-- Time complexity for data adding is O(1).
-- Time complexity for data searching is O(1).
-- Space complexity is O(n).

indexing

  names: queue;
  contents: generic;

  author: "Guichard Damien";
  created: 9,November,1995;
  modified: 9,November,1995

class QUEUE inherit LIST
  rename next as first
  export {NONE} find, item
  end
feature
  remove_first is
    -- Remove first element of the queue.
    do
      if first /= Void then
        first := first.first
      end
    end -- remove_first
end -- class 'QUEUE'

