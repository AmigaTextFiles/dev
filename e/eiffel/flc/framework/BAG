
-- a BAG is an unordered collection of elements.
-- Elements can occur any number of times.
-- Time complexity for data adding is O(1).
-- Time complexity for data searching is O(n).
-- Space complexity is O(n).

indexing

  names: bag;
  contents: generic;

  author: "Guichard Damien";
  created: 9,November,1995;
  modified: 9,November,1995

class BAG inherit COLLECTION
feature
  next:BAG
  count:INTEGER is
    -- Available elements.
    local b:BAG
    do
      from b := Current
      until b = Void
      loop
        b := b.next
        Result := Result + 1
      end
    end -- count
  add (element:BAG) is
    -- Add an element to the collection.
    do
      element.set_next(next)
      set_next(element)
    end  -- add
  find (element:BAG):BAG is
    -- Find an element in the bag.
    do
      from Result := Current
      until Result = Void or else element.is_equal(Result)
      loop
        Result := Result.next
      end
    end -- find
feature{BAG}
  set_next (b:BAG) is
    -- Set next bag element.
    do
      next := b
    end -- set_next
end  -- class 'BAG'

