@node Main "INTEGER class short form"
-- Integers
@{b}expanded class interface@{ub} INTEGER @{b}exported features@{ub}
    -- All features from @{"COMPARABLE" LINK comparable.guide/main} and @{"NUMERIC" LINK numeric.guide/main}
    @{b}infix@{ub} "//" (other:INTEGER):INTEGER
        -- Integer division of current integer by 'other'
      @{b}require@{ub}
        no_null_divisor : other /= 0
    @{b}infix@{ub} "\\" (other:INTEGER):INTEGER
        -- Remainder of integer division of current integer by 'other'
      @{b}require@{ub}
        no_null_divisor : other /= 0
@{b}end interface@{ub} -- class 'INTEGER'
@endnode
