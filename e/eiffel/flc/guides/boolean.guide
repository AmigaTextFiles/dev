@node Main "BOOLEAN class short form"
-- Boolean values
@{b}expanded class interface@{ub} BOOLEAN @{b}exported features@{ub}
    @{b}infix@{ub} "and" (other:BOOLEAN):BOOLEAN
        -- Boolean conjunction of current boolean and 'other'
    @{b}infix@{ub} "and then" (other:BOOLEAN):BOOLEAN
        -- Boolean semi-strict conjunction of current boolean and 'other'
    @{b}infix@{ub} "implies" (other:BOOLEAN):BOOLEAN
        -- Boolean implication of current boolean and 'other' (semi-strict)
    @{b}prefix@{ub} "not":BOOLEAN
        -- Negation of current boolean
    @{b}infix@{ub} "or" (other:BOOLEAN):BOOLEAN
        -- Boolean disjunction of current boolean and 'other'
    @{b}infix@{ub} "or else" (other:BOOLEAN):BOOLEAN
        -- Boolean semi-strict disjounction of current boolean and 'other'
    @{b}infix@{ub} "xor" (other:BOOLEAN):BOOLEAN
        -- Boolean exclusive or of current boolean and 'other'
@{b}end interface@{ub} -- class 'BOOLEAN'
@endnode
