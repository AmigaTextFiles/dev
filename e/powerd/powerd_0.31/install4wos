echo "preparing batch file..."
list dmodules: p #?.m files lformat="echo %N *Ndc dmodules:%N cpu=wos lg=T:test >NIL:*Ncopy ram:t/test/#?.lib d:lib/warpos quiet *Ndelete T:test all quiet" >T:run_me

echo "backuping if wos files available..."
IF NOT EXISTS d:lib/warpos
	makedir d:lib/warpos
ELSE
	rename d:lib/warpos as d:lib/warpos.old
ENDIF

echo "compiling, it will take a while..."
execute T:run_me

echo "cleaning..."
delete T:run_me quiet

echo "done."
