echo "preparing batch file..."
list dmodules: p #?.m files lformat="echo %N *Ndc dmodules:%N cpu=mos lg=T:test >NIL:*Ncopy ram:t/test/#?.lib d:lib/morphos quiet *Ndelete T:test all quiet" >T:run_me

echo "backuping if mos files available..."
IF NOT EXISTS d:lib/morphos
	makedir d:lib/morphos
ELSE
	rename d:lib/morphos as d:lib/morphos.old
ENDIF

echo "compiling, it will take a while..."
execute T:run_me

echo "cleaning..."
delete T:run_me quiet

echo "done."
