MODULE 'intuition/intuition'
MODULE 'graphics/rastport'

DEF fortune,  eightwin,  class
DEF arinf:PTR TO areainfo
DEF arbuf[30]:ARRAY

PROC main()
  arinf := [0,0,0,0,0,0,0,0]:areainfo

  IF eightwin := OpenW(0,0,200,200,
                       (IDCMP_MOUSEBUTTONS OR IDCMP_CLOSEWINDOW),
                       (WFLG_DRAGBAR OR WFLG_DEPTHGADGET OR
                        WFLG_CLOSEGADGET OR WFLG_ACTIVATE),
                       'Window',0,1,0)

    fortune := ['one',
                'two',
                'three',
                'four',
                'five',
                'six',
                'seven']

    Colour(1,0)
    InitArea(arinf,arbuf,6)     /* seems to crash near here */
    AreaEllipse(stdrast,100,100,40,40)
    AreaEnd(stdrast)

    WHILE (class := WaitIMessage(eightwin)) <> IDCMP_CLOSEWINDOW

      IF (class=IDCMP_MOUSEBUTTONS) AND (MsgCode()=SELECTUP)
        Box(50,94,190,102,1)
        Colour(2,1)
        TextF(50,100,'\s',ListItem(fortune,(Rnd(7))) )
/*        TextF(50,120,'myrast = \d',stdrast)  for debugging */
      ENDIF

    ENDWHILE

    CloseW(eightwin)

  ENDIF

ENDPROC



