Does anybody know how to put an image on a (gadtools) gadget? We've
tried something like this:


/*------------------------ START OF SOURCE ------------------------- */

/* An experimental source to get an image on a gadtools gadget - 
   does not work!

   The image is a brush of a magnify glass converted to bitmap by a
   program from the stone age - DeLuxeConvert.
*/

ENUM	NONE,
	ER_OPENLIB,
	ER_WB,
	ER_VISUAL,
	ER_CONTEXT,
	ER_GADGET,
	ER_WINDOW,
	ER_MEMORY

MODULE	'intuition/intuition',
	'intuition/gadgetclass',
	'intuition/screens',
	'gadtools',
	'libraries/gadtools',
	'exec/memory'
	'exec/nodes',

DEF	scr=NIL:PTR TO screen,
	wnd=NIL:PTR TO window,
	magnify=NIL:PTR TO gadget,
	picture=NIL:PTR TO image,
	visual=NIL,
	glist=NIL,
	offsety,
	g,
	type,
	infos,
	memblock


PROC main() HANDLE
	openinterface()
	REPEAT
		wait4message()
	UNTIL type=IDCMP_CLOSEWINDOW
	Raise(NONE)
EXCEPT
	closeinterface()
	IF exception>0 THEN WriteF('Could not \s!\n',
		ListItem(['',
			'open "gadtools.library" v37',
			'lock workbench',
			'get visual infos',
			'create context',
			'create gadget',
			'open window',
			'alloc chip memory'],exception))
ENDPROC

PROC openinterface()

/* Prepare everything */

	IF (gadtoolsbase:=OpenLibrary('gadtools.library',37))=NIL THEN Raise(ER_OPENLIB)
	IF (scr:=LockPubScreen('Workbench'))=NIL THEN Raise(ER_WB)
	IF (visual:=GetVisualInfoA(scr,NIL))=NIL THEN Raise(ER_VISUAL)
	offsety:=scr.wbortop+Int(scr.rastport+58)+1
	IF (g:=CreateContext({glist}))=NIL THEN Raise(ER_CONTEXT)

/* Alloc and copy image to chip memory */

	IF (memblock:=AllocMem(128,MEMF_CHIP))=NIL THEN Raise(ER_MEMORY)
	CopyMem({bitmap},memblock,128)

/* Define image structure */

	picture.leftedge:=0
	picture.topedge:=0
	picture.width:=25
	picture.height:=15
	picture.depth:=2
	picture.imagedata:=memblock
	picture.planepick:=NIL
	picture.planeonoff:=NIL
	picture.nextimage:=NIL

/* Define newgadget structure */

	magnify.nextgadget:=0
	magnify.leftedge:=154
	magnify.topedge:=148
	magnify.width:=25
	magnify.height:=15
	magnify.flags:=4
	magnify.activation:=0
	magnify.gadgettype:=1
	magnify.gadgetrender:=picture
	magnify.selectrender:=0
	magnify.gadgettext:=0
	magnify.mutualexclude:=0
	magnify.specialinfo:=0
	magnify.gadgetid:=3
	magnify.userdata:=0

/* Create imagegadget */

	IF (magnify:=g:=CreateGadgetA(BUTTON_KIND,g,
		[154,148,25,15,NIL,NIL,1,0,visual,0]:newgadget,
		[GA_IMAGE,picture,
		NIL]))=NIL THEN Raise(ER_GADGET)

/* Open and refresh window */

	IF (wnd:=OpenW(10,15,200,offsety+165,$304 OR SCROLLERIDCMP,$E,
		'ImageGadget',NIL,1,glist))=NIL THEN Raise(ER_WINDOW)
	Gt_RefreshWindow(wnd,NIL)
ENDPROC

PROC closeinterface()
	IF visual THEN FreeVisualInfo(visual)
	IF wnd THEN CloseWindow(wnd)
	IF glist THEN FreeGadgets(glist)
	IF scr THEN UnlockPubScreen(NIL,scr)
	IF gadtoolsbase THEN CloseLibrary(gadtoolsbase)
ENDPROC

PROC wait4message()
	DEF mes:PTR TO intuimessage,g:PTR TO gadget
	REPEAT
		type:=0
		IF mes:=Gt_GetIMsg(wnd.userport)
			type:=mes.class
			IF (type=IDCMP_GADGETDOWN) OR (type=IDCMP_GADGETUP)
				g:=mes.iaddress
				infos:=g.gadgetid
			ELSEIF type=IDCMP_REFRESHWINDOW
				Gt_BeginRefresh(wnd)
				Gt_EndRefresh(wnd,TRUE)
				type:=0
			ELSEIF type<>IDCMP_CLOSEWINDOW
				type:=0
			ENDIF
			Gt_ReplyIMsg(mes)
		ELSE
			Wait(-1)
		ENDIF
UNTIL type
ENDPROC

/* Include binary file of image (raw bitmap without colour map) from 
   hard disk. 
*/

bitmap:
INCBIN 'rf:test.bitm'

/*------------------------- END OF SOURCE -------------------------- */


This source gives us a window with an empty gadget - no image!
