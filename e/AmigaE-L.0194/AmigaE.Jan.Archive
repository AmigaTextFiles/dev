This is another example for lists of strings in E.The special thing about this example
is the use of the function insert().This function inserts a string in a sorted list,
so that it remains sorted.So,starting from NIL,simple calling insert() again and again
will create a sorted list of strings.This is quite handy if you do not want to sort the
list after it has been build.The whole example is in the Public Domain,so use it and change
it at will.Have Fun!


/* Demo of sorted lists of strings,using the
   procedures insert() and contains()  */

/* include module 'utility' for use of Stricmp() */

MODULE 'utility'

ENUM ER_NONE,ER_MEM,ER_LIB,ER_BREAK,ER_DUP

/* Used Exceptions */
/* If opening of library fails,it is indicated by raising ER_LIB and the user is told so */
   
RAISE ER_MEM IF String()=NIL,	
      ER_MEM IF List()=NIL,
      ER_LIB IF OpenLibrary()=NIL,
      ER_BREAK IF CtrlC()=TRUE
       
DEF in[20]:STRING,newlist=NIL

PROC main() HANDLE
utilitybase := OpenLibrary('utility.library',37)
WriteF('Please enter some strings.Press <RETURN> to end input.\n')
 REPEAT
  ReadStr(stdout,in)
  /* This example won't allow double insertion of a string */
  IF insert({newlist},in) THEN Raise(ER_DUP)
 UNTIL StrCmp(in,'',ALL)
 WriteF('Sorted List :\n')
 writeList(newlist)
 WriteF('\nSpecify element : ') 
 ReadStr(stdout,in)
 WriteF('Specified element is\s available\n',IF contains(newlist,in) THEN '' ELSE ' not')
 DisposeLink(newlist)
EXCEPT
 WriteF('Error: ')
 SELECT exception
  CASE ER_MEM; WriteF('Out of Memory.\n')
  CASE ER_LIB; WriteF('Could not open utility.library V37.\n')
  CASE ER_BREAK; WriteF('User aborted program.\n')
  CASE ER_DUP; WriteF('Duplicate insertion in list.\n')
  DEFAULT; NOP
 ENDSELECT
ENDPROC

/* Insert string item into the list thelist,so that the list remains sorted */
/* Procedure will return TRUE if item was already in the list else FALSE */
/* You should raise an exception if you really want a SET of strings and the result is TRUE */
/* The address of the list must be passed */

PROC insert(thelist,item)
DEF tail,thestr,done=FALSE,next,prev,theres
 thestr := String(EstrLen(item))
 StrCopy(thestr,item,ALL)
 IF ^thelist = NIL /* the simplest case : list is empty */
	^thelist := Link(thestr,NIL)
 ELSE	
  tail := ^thelist
  prev := tail
  IF Stricmp(thestr,tail)<0 /* is item is the least element ? */
   ^thelist := Link(thestr,^thelist)
  ELSE	
   /* This is the loop to search the appropriate place for the string */
   REPEAT
    CtrlC()
    theres := Stricmp(thestr,tail)
    IF tail = NIL  /* did we reach the end of the list ? */
        Link(prev,Link(thestr,NIL))
        done := TRUE
    ELSEIF theres>0 /* is item greater than current element => step forward */
        prev := tail
	tail := Next(tail)
    ELSEIF theres < 0 /* we have found the right place */
	Link(prev,Link(thestr,tail))
        done := TRUE
    ELSE /* here we have also found the right place but the same string already exists */
 	next := Next(tail)
        Link(tail,Link(thestr,next))
        RETURN TRUE
    ENDIF
   UNTIL done
  ENDIF
 ENDIF
ENDPROC FALSE

/* Find out if list thelist contains string item */
/* Return value is set accordingly */

PROC contains(thelist,item)
DEF first
 first := thelist
 LOOP
    IF first = NIL THEN JUMP c_out
    IF StrCmp(first,item,ALL) THEN RETURN TRUE
    first := Next(first)
 ENDLOOP
c_out:
ENDPROC FALSE

/* Write list thelist to stdout */

PROC writeList(thelist)
DEF first
first := thelist
LOOP
 CtrlC()
 IF first = NIL THEN JUMP w_out
 WriteF('\s\n',first)
 first := Next(first)
ENDLOOP
w_out:
ENDPROC

