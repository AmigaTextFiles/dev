/*
   A cry for help:

   This is the code I posted yesterday with a minor modification to the
   function main().  The function floatToString() seems to work fine when
   it's in a WriteF() statement all by itself, but if I add another
   argument it does some really weird things.  I can't figure it out.  Either
   I'm being really dense, or there's a nasty boag somewhere...or both. :-)

   Wouter, is there some conflict going on with WriteF() and StringF()?
*/

CONST FLOAT_MAX_AFT = 8,
      FLOAT_MAX_STRING_LENGTH = 9

PROC floatToString (string, float, maxAft)
  DEF fore, aft,
      aftString [FLOAT_MAX_AFT] : STRING
  fore := SpFix (float)
  aft := SpFix (SpMul (SpSub (SpFlt (SpFix (float)), float), 10000000.0))
  StringF (string, '\d.', fore)
  StringF (aftString, '\z\d[7]', aft)
  maxAft := IF maxAft > FLOAT_MAX_AFT THEN FLOAT_MAX_AFT ELSE maxAft
  SetStr (aftString, maxAft)
  StrAdd (string, aftString, ALL)
ENDPROC  string

PROC main ()
  DEF s [FLOAT_MAX_STRING_LENGTH] : STRING
  WriteF ('\s\n', floatToString (s, 2.0, 1))
  WriteF ('\s\n', floatToString (s, 2.0, 4))
  WriteF ('\s\n', floatToString (s, 200.00001, 5))
  WriteF ('\s\n', floatToString (s, 2000000.0, 4))
  WriteF ('\s\n', floatToString (s, 4.001, 3))
  WriteF ('\s\n', floatToString (s, 0.0004, 4))
  WriteF ('\s\n', floatToString (s, 0.0000004, FLOAT_MAX_AFT))
  WriteF ('\s\n', floatToString (s, 0.4444444, FLOAT_MAX_AFT))
  WriteF ('\s\n', floatToString (s, SpDiv (7.0, 22.0), 7))

  WriteF ('/*** The problems start here: ***/\n')
  WriteF ('\d \s\n', 1, floatToString (s, 2.0, 1))
  WriteF ('\d \s\n', 2, floatToString (s, 2.0, 4))
  WriteF ('\d \s\n', 3, floatToString (s, 200.00001, 5))
  WriteF ('\d \s\n', 4, floatToString (s, 2000000.0, 4))
  WriteF ('\d \s\n', 5, floatToString (s, 4.001, 3))
  WriteF ('\d \s\n', 6, floatToString (s, 0.0004, 4))
  WriteF ('\d \s\n', 7, floatToString (s, 0.0000004, FLOAT_MAX_AFT))
  WriteF ('\d \s\n', 8, floatToString (s, 0.4444444, FLOAT_MAX_AFT))
  WriteF ('\d \s\n', 9, floatToString (s, SpDiv (7.0, 22.0), 7))
ENDPROC

