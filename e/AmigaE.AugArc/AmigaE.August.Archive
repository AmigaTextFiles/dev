Here is the related part, which does works now (i'm sorry for the length):

/* Start of short example */
DEF busyPointer:PTR TO INT,my_mem=NIL

PROC setwaitpointer(ourwin:PTR TO window)

    busyPointer:=[$0000, $0000,     /* Reserved, must be NULL */
                  $0400, $07c0,
                  $0000, $07c0,
                  $0100, $0380,
                  $0000, $07e0,
                  $07c0, $1ff8,
                  $1ff0, $3fec,
                  $3ff8, $7fde,
                  $3ff8, $7fbe,
                  $7ffc, $ff7f,
                  $7efc, $ffff,
                  $7ffc, $ffff,
                  $3ff8, $7ffe,
                  $3ff8, $7ffe,
                  $1ff0, $3ffc,
                  $07c0, $1ff8,
                  $0000, $07e0,
                  $0000, $0000]:INT     /* Reserved, must be NULL */

    sizepoint:=72           /* Size of the busyPointer[] array */

    /* The Pointer-data must be in CHIP-memory */
    IF (my_mem:=AllocMem(sizepoint,MEMF_CHIP))

        /* Copy Pointer-data to allocated CHIP-memory!! */
        CopyMem(busyPointer,my_mem,sizepoint)

    /* SetPointer() : a function from the intuition.library */
    SetPointer(ourwin, my_mem, 16, 16, -6, 0)
ENDIF
/* End of short example */


