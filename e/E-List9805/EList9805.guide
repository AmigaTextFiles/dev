Content-Description: The message handler

PROC domsg_complilationwindow()
DEF imsg:PTR TO intuimessage, iaddress:PTR TO gadget, c=0

    WHILE imsg:=Gt_GetIMsg(complilationwindow.userport);
            IF imsg.class=IDCMP_CLOSEWINDOW;
                IF imsg<>0 THEN Gt_ReplyIMsg(imsg);imsg:=0;
                closecomplilationwindow()
                RETURN 0;
        ENDIF
        IF (imsg.class=IDCMP_GADGETUP) OR (imsg.class=IDCMP_GADGETDOWN)
            iaddress:=imsg.iaddress;
            c:=imsg.code

            IF imsg<>0 THEN Gt_ReplyIMsg(imsg);imsg:=0

            IF iaddress.gadgetid=BUNREF THEN openunrefwindow()

            IF iaddress.gadgetid=BVIEW
                IF imsg<>0 THEN Gt_ReplyIMsg(imsg);imsg:=0
                view_source()
            ENDIF
            IF iaddress.gadgetid=BRUN
                IF imsg<>0 THEN Gt_ReplyIMsg(imsg);imsg:=0
                run_prog()
            ENDIF
            IF iaddress.gadgetid=BEDIT
                IF imsg<>0 THEN Gt_ReplyIMsg(imsg);imsg:=0
                edit_source()
            ENDIF
            IF iaddress.gadgetid=BSHOWBUF
                IF imsg<>0 THEN Gt_ReplyIMsg(imsg);imsg:=0;
                openshowbufwindow()
            ENDIF

        ENDIF

        IF imsg<>0 THEN Gt_ReplyIMsg(imsg);imsg:=0

    ENDWHILE
ENDPROC

