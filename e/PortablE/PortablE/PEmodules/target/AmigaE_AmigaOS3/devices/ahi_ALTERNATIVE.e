ENDOBJECT     /* SIZEOF=20 */

OBJECT ahieffmastervolume
  effect:VALUE
  volume:VALUE
ENDOBJECT     /* SIZEOF=8 */

OBJECT ahiaudiomoderequester
  audioid:VALUE
  mixfreq:VALUE
  leftedge:INT
  topedge:INT
  width:INT
  height:INT
  infoopened:INT
  infoleftedge:INT
  infotopedge:INT
  infowidth:INT
  infoheight:INT
  userdata:VALUE
ENDOBJECT     /* SIZEOF=30 */

OBJECT ahisampleinfo
  type:VALUE
  address:VALUE
  length:VALUE
ENDOBJECT     /* SIZEOF=12 */

OBJECT ahirecordmessage
  type:VALUE
  buffer:VALUE
  length:VALUE
ENDOBJECT     /* SIZEOF=12 */

OBJECT ahisoundmessage
  channel:INT
ENDOBJECT     /* SIZEOF=2 */

OBJECT ahiaudioctrl
  userdata:VALUE
ENDOBJECT     /* SIZEOF=4 */

