ctype.m: functions usually found in ctype.h

tolower(c)	converts any char to lowercase
toupper(c)	converts any char to uppercase

isspace(a)	" "
isupper(a)	"A" .. "Z"
islower(a)	"a" .. "z"
isalpha(a)	"a" .. "z", "A" .. "Z"
isalnum(a)	"a" .. "z", "A" .. "Z", "0" .. "9"
isxdigit(a)	"0" .. "9", "a" .. "f", "A" .. "Z"
isdigit(a)	"0" .. "9"
iscntrl(a)	0 .. 31, 128 .. 159
isprint(a)	32 .. 127, 160 .. 255
isgraph(a)	isprint - isspace
ispunct(a)	isgraph - isalnum
