AbsExecBase	EQU $00000004

RawDoFmt	EQU -$20a

	SECTION text,code

	XDEF _Sprintf

*************************************************

_Sprintf:
	movem.l a2/a3/a6,-(SP)
	move.l 16(SP),a3
	move.l 20(SP),a0
	lea 24(SP),a1
	lea PutChProc(PC),a2
	move.l AbsExecBase.w,a6
	jsr RawDoFmt(a6)
	movem.l (SP)+,a2/a3/a6
	rts
PutChProc:
	move.b d0,(a3)+
	rts

*************************************************

	END
