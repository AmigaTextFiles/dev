@NODE fseek "fseek"
@{FUNC}fseek@{UFUNC}
Sets a file's byte cursor to a new position.@{PAR}

@{SYN}Overview@{USYN}
#include <stdio.h>@{PAR}

r = fseek(f,offset,mode);@{PAR}

int r;@{PAR}
FILE *f;@{PAR}
