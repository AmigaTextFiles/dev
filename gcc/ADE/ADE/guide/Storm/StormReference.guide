		showbase   = 0x00000080,@{PAR}
		showpoint  = 0x00000100,@{PAR}
		uppercase  = 0x00000200,@{PAR}
		showpos    = 0x00000400,@{PAR}
		scientific = 0x00000800,@{PAR}
		fixed      = 0x00001000,@{PAR}
