	{@{PAR}
		f = fopen(argv[1],"r");@{PAR}
		if(f == NULL)@{PAR}
		{@{PAR}
			fprintf(stderr,@{PAR}
						"Can't open file \\"%s\\"\\n,@{PAR}
						argv[1]);@{PAR}
			exit(100);@{PAR}
