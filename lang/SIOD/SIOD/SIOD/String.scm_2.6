(define (string<? x y)
        (< (string-cmp x y) 0))        

(define (string>? x y)
        (> (string-cmp x y) 0))

(define (string=? x y)
        (= (string-cmp x y) 0))

(define (string<=? x y)
        (<= (string-cmp x y) 0))

(define (string>=? x y)
        (>= (string-cmp x y) 0))

(define (string-CI<? x y)
        (< (string-cmp-CI x y) 0))        

(define (string-CI=? x y)
        (= (string-cmp-CI x y) 0))

(define (string-null? x)
        (= (string-cmp x "") 0))

