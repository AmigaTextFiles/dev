#include <stdio.h>

char Q[10001];

int main() {
  int x, y, eH, eQ, e9;
  scanf("%10000s", Q);
  for(x=y=eH=eQ=e9=0;Q[x];++x) {
    if(Q[x]!='H'&&Q[x]!='Q'&&Q[x]!='9'&&Q[x]!='+'&&Q[x]!=' ')
      if(Q[x]!='h'&&Q[x]!='q') {
        printf("Fatal error: Unrecognized character '%c' on line 1!\n", Q[x]);
        return(0);
      } else if(!y++) printf("Warning: Nonstandard letter case used.\n\n");
    if((Q[x]&~32)=='H') ++eH;
    else if((Q[x]&~32)=='Q') ++eQ;
    else if(Q[x]=='9') ++e9;
  }
  printf("#include <stdio.h>\n");
  if(eH) printf("\nchar H[]=\"Hello, world!\\n\";\n");
  if(eQ) printf("\nchar Q[]=\"%s\\n\";\n", Q);
  if(e9) {
    printf("\nvoid beer() {\n  int b;\n  for(b=99;b;--b) {\n    pri");
    printf("ntf(\"%%d bottle%%s of beer on the wall,\\n\", b, b==1?");
    printf("\"\":\"s\");\n    printf(\"%%d bottle%%s of beer,\\n\",");
    printf(" b, b==1?\"\":\"s\");\n    printf(\"You take one down, ");
    printf("pass it around,\\n\");\n    printf(\"%%d bottle%%s of b");
    printf("eer on the wall.\\n\\n\", b-1, b==2?\"\":\"s\");\n  }\n");
    printf("}\n");
  }
  printf("\nint main() {\n  int count=0;\n");
  for(x=0;Q[x];++x) switch(Q[x]) {
    case 'H': case 'h': printf("  printf(H);\n"); break;
    case 'Q': case 'q': printf("  printf(Q);\n"); break;
    case '9': printf("  beer();\n"); break;
    case '+': printf("  ++count;\n");
  }
  printf("  return(0);\n}\n\n");
  return(0);
}
