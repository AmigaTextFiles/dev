.Key path1,path2

IF "<path1>" EQ ""
   echo " "
   echo "  Must Specify both paths in Tooltypes EXECUTE command!"
   echo ""
   echo "  Example:  EXECUTE=Unpack->HD DH0: DH0:Scheme/"
   skip DONE
ENDIF

IF "<path2>" EQ ""
   echo " "
   echo "  Must Specify both paths in Tooltypes EXECUTE command!"
   echo ""
   echo "  Example:  EXECUTE=Unpack->HD DH0: DH0:Scheme/"
   skip DONE
ENDIF

lha x Gambit.lha <path1>
assign Gambit: <path1>Gambit

lha x SCM.lha <path2>
assign SCM: <path2>SCM
copy   SCM:init.scm S:

echo ""
echo "Add the following assignments to your startup sequence"
echo " ASSIGN GAMBIT: <path1>Gambit"
echo " ASSIGN SCM: <path2>SCM"

lab DONE

echo ""
echo "  All Done.  Click close gadget to get rid of window."
endcli
