{{<&
   ( n1 n2 == flag )&
&
<- signed_number(n1), signed_limit(n2). &
&
-> 1_or_0(flag). &
}}
{{<=&
   ( n1 n2 == flag )&
&
<- signed_number(n1), signed_limit(n2). &
&
-> 1_or_0(flag). &
