# SAS/C PPC makefile for powerUP (TM)
# written by Andreas R. Kleinert in 1998

CC   = SCPPC
OBJS =  environment.o gc.o gcutil.o globals.o init_subr.o init_subr2.o              \
        inspect.o io.o io2.o leval.o lists.o math.o math2.o math3.o                 \
        mathpred.o port.o repl.o repl2.o specialforms.o string.o sym_clo.o vector.o \

siod.elf :      $(OBJS)
                ppc-amigaos-ld -r lib:c_ppc.o $(OBJS) LIB:scppc.a lib:end.o -o siod.elf
                Copy siod.elf /siod
                Delete siod.elf
