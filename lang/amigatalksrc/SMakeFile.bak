TARGET = AmigaTalk.lha
#
all:
  SMake makeArchive
  SMake makeList
#
clean : $(TARGET)
  Delete $(TARGET) QUIET FORCE
#
makeList:
  $(ACMD) list $(TARGET) >Archive.list  
#
#  $(ACMD) $(OPTS) add $(TARGET) ARexxScripts/ATalkCommands.rexx
#  $(ACMD) $(OPTS) add $(TARGET) ARexxScripts/Kill.rexx
