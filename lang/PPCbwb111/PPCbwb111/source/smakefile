CC= SCPPC

OBJS =		bwbasic.o bwb_int.o bwb_tbl.o bwb_cmd.o bwb_prn.o \
		bwb_exp.o bwb_var.o bwb_inp.o bwb_fnc.o bwb_cnd.o \
		bwb_ops.o bwb_dio.o bwb_str.o bwb_elx.o bwb_mth.o

bwbasic.elf: $(OBJS)

          ppc-amigaos-ld -r LIB:c_ppc.o $(OBJS) LIB:scppc.a lib:end.o -o bwbasic.elf
          Copy bwbasic.elf /
          Delete bwbasic.elf
