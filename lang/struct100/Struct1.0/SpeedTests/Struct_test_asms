	BNE StructLOSkip
	MOVE.L #StructErrorName,A1
	CLR.L D0
	JSR -552(A6)
	MOVE.L D0,A6
	JSR -60(A6)
	MOVE.L D0,A4
	MOVE.L A4,D1
	MOVE.L #StructLibraryError,D2
	MOVE.L #14,D3
	JSR -48(A6)
	MOVE.L A4,D1
	MOVE.L A3,D2
StructLOLoop:
	CMP.B #0,(A3)+
	BNE StructLOLoop
	MOVE.L A3,D3
	SUB.L D2,D3
	SUB.L #1,D3
	JSR -48(A6)
	MOVE.L A4,D1
