	MOVE.W (A7)+,StructGlobalvarCOMMANDLENGTH(A2)
	MOVE.L (A7)+,StructGlobalvarCOMMANDPOINTER(A2)
	JSR StructPDENDYBARStart
	MOVE.L 4,A6
	MOVE.L A2,A1
	MOVE.L #30,D0
	JSR -210(A6)
	RTS
