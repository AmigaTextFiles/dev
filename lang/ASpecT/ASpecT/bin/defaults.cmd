cmd(
  "GNU-C Version (Amiga)",
  ".IMP",
  [".AS0",".IMP",".AS1",".OPD",".c",".h",".ami.o"],
  "aspect:",

  "%pbin/sc -q -dbtest %f",
  "aspserv",4,

  "Linking",
  "@.lnk@GCC:compilers/amiga/2.3.3/ld -dc @%l -o %f",".ami.o","\n",

  ["lib:crt0.o",
   "%pruntime/runtime.ami.o"],
  ["lib:libgcc.a",
   "lib:libc.a"],

  [com(true,
       "Parsing",
       "%pbin/asp -q %f",
       [".IMP"],   [],           [".AS1"],[],     [".AS1"],
       "","",
       "","",
       false,
       always),
   com(true,
       "Generating",
       "%pbin/sc -q %d %f",
       [".AS1"],   [],           [".OPD"],[],     [".c",".h",".OPD"],
       "","",
       "-db","",
       false,
       not_install),
   com(true,
       "Generating",
       "%pbin/sc -q -noheader %d %f",
       [".AS1"],   [],           [".OPD"],[],     [".c"],
       "","",
       "-db","",
       true,
       not_install),
   com(true,
       "Installing",
       "%pbin/sc -q -todir%i %d %f",
       [".AS1"],   [],           [],[],           [".c",".h"],
       "","",
       "-db","",
       false,
       install),
   com(true,
       "Compiling",
       "bin:gcc -g -c -fcaller-saves -fomit-frame-pointer -fforce-addr -fforce-mem -O %m -I%pruntime %f.c -o %f.ami.o",
       [".c",".h"],[".xc",".xh"],[".h"],  [".xh"],[".ami.o"],
       "-DMAIN","",
       "","",
       false,
       not_install)]
)
