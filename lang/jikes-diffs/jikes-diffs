+${ac_uA}HAVE_STATIC_CAST${ac_uB}HAVE_STATIC_CAST${ac_uC}${ac_uD}
+${ac_uA}SIZEOF_UNSIGNED_INT8_T${ac_uB}SIZEOF_UNSIGNED_INT8_T${ac_uC}0${ac_uD}
+${ac_uA}SIZEOF_UNSIGNED_CHAR${ac_uB}SIZEOF_UNSIGNED_CHAR${ac_uC}1${ac_uD}
+${ac_uA}uint8_t${ac_uB}uint8_t${ac_uC}unsigned char${ac_uD}
+${ac_uA}SIZEOF_UNSIGNED_INT16_T${ac_uB}SIZEOF_UNSIGNED_INT16_T${ac_uC}0${ac_uD}
+${ac_uA}SIZEOF_UNSIGNED_SHORT${ac_uB}SIZEOF_UNSIGNED_SHORT${ac_uC}2${ac_uD}
+${ac_uA}uint16_t${ac_uB}uint16_t${ac_uC}unsigned short${ac_uD}
+${ac_uA}SIZEOF_UNSIGNED_INT32_T${ac_uB}SIZEOF_UNSIGNED_INT32_T${ac_uC}0${ac_uD}
