/* print out exec resident modules - not the resident commands */

PROC main()
  DEF sysbase[4]:LONG, resarray:LONG, name:LONG, res:LONG
  resarray := MLONG(sysbase+300)
  res := MLONG(resarray)
  WRITE 'Exec Resident modules:\n----------------------\n'
  WHILE res?NIL
    name := MLONG(res+14)
    WRITE '\s\n',name
    INC 4,resarray
    res := MLONG(resarray) 
  ENDWHILE 
ENDPROC
