/* speedtest: 10.000.000 loops in 16 secs */

PROC main()
  DEF a:REGINT
  WRITE 'Now i\am doing 10.000.000 loops in multitasking with pri = 0\n'
  DOWN a,10000000
  ENDDOWN
  WRITE '... and now i\am ready. About 16 secs on a 7Mhz 68000\n\n'
  WRITE 'Only *ONE* higher programming language can reach this speed\n'
  WRITE 'TurboDEX. What else? :-)\n'
ENDPROC
