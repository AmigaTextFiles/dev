- 				printf("lex %s\n", yytoknames[yychar]);
- 			else
- 				printf("lex (%c)\n", yychar);
- 		}
  #endif
! 		if(yychar < 0)
! 			yychar = 0;
! 	}
! 	if((yyn += yychar) < 0 || yyn >= YYLAST)
! 		goto yydefault;
! 	if( yychk[ yyn=yyact[ yyn ] ] == yychar ){ /* valid shift */
! 		yychar = -1;
! 		yyval = yylval;
