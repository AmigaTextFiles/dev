! #if YYDEBUG != 0
!       if (yydebug)
! 	fprintf(stderr, "Now at end of input.\n");
! #endif
!     }
!   else
!     {
!       yychar1 = YYTRANSLATE(yychar);
! 
! #if YYDEBUG != 0
!       if (yydebug)
! 	{
! 	  fprintf (stderr, "Next token is %d (%s", yychar, yytname[yychar1]);
