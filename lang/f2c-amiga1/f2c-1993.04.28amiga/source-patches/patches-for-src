! 		if( (yyn = yyxi[1]) < 0 ) return(0);   /* accept */
! 	}
! 	if( yyn == 0 ){ /* error */
! 		/* error ... attempt to resume parsing */
! 		switch( yyerrflag ){
! 		case 0:   /* brand new error */
! #ifdef YYDEBUG
! 			yyerror("syntax error\n%s", yystates[yystate]);
! 			if(yytoknames[yychar])
