! 	yypv= &yyv[-1];
! 
! yystack:    /* put a state and value onto the stack */
! #ifdef YYDEBUG
! 	if(yydebug >= 3)
! 		if(yychar < 0 || yytoknames[yychar] == 0)
! 			printf("char %d in %s", yychar, yystates[yystate]);
! 		else
! 			printf("%s in %s", yytoknames[yychar], yystates[yystate]);
  #endif
