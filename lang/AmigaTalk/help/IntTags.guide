     #FRAME_DEFAULT
     #FRAME_BUTTON
     #FRAME_RIDGE
     #FRAME_ICONDROPBOX
     @{FG SHINE}
     Image message id's. @{FG TEXT}

     #IM_DRAW               draw yourself.
     #IM_HITTEST            return TRUE if click hits image.
     #IM_ERASE              erase yourself.
     #IM_MOVE               draw new and erase old.
     #IM_DRAWFRAME          draw with specified dimensions.
     #IM_FRAMEBOX           get frame around some box.
     #IM_HITFRAME           hittest with dimensions.
     #IM_ERASEFRAME         erase with dimensions.
     #IM_DOMAINFRAME        query image for its info.
     #IDS_NORMAL            Image draw states or styles, for IM_DRAW.
     #IDS_SELECTED          for selected gadgets.
     #IDS_DISABLED          for disabled gadgets.
     #IDS_BUSY              for future functionality.
     #IDS_INDETERMINATE     for future functionality (use IDS_INDETERMINANT).
     #IDS_INDETERMINANT     Oops, please forgive spelling error by jimm.
     #IDS_INACTIVENORMAL    normal, in inactive window border.
     #IDS_INACTIVESELECTED  selected, in inactive border.
     #IDS_INACTIVEDISABLED  disabled, in inactive border.
     #IDS_SELECTEDDISABLED  disabled and selected.
     #FRAMEF_SPECIFY        For IM_FRAMEBOX.
     @{FG SHINE}
     Accepted vales for imp_Which: @{FG TEXT}

     #IDOMAIN_MINIMUM
     #IDOMAIN_NOMINAL
     #IDOMAIN_MAXIMUM

@ENDNODE

