df0 <- TrackDisk new: 'DF0:'

df0 open: 0

rval <- df0 isDiskPresent
('Disk Presence is ', (rval asString)) print

rval <- df0 getSectorSize
('Sector size for DF0: is ', (rval asString)) print

rval <- df0 getTrackSize
('Track size for DF0: is ', (rval asString)) print

rval <- df0 getNumberOfTracks
('# of tracks for DF0: is ', (rval asString)) print

rval <- df0 getTotalSectors
('Total # of Sectors for DF0: is ', (rval asString)) print

rval <- df0 getDriveType
('Drive type for DF0: is ', (rval asString)) print

df0 displayDriveType

df0 seekTrack: 80

rval <- df0 isWriteProtected 
('Disk Write-Protection is ', (rval asString)) print

(df0 getDeviceType) print

rval <- df0 readTrack: 50

df0 displayBytes: rval

df0 clearReadBuffer

df0 ejectDisk

(df0 getErrorString) print

df0 close

