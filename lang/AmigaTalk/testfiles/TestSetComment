dos <- SafeDOS new

dos setCommentFieldOf: 'AmigaTalk:AmigaTalk.info' to: 'Testing SafeDos'

mask <- dos stringToProtectionMask: '+HPRW-EDSA'

dos setProtectionOf: 'AmigaTalk:AmigaTalk.info' to: mask

path <- dos getRealPathPart: 'AmigaTalk:Help/ados.guide'

('Real Path name is:  ', path ) print

path <- dos getPathPart: 'AmigaTalk:Help/ados.guide'

('AmigaDOS PathPart returns:  ', path ) print

name <- dos getFilePart: 'AmigaTalk:Help/ados.guide'

('File part is:  ', name ) print

oldErr <- dos setIoErrTo: 121

str <- dos translateIoErrToString: (dos getIoErr)

('121 is:  ', str ) print

('oldErr # is: ' oldErr ) print