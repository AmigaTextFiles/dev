lib <- openLibrary: 'exec.library' version: 40

'For exec.library:' print

('  ID      : ', (lib getIDString )) print
('  Ver     : ', (lib getVersion  )) print
('  Rev     : ', (lib getRevision )) print
('  Flags   : ', (lib getFlags    )) print
('  NegSize : ', (lib getNegSize  )) print
('  PosSize : ', (lib getPosSize  )) print
('  Count   : ', (lib getOpenCount)) print

('  CheckSum: ', <primitive 26 (lib getCheckSum) 16>) print

lib close
