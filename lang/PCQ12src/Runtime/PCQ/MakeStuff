join MinStart.o CheckIO.o Openers.o CheckRange.o StdInfo.o to t:Stuff1.lib
join Exp.o IntToStr.o LMath.o Ln.o PCQEnd.o Memory.o to t:Stuff2.lib
join PCQStart.o openstandards.o outbuffer.o sqrt.o to t:Stuff3.lib
join StdNames.o StringLib.o Trig.o to t:Stuff4.lib
join t:stuff1.lib t:stuff2.lib t:stuff3.lib t:stuff4.lib to Stuff.lib
delete t:stuff(1|2|3|4).lib

