
String s;

s.loadLocal("input.xml", 1);
trace "s="+s;

TreeNode troot<=s.parseXML();
trace "troot="+troot;
if(troot)
{
    trace "tree has "+troot.numNodes+" nodes.";
    HashTable ht;
    troot.writeToHashTable(ht);
    String key; foreach key in ht
	{
	    trace "ht[\""+key+"\"]="+ht[key];
	}
}


